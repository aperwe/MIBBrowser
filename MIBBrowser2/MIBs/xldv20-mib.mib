     -- --------------------------------------------------------------
     --        TLM  Metallic  Test Access control subgroup
     -- --------------------------------------------------------------
     --
     -- This managed entity manages the control command 
     -- for Metallic Test Access (MTA) of SHSDSL lines.
     --

     xldv20MtaControl OBJECT IDENTIFIER ::= { xldv20Tlm 16 }
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -

     xldv20MtaCtrlControlReq OBJECT-TYPE
        -- info-VALUE:   mtaActivate, mtaDeactivate
        -- info-ROU:     GET db SET tt  
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlReq
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             "  Controls the metallic test access (MTA) feature
             (activate or deactivate) at SHDSL line interfaces.
             MTA is provided only for SHDSL lines and for
             one line at a given time. 
             The plugin unit SU must be unlocked.
             If MTA has been activated for a line the command
             command mtaActivate implicitly deactivates MTA 
             for that line and activates it on the new line.
             MTA is service affecting.
             The command mtaDeactivate terminates MTA.
             MTA is terminated by the DSLAM autonomously if
             the SUSHDSL will be locked or performs a restart."
        --DEFAULT test-function-async     fsnMtaCtrlTest        
        --DEFAULT next-function-async     fsnMtaCtrlNext
        --DEFAULT set-function-async      fsnMtaCtrlSet
        --DEFAULT get-function-async      fsnMtaCtrlGet
        ::= { xldv20MtaControl 1 }

     xldv20MtaCtrlControlStatus OBJECT-TYPE
        -- info-VALUE:   requestIdle(1), requestBusy(2)
        -- info-VALUE:   requestPassed(3), requestFailed(4)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlStatus
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The status of a control request. "
        --DEFAULT next-function-async     fsnMtaCtrlNext
        --DEFAULT get-function-async      fsnMtaCtrlGet
        ::= { xldv20MtaControl 2 }

     xldv20MtaCtrlIfIndex OBJECT-TYPE
        -- info-VALUE:   0..65535
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The interface index of the SHDSL central PPTP
             for which metallic test access shall be activated. "
        --DEFAULT test-function-async     fsnMtaCtrlTest        
        --DEFAULT next-function-async     fsnMtaCtrlNext
        --DEFAULT set-function-async      fsnMtaCtrlSet
        --DEFAULT get-function-async      fsnMtaCtrlGet
        ::= { xldv20MtaControl 3 }

     xldv20MtaCtrlIfIndexUnderTest OBJECT-TYPE
        -- info-VALUE:   0..65535
        -- info-ROU:     GET db
        SYNTAX        Unsigned16
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " If not 0 the interface index of the SHDSL central PPTP
             for which metallic test access is already activated. "
        --DEFAULT next-function-async     fsnMtaCtrlNext
        --DEFAULT get-function-async      fsnMtaCtrlGet
        ::= { xldv20MtaControl 4 }

