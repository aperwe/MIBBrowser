     -- --------------------------------------------------------------
     --             TLM Dual Port SHDSL logical interface Table
     -- --------------------------------------------------------------

     -- This managed entity represents the infomation (configuration and 
     -- datarate and ServiceType etc.) only at the Dual Port SHDSL 
     -- "master" interface specific to a pair of lines at SHDSL PPTP. 
     -- The "master" interface shcDual contains the infomation from 
     -- the logical interface table xldv20ShcDualTable and the physical
     -- interface table xldv20ShcPPTPTable.
     -- The "slave" interface implies the 2nd ShcPPTPTable.
     --

     xldv20ShcDualTable OBJECT-TYPE
            -- info-IMPL:    V3.0->
            -- info-VERCTRL: -
        SYNTAX        SEQUENCE OF Xldv20ShcDualEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The logical interface table for Dual Port SHDSL."
              
        --DEFAULT test-function-async     fsnShcDualEntryTest
        --DEFAULT next-function-async     fsnShcDualEntryNext
        --DEFAULT get-function-async      fsnShcDualEntryGet
        --DEFAULT set-function-async      fsnShcDualEntrySet
        ::= { xldv20Tlm 21 }

     xldv20ShcDualEntry OBJECT-TYPE
            -- info-IMPL:    V3.0->
            -- info-VERCTRL: -
        SYNTAX        Xldv20ShcDualEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing information for the configurations and
               datarate specific to a pair of SHDSL lines etc. 
               Notifications : see xldv20IfEntry. "
        INDEX         { xldv20IfIndex }
        ::= { xldv20ShcDualTable 1 }

     Xldv20ShcDualEntry ::=
        SEQUENCE { xldv20ShcDualMinRateCfg                      Xldv20ShdslDualDataRate,
                   xldv20ShcDualMaxRateCfg                      Xldv20ShdslDualDataRate,
                   xldv20ShcDualMarginCfg                       Xldv20ShdslMargin,
                   xldv20ShcDualMinMarginCfg                    Xldv20AdslMinMargin,
                   xldv20ShcDualLineCurrentCfg                  Xldv20ShdslLineCurrentCfg,
                   xldv20ShcDualServiceTypeCfg                  Xldv20ShdslDualServiceType,
                   xldv20ShcDualRate                            Xldv20ShdslDualDataRateCurrent,
                   xldv20ShcDualAISOnLOS                        Xldv20OperState,
                   xldv20ShcDualAISOnACT                        Xldv20OperState ,
                   xldv20ShcDualGuaranteedBandwidthUsage           INTEGER,
                   xldv20ShcDualServiceType                     Xldv20ShdslDualServiceTypeCurrent
                   }

     xldv20ShcDualMinRateCfg OBJECT-TYPE
        -- info-VALUE:   384..4608 (default 384)
        -- info-VALUE:   for values see Xldv20ShcCtrlDualMinRate
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ShdslDualDataRate
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The desired lower value of the data rate.  The data
               rate ranges from 384 to 4608 Kbit/sec and 
               is splitted into units of 128 Kbit/sec.
               This corresponds to a cell rate between 905 and 10866 cells/sec.
               Default is 384 kbps/905 cps). "
        ::= { xldv20ShcDualEntry 1 }

     xldv20ShcDualMaxRateCfg OBJECT-TYPE
        -- info-VALUE:   384..4608 (default 4608)
        -- info-VALUE:   for values see Xldv20ShcCtrlDualMinRate
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ShdslDualDataRate
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The desired upper value of the data rate. The data
               rate ranges from 384 to 4608 Kbit/sec and 
               is splitted into units of 128 Kbit/sec.
               This corresponds to a cell rate between and cells/sec.
               Default is 4608 kbps/cps). "
        ::= { xldv20ShcDualEntry 2 }

     xldv20ShcDualMarginCfg OBJECT-TYPE
        -- info-VALUE:   see xldv20ShcCtrlMargin
        -- info-VALUE:   0..15 dB (default 3)
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ShdslMargin
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Desired margin in units of dB.  "
        ::= { xldv20ShcDualEntry 3 }

     xldv20ShcDualMinMarginCfg OBJECT-TYPE
        -- info-VALUE:   see xldv20ShcCtrlMinMargin
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AdslMinMargin
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Desired threshold for margin when the alarm
             trap TrImmMNR (immediate margin not reached) should
             be sent. The unit of measurement is dB. "
        ::= { xldv20ShcDualEntry 4 }

     xldv20ShcDualLineCurrentCfg OBJECT-TYPE
        -- info-VALUE:   off (1) (default)
        -- info-VALUE:   wettingCurrent (2)
        -- info-VALUE: 
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ShdslLineCurrentCfg
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The desired line current. "
        ::= { xldv20ShcDualEntry 5 }

     xldv20ShcDualServiceTypeCfg OBJECT-TYPE
        -- info-VALUE:   annexaDualSymmetricPSD  (1)
        -- info-VALUE:   annexaDualAsymmetricPSD (2)
        -- info-VALUE:   annexbDualSymmetricPSD  (3) (default)
        -- info-VALUE:   annexbDualAsymmetricPSD (4)
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ShdslDualServiceType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The configured service type of a SHDSL line. "
        ::= { xldv20ShcDualEntry 6 }

     xldv20ShcDualRate OBJECT-TYPE
        -- info-VALUE:   384..4608 kbps
        -- info-VALUE:   rateInvalid(32767)
        -- info-VALUE:   for detailed values see Xldv20ShcCtrlDualMinRate
        -- info-ROU:     GET tm
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ShdslDualDataRateCurrent
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The current data rate. The data rate
             ranges from 384 to 4608 Kbit/sec and 
             is splitted into units of 128 Kbit/sec.
             This corresponds to a cell rate between and cells/sec.
             Default is rateInvalid (32767). "
        ::= { xldv20ShcDualEntry 7 }

     xldv20ShcDualAISOnLOS  OBJECT-TYPE
        -- info-VALUE:   disabled : No VP-AIS generation (default)
        -- info-VALUE:   enabled  :    VP-AIS generation 
        -- info-ROU:     GET db SET tm
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables or disables the VP-AIS generation
               after defect LOS. "
        ::= { xldv20ShcDualEntry 8 }

     xldv20ShcDualAISOnACT  OBJECT-TYPE
        -- info-VALUE:   disabled : No VP-AIS generation (default)
        -- info-VALUE:   enabled  :    VP-AIS generation 
        -- info-ROU:     GET db SET tm
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables or disables the VP-AIS generation
               after link activation fault. "
        ::= { xldv20ShcDualEntry 9 }

        xldv20ShcDualGuaranteedBandwidthUsage OBJECT-TYPE
        -- info-VALUE:   0..100, default 0
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        INTEGER (0..100)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Garanteed bandwidth in percent. "
        ::= { xldv20ShcDualEntry 10 }

     xldv20ShcDualServiceType OBJECT-TYPE
        -- info-VALUE:   annexaDualSymmetricPSD  (1),
        -- info-VALUE:   annexaDualAsymmetricPSD (2),
        -- info-VALUE:   annexbDualSymmetricPSD  (3),
        -- info-VALUE:   annexbDualAsymmetricPSD (4),
        -- info-VALUE:   serviceTypeUnknown         (10) (default)
        -- info-ROU:     GET tm
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ShdslDualServiceTypeCurrent
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The current service type of a SHDSL line
             delivered by the SU-HW.
             Default value is serviceTypeUnknown. This
             value is returned in error case,
             e.g. no HW plugged. "
        ::= { xldv20ShcDualEntry 11 }



