         xldv20VpcTpTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20VpcTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " This table contains the entries for VPC termination points. "
            --DEFAULT test-function-async     fsnVpcEntryTest
            --DEFAULT next-function-async     fsnVpcEntryNext
            --DEFAULT set-function-async      fsnVpcEntrySet
            --DEFAULT get-function-async      fsnVpcEntryGet
            ::= { xldv20CallP 4 }

         xldv20VpcTpEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20VpcTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                   "Each entry represents the point in the switch
                   where the VPC and its associated overhead (F4 OAM cells) are 
                   terminated or originated.
                   Instances of this managed entity may be created automatically
                   by the ATM NE when VP or VC cross connects are created at
                   at an Ethernet port. Or they are created by command.
                   Similarly instances of this managed entity may be deleted
                   automatically by the ATM NE or as a result of an ATM Cross
                   Connect release request.
                   Notifications : 
                   xldv20TrRDI.
                   xldv20TrAIS alarms when fault is detected or cleared.
                   xldv20TrStateChangeOper. "
            INDEX         { xldv20VpcIndex }
            ::= { xldv20VpcTpTable 1 }

         Xldv20VpcTpEntry ::=
            SEQUENCE {
               xldv20VpcOperationalState              Xldv20OperState,
               xldv20VpcVpiValue                      INTEGER,
               xldv20VpcHwUnitIndex                   Unsigned16,
               xldv20VpcIfIndex                       Unsigned16,
               xldv20VpcPeakCellRateUpstream          Unsigned32,
               xldv20VpcPeakCellRateDownstream        Unsigned32,
               xldv20VpcCellDelayVariationToleranceUpstream  INTEGER,
               xldv20VpcCellDelayVariationToleranceDownstream  INTEGER,
               xldv20VpcTrafficType                   Xldv20TrafficType,
               xldv20VpcTrafficDirection              Xldv20TrafficDirection,
               xldv20VpcLineIndex                     Unsigned16,
               xldv20VpcSegmentEndPoint               INTEGER,
               xldv20VpcSustainableCellRateUpstream   Unsigned32,
               xldv20VpcSustainableCellRateDownstream Unsigned32,
               xldv20VpcMaximumBurstSizeUpstream      INTEGER,
               xldv20VpcMaximumBurstSizeDownstream    INTEGER,
               xldv20VpcAlarmSeverityIndex            Xldv20AlmSevProfileIndex,
               xldv20VpcAlarmFilteringIndex           Xldv20AlmFiltProfileIndex,
               xldv20VpcCvpIndexSegment               INTEGER,    -- continuity check
               xldv20VpcCvpIndexEndToEnd              INTEGER,    -- continuity check
               xldv20VpcIndex                         INTEGER,
               xldv20VpcAISAlarm                      Xldv20TpAlarmState,
               xldv20VpcRDIAlarm                      Xldv20TpAlarmState,
               xldv20VpcLOCAlarm                      Xldv20TpAlarmState,
               xldv20VpcNumberOfVcs                   INTEGER,
               xldv20VpcVbrCDVTUpstream               INTEGER,
               xldv20VpcVbrCDVTDownstream             INTEGER,
               xldv20VpcEmSpecific                    Unsigned16,
               xldv20VpcEthVpcIndex                   INTEGER,   -- new MR 2525
               xldv20VpcAtmfVplIndex                  INTEGER,   -- new MR 2525
               xldv20VpcMinimumCellRateUpstream       Unsigned32,-- MR2837 ubrPlus
               xldv20VpcMinimumCellRateDownstream     Unsigned32,
               xldv20VpcOverbookingFactor             INTEGER,
               xldv20VpcShaping                       Xldv20OperState
               }

