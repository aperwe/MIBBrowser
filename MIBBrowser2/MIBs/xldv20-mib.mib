    xldv20IlmiAAL2CpsCIDUpperLimit OBJECT-TYPE
        -- info-VALUE:   DEFVAL { 223 }
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
