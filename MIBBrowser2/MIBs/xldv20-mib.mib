            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20ControlStatus
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The status of a control request. "
            --DEFAULT next-function-async     fsnHwuNext
            --DEFAULT get-function-async      fsnHwuGet
            ::= { xldv20HwuControl 2 }

