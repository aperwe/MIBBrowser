    ------------------------------------------------------------------
    -- ATM Forum ILMI Extension for Auto Configuration
    --
    --            ATM Service Type
    ------------------------------------------------------------------

    -- A set of service types is pre-configured in the XL DSLAM.
    -- All attributes (beside the INDEX) are read-write
    -- in order the pre-configuration can be changed (SetRequests).
    --
    xldv20IlmiAtmServiceTypeTable OBJECT-TYPE
        -- info-IMPL:    V3.0->
        SYNTAX SEQUENCE OF Xldv20IlmiAtmServiceTypeEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "The XL specific ILMI Auto Configuration Extension ATM
            Service Type Information. "
        --DEFAULT test-function-async     fsnIlmiAstEntryTest
        --DEFAULT next-function-async     fsnIlmiAstEntryNext
        --DEFAULT set-function-async      fsnIlmiAstEntrySet
        --DEFAULT get-function-async      fsnIlmiAstEntryGet
        ::= { xldv20Ilmi 1 }

    xldv20IlmiAtmServiceTypeEntry OBJECT-TYPE
        -- info-IMPL:    V3.0->
        SYNTAX Xldv20IlmiAtmServiceTypeEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "An entry in the service type table describes the reference
            information about a service that may be associated with
            individual ATM connections for the purpose of auto-
            configuration. This information describe the name and sub-
            name of the service, the client layer protocol using the ATM
            service, the TM 4.0 service category, and the TM 4.0
            conformance definition."
        INDEX { xldv20IlmiAtmServiceTypeIndex }
        ::= { xldv20IlmiAtmServiceTypeTable 1 }

    Xldv20IlmiAtmServiceTypeEntry ::= SEQUENCE {
         xldv20IlmiAtmServiceTypeIndex        Xldv20NumberOfServiceTypes,
         xldv20IlmiAtmServiceProviderName     DisplayString,
         xldv20IlmiAtmServiceName             DisplayString,
         xldv20IlmiAtmServiceSubName          DisplayString,
         xldv20IlmiAtmServiceClient           DisplayString,
         xldv20IlmiAtmServiceTMCategory       INTEGER,
         xldv20IlmiAtmServiceTMConformanceDef INTEGER
    }

    xldv20IlmiAtmServiceTypeIndex OBJECT-TYPE
        -- info-VALUE:   1..10
        -- info-ROU:     GET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX Xldv20NumberOfServiceTypes
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "A number between which identifies the entry in the Service
            Type Table. "
        ::= { xldv20IlmiAtmServiceTypeEntry 1 }

    xldv20IlmiAtmServiceProviderName OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX DisplayString
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "This string identifies the name of the service provider for
            the service described in this profile. The value of this
            attribute should not set to NULL."
        ::= { xldv20IlmiAtmServiceTypeEntry 2 }

    xldv20IlmiAtmServiceName OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX DisplayString
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "This string identifies the name of the service described in
            this profile. The value of this attribute should not be set
            to NULL. "
        ::= { xldv20IlmiAtmServiceTypeEntry 3 }

    xldv20IlmiAtmServiceSubName OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX DisplayString
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "This string identifies the sub-name of the service described
            in this profile. A Service may have zero or more sub-names,
            each with its own service type profile. "
        ::= { xldv20IlmiAtmServiceTypeEntry 4 }

    xldv20IlmiAtmServiceClient OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V3.0->
        -- info-VERCTRL: -
        SYNTAX DisplayString
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
