             -- --------------------------------------------------------------
             --                            States
             -- --------------------------------------------------------------

             -- The control status of an entity.
             --
             Xldv20ControlStatus ::= TEXTUAL-CONVENTION
                  STATUS current
                  DESCRIPTION
                    "This data type is used as the syntax for the control status 
                     attributes."
                  SYNTAX INTEGER {
                    requestIdle(1),
                    requestBusy(2),
                    requestPassed(3),
                    requestFailed(4)
                  }

             Xldv20ControlStatus ::=
                 INTEGER {
                     requestIdle(1),
                     requestBusy(2),
                     requestPassed(3),
                     requestFailed(4)
                 }

             -- The operating state of an entity.
             --
              -- The operatational state of an entity.
             --
             -- inserted for MSAP-MIB for ACIs Brass

             Xldv20OperState ::= TEXTUAL-CONVENTION
                  STATUS current
                  DESCRIPTION
                    "This data type is used as the syntax for the oper state 
                     attributes. "
                  SYNTAX INTEGER {
                     enabled(1),
                     disabled(2)
                  }

             Xldv20OperState ::=
                 INTEGER {
                     enabled(1),
                     disabled(2)
                 }

