        -- info-IMPL:    V2.1->
        SYNTAX  Xldv20E1nPPTPEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
           " An entry represents an E1nPPTP (IfType e1n). "
        INDEX   { xldv20IfIndex }
        ::= { xldv20E1nPPTPTable 1 }

     Xldv20E1nPPTPEntry ::=
         SEQUENCE {
             xldv20E1nLineType            Xldv20E1nLineType,
             xldv20E1nLineCoding          Xldv20E1nLineCoding,
             xldv20E1nLoopbackConfig      Xldv20E1nLoopbackConfig,
             xldv20E1nSignalMode          Xldv20E1nSignalMode,
             xldv20E1nTransmitClockSource Xldv20E1nTransmitClockSource,
             xldv20E1nFdl                 Xldv20E1nFdl
         }
 
        xldv20E1nLineType OBJECT-TYPE
        -- info-VALUE:   dsx1E1(4) (default),
        -- info-VALUE:   dsx1E1-CRC(5) 
        -- info-ROU:     GET db SET tm
        -- info-IMPL:    V2.1->
        -- info-VERCTRL: -
        SYNTAX  Xldv20E1nLineType
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
           " This variable indicates  the  variety  of  DS1
           Line  implementing  this  circuit.  The type of
           circuit affects the number of bits  per  second
           that  the circuit can reasonably carry, as well
           as the interpretation of the  usage  and  error
           statistics.  "
        ::= { xldv20E1nPPTPEntry 1 }

        xldv20E1nLineCoding OBJECT-TYPE
        -- info-VALUE:   dsx1HDB3 (3) (default)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.1->
        -- info-VERCTRL: -
        SYNTAX  Xldv20E1nLineCoding
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
           " This variable describes the  variety  of  Zero
           Code  Suppression  used  on  the link, which in
           turn affects a number of its characteristics.
           E1 links, with or without CRC, use dsx1HDB3  or
           dsx1AMI. "
       ::= { xldv20E1nPPTPEntry 2 }
       
        xldv20E1nLoopbackConfig OBJECT-TYPE
        -- info-VALUE:   dsx1LineLoop(3)
        -- info-VALUE:   dsx1NoLoop(1) (default)
        -- info-ROU:     GET db SET tm
        -- info-IMPL:    V2.1->
        -- info-VERCTRL: -
        SYNTAX   Xldv20E1nLoopbackConfig
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
           " This variable represents the  loopback  configuration
           of the DS1 interface.  Agents supporting read/write
           access should return badValue in
           response to a requested loopback state that the
           interface does not support. 
 
                dsx1NoLoop : 
                     Not in the loopback state.  A device  that
                     is not capable of performing a loopback on
                     the interface shall always return this  as
                     its value.
 
                dsx1LineLoop
                     The received signal at this interface does
                     not  go  through the device (minimum pene-
                     tration) but is looped back out. "
       ::= { xldv20E1nPPTPEntry 3 }
 
