VGW-MIB DEFINITIONS ::= BEGIN

  IMPORTS
    OBJECT-TYPE
      FROM RFC-1212
    TRAP-TYPE
      FROM RFC-1215
    onu
      FROM AN-MIB
    xldv20Traps
      FROM XLDV20-MIB;

  vgw OBJECT IDENTIFIER
  ::= { onu 9 }



  -- *************************************************************************
  -- *************************************************************************
  -- *************************************************************************
  -- *************************************************************************
  -- VGW-MIB Version Control
  -- *************************************************************************
  -- *************************************************************************
  -- *************************************************************************
  -- *************************************************************************
  vgwMibVersionControl OBJECT IDENTIFIER ::= { vgw 3 }

  vgwMibVersion OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The version of the VGW-MIB
       - 1: first version with V5.2 and AAL2 for VGW V2.0"
  ::= { vgwMibVersionControl 1 }



  -- *************************************************************************
  -- *************************************************************************
  -- *************************************************************************
  -- *************************************************************************
  -- V5 Objects
  -- *************************************************************************
  -- *************************************************************************
  -- *************************************************************************
  -- All configuration objects which manage V5 related data can be found
  -- under this node.
  vgwV5Objects OBJECT IDENTIFIER ::= { vgw 1 }

  -- *** general used types **************************************************
  -- Limiting the value ranges of the table indexes, we define the number of
  -- interfaces which can be part of a VGW. Normally this is not done in a MIB.
  -- But the XLD system, which is the base of the VGW, requires the definition
  -- of all dimensions. The data base, which contain all described data, has
  -- a size according the ranges defined here.
  -- The following limitations doe exist:
  --   - max. 14 SU_VGWs
  --   - max. 2 V5.2 interfaces per SU_VGW
  --   - max. 4096 user ports per SU_VGW
  --   - max. 2048 communication channels (ISDN-D-channel and ELCP) per SU_VGW
  -- A change of the system requirements requires a change of the MIB.
  -- The MIB defines already values for later steps of the VGW which are
  -- not used in VGW V2.0

  -- The index of the vgwV5InterfaceTable[]
  -- (14 slots) * (2 V5-interfaces) = 28
  VgwV5InterfaceIndexType ::= INTEGER (1..28)

  -- The index of the vgwV5LinkTable[]
  -- (14 slots) * (16 links) = 224
  VgwV5LinkIndexType ::= INTEGER (1..224)

  -- The index of the vgwV5PhyComChanTable[]
  -- (14 slots) * (16 links) * (3 timeslots) = 672
  -- Note, 0 is used if a logical C-channel is not assigned to a physical
  -- C-channel. This can happen after protection switch during normal
  -- operation.
  VgwV5PhyComChanIndexType ::= INTEGER (0..672)

  -- The index of the vgwV5LogComChanTable[]
  -- (number of physical C-channels) - (14 slots)*(4 standby) = 616
  -- The maximal value can only be reached if only one V5 interface is
  -- configured per PIU.
  -- Note, 0 is used in vgwV5?TypeChanIndex if these items are not in use.
  VgwV5LogComChanIndexType ::= INTEGER (0..616)

  -- The index of the vgwV5ComPathTable[]
  -- (6 C-paths) * (2 V5-interfaces) * (14 slots) = 168
  -- The maximal value can only by reached if two V5 interfaces are configured
  -- per SU_VGW.
  VgwV5ComPathIndexType ::= INTEGER (1..168)

  -- The index of the vgwV5UserPortTable[]
  -- (14 slots) * (4096 user ports) = 57334
  VgwV5UserPortIndexType ::= INTEGER (1..57334)

  -- The index of the vgwV5ProfileTable[]
  -- (one profile for each V5.2 interface possible)
  -- Note, 0 is used in vgwV5IfProfileIndex if no profile is selected.
  VgwV5ProfileIndexType ::= INTEGER (0..28)


  -- The V5-interface-id of a V5 interface (24 bits)
  VgwV5InterfaceIdType ::= INTEGER (0..16777215)

  -- The V5 variant number of a V5 interface
  VgwV5VariantNoType ::= INTEGER (0..127)

  -- The link-id of a logical link
  VgwV5LinkIdType ::= INTEGER (0..255)

  -- The index of the xldv20IfTable[] used by V5 objects
  VgwV5PhyLineIndexType ::= INTEGER

  -- The index of the xldv20HwUnitTable[] used by V5 objects
  VgwV5HwUnitIndexType ::= INTEGER

  -- The channel-id of the logical C-channel (16 bits)
  VgwV5CChannelIdType ::= INTEGER (0..65535)

  -- The administrative state of a V5-interface
  VgwV5InterfaceAdminStateType ::= INTEGER
                                   {
                                     unlocked (1),
                                     locked (2)
                                   }

  -- The operational state of a V5-interface
  VgwV5InterfaceOperStateType ::= INTEGER
                                  {
                                    enabled (1),
                                    disabled (2)
                                  }

  -- The type of a V5-interface
  VgwV5InterfaceTypeType ::= INTEGER
                             {
                               v51 (1),
                               v52 (2)
                             }

  -- The alarm state of a V5 interface. Each bit represents an alarm.
  -- The alarm is active if the bit is set.
  --  -bit 0: interface error (wrong interface-id, wrong variant number)
  --  -bit 1: restart timeout exchange
  VgwV5InterfaceAlarmStateType ::= INTEGER (0..3)

  -- The administrative state of a logical link
  VgwV5LinkAdminStateType ::= INTEGER
                              {
                                unlocked (1),
                                locked (2),
                                shutdown (3)
                              }

  -- The operational state of a logical link
  VgwV5LinkOperStateType ::= INTEGER
                             {
                               enabled (1),
                               disabled (2)
                             }

  -- The timeslot used by the physical C-channel
  VgwV5TimeSlotType ::= INTEGER
                        {
                          ts16 (16),
                          ts15 (15),
                          ts31 (31)
                        }

  -- The type of a C-channel
  VgwV5CChannelTypeType ::= INTEGER
                            {
                              active (1),
                              standby (2)
                            }

  -- The protection group to which a C-channel belongs
  VgwV5ProtectionGroupType ::= INTEGER
                               {
                                 protGroup1 (1),
                                 protGroup2 (2)
                               }

  -- The types of a C-path
  VgwV5ComPathTypeType ::= INTEGER
                           {
                             common (1),
                             pstn (2),
                             bcc (3),
                             link (4),
                             prot1 (5),
                             prot2 (6)
                           }

  -- The address of a user port. This can be a L3 address (PSTN subscriber)
  -- or an EFA (ISDN subscriber).
  VgwV5UserPortAddrType ::= INTEGER (0..32767)

  -- The administrative state of a user port
  VgwV5UserPortAdminStateType ::= INTEGER
                                  {
                                    unlocked (1),
                                    locked (2)
                                  }

  -- The operational state of a user port
  VgwV5UserPortOperStateType ::= INTEGER
                                 {
                                   enabled (1),
                                   disabled (2)
                                 }

  -- The additional state of a user port at AN- and LE-side.
  VgwV5UserPortAddStateType ::= INTEGER (0..255)

  -- The index of the selected interface table where the user port
  -- really exist (AN side)
  VgwV5UserPortTableIndexType ::= INTEGER

  -- The id of the user port (AN side).
  VgwV5UserPortIdType ::= INTEGER (1..255)

  -- The type of the interface where the user port is located (AN side)
  VgwV5UserPortInterfaceTypeType ::= INTEGER
                                     {
                                       aal2 (1)
                                     }

  -- The user port types
  VgwV5TypeOfPortType ::= INTEGER
                          {
                            pstn (1),
                            isdn-ba (2),
                            isdn-plc (3)
                          }

  -- The state of a reported alarm
  VgwV5AlarmStateType ::= INTEGER
                          {
                            noAlarm (0),
                            alarm (1)
                          }

  -- The description of a profile
  VgwV5ProfileDescrType ::= OCTET STRING (SIZE (0..255))

  -- The V5-profile number (16 bits)
  VgwV5ProfileNoType ::= INTEGER (0..65535)

  -- The index of the xldv20AlarmSeverityTable[]. It is used by table items
  -- which can report alarms.
  VgwV5AlarmSeverityIndexType ::= INTEGER


  -- *************************************************************************
  -- *************************************************************************
  -- vgwV5InterfaceTable
  -- *************************************************************************
  -- *************************************************************************
  vgwV5InterfaceTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF VgwV5InterfaceEntry
    ACCESS  not-accessible
    STATUS  mandatory
	DESCRIPTION
      "This table contains information about the configuration of the
	   V5 interfaces at the CO-IWF."

	          
        --DEFAULT next-function-async     fsnV5InterfaceEntryNext
        --DEFAULT get-function-async      fsnV5InterfaceEntryGet

  ::= { vgwV5Objects 1 }

  vgwV5InterfaceEntry OBJECT-TYPE
    SYNTAX  VgwV5InterfaceEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "An entry in the V5InterfaceTable[] that represents a single V5 interface."
    INDEX { vgwV5IfInterfaceIndex }
  ::= { vgwV5InterfaceTable 1 }

  VgwV5InterfaceEntry ::=
    SEQUENCE
    {
      vgwV5IfInterfaceIndex      VgwV5InterfaceIndexType,
      vgwV5IfInterfaceId         VgwV5InterfaceIdType,
      vgwV5IfVariantNo           VgwV5VariantNoType,
      vgwV5IfHwUnitIndex         VgwV5HwUnitIndexType,
      vgwV5IfAdminState          VgwV5InterfaceAdminStateType,
      vgwV5IfOperState           VgwV5InterfaceOperStateType,
      vgwV5IfInterfaceType       VgwV5InterfaceTypeType,
      vgwV5IfAlarmState          VgwV5InterfaceAlarmStateType,
      vgwV5IfAlarmSeverityIndex  VgwV5AlarmSeverityIndexType,
      vgwV5IfProfileIndex        VgwV5ProfileIndexType
    }

  -- *************************************************************************
  -- The items of a vgwV5InterfaceEntry
  -- *************************************************************************
  vgwV5IfInterfaceIndex OBJECT-TYPE
    SYNTAX  VgwV5InterfaceIndexType
	ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "The index of the V5 interface in the vgwV5InterfaceTable[]."
  ::= { vgwV5InterfaceEntry 1 }

  vgwV5IfInterfaceId OBJECT-TYPE
    SYNTAX  VgwV5InterfaceIdType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The interface-id of the V5 interface."
  ::= { vgwV5InterfaceEntry 2 }

  vgwV5IfVariantNo OBJECT-TYPE
    SYNTAX  VgwV5VariantNoType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The number of the V5 variant."
  ::= { vgwV5InterfaceEntry 3 }

  vgwV5IfHwUnitIndex OBJECT-TYPE
    SYNTAX  VgwV5HwUnitIndexType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index in the xldv20HwUnitTable[] on which the V5 interface is
       located.
       -VGW V2.0: the selected PIU must be a SU_VGW."
  ::= { vgwV5InterfaceEntry 4 }

  vgwV5IfAdminState OBJECT-TYPE
    SYNTAX  VgwV5InterfaceAdminStateType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The administrative state of the V5 interface."
  ::= { vgwV5InterfaceEntry 5 }

  vgwV5IfOperState OBJECT-TYPE
    SYNTAX  VgwV5InterfaceOperStateType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The operative state of the V5 interface."
  ::= { vgwV5InterfaceEntry 6 }

  vgwV5IfInterfaceType OBJECT-TYPE
    SYNTAX  VgwV5InterfaceTypeType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The type of the V5 interface.
       -VGW V2.0: only v52 is possible."
  ::= { vgwV5InterfaceEntry 7 }

  vgwV5IfAlarmState OBJECT-TYPE
    SYNTAX  VgwV5InterfaceAlarmStateType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The current alarm state of the V5 interface. Each possible alarm is
       represented by one bit:
       -bit 0: interface error (wrong interface-id, wrong variant number)
       -bit 1: restart timeout exchange"
  ::= { vgwV5InterfaceEntry 8 }

  vgwV5IfAlarmSeverityIndex OBJECT-TYPE
    SYNTAX  VgwV5AlarmSeverityIndexType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "This index selects an item in the xldv20AlarmSeverityTable[]. It is
       used to assign the reported alarm to a severity."
  ::= { vgwV5InterfaceEntry 9 }

  vgwV5IfProfileIndex OBJECT-TYPE
    SYNTAX  VgwV5ProfileIndexType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "This is an index in the VgwV5ProfileTable[]. It selects one item which
       contains a V5-profile. Such a profile contains special settings for
       the V5 interface (e.g. adaptions for a LE type).
       If no V5-profile is selected, this attribute has the value 0.
       In this case default values are used by the V5 protocol implementation."
  ::= { vgwV5InterfaceEntry 10 }


  -- *************************************************************************
  -- *************************************************************************
  -- vgwV5LinkTable
  -- *************************************************************************
  -- *************************************************************************
  vgwV5LinkTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF VgwV5LinkEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "This table contains information about the configuration of the
       V5 links at the CO-IWF (logical links)."

        --DEFAULT next-function-async     fsnV5LinkEntryNext
        --DEFAULT get-function-async      fsnV5LinkEntryGet

  ::= { vgwV5Objects 2 }

  vgwV5LinkEntry OBJECT-TYPE
    SYNTAX  VgwV5LinkEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "An entry in the vgwV5LinkTable[] that represents a single V5 link."
    INDEX { vgwV5LiLinkIndex }
  ::= { vgwV5LinkTable 1 }

  VgwV5LinkEntry ::=
    SEQUENCE
    {
      vgwV5LiLinkIndex           VgwV5LinkIndexType,
      vgwV5LiLinkId              VgwV5LinkIdType,
      vgwV5LiV5InterfaceIndex    VgwV5InterfaceIndexType,
      vgwV5LiPhyLineIndex        VgwV5PhyLineIndexType,
      vgwV5LiAdminState          VgwV5LinkAdminStateType,
      vgwV5LiOperState           VgwV5LinkOperStateType,
      vgwV5LiAlarmState          VgwV5AlarmStateType,
      vgwV5LiAlarmSeverityIndex  VgwV5AlarmSeverityIndexType
    }

  -- *************************************************************************
  -- The items of a vgwV5LinkEntry
  -- *************************************************************************
  vgwV5LiLinkIndex OBJECT-TYPE
    SYNTAX  VgwV5LinkIndexType
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "The index in the vgwV5LinkTable[]."
  ::= { vgwV5LinkEntry 1 }

  vgwV5LiLinkId OBJECT-TYPE
    SYNTAX  VgwV5LinkIdType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The identifier of the V5 link (link-id). The identifier has to be
       unambiguous for the V5 interface."
    ::= { vgwV5LinkEntry 2 }

  vgwV5LiV5InterfaceIndex OBJECT-TYPE
    SYNTAX  VgwV5InterfaceIndexType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index of the V5 interface in the vgwV5InterfaceTable[]
       to which the logical link belongs."
  ::= { vgwV5LinkEntry 3 }

  vgwV5LiPhyLineIndex OBJECT-TYPE
    SYNTAX  VgwV5PhyLineIndexType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index of the physical E1 link in the xldv20IfTable[] which is
       used by this logical link."
  ::= { vgwV5LinkEntry 4 }

  vgwV5LiAdminState OBJECT-TYPE
    SYNTAX  VgwV5LinkAdminStateType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The administrative state of the V5 logical link."
    ::= { vgwV5LinkEntry 5 }

  vgwV5LiOperState OBJECT-TYPE
    SYNTAX  VgwV5LinkOperStateType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The operative state of the V5 link."
  ::= { vgwV5LinkEntry 6 }

  vgwV5LiAlarmState OBJECT-TYPE
    SYNTAX  VgwV5AlarmStateType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The current alarm state of the logical link. If the alarm is on,
       a link check failure is reported (wrong link-id)."
  ::= { vgwV5LinkEntry 7 }

  vgwV5LiAlarmSeverityIndex OBJECT-TYPE
    SYNTAX  VgwV5AlarmSeverityIndexType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "This index selects an item in the xldv20AlarmSeverityTable[]. It is
       used to assign the reported alarm to a severity."
  ::= { vgwV5LinkEntry 8 }


  -- *************************************************************************
  -- *************************************************************************
  -- vgwV5PhyComChanTable
  -- *************************************************************************
  -- *************************************************************************
  vgwV5PhyComChanTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF VgwV5PhyComChanEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "This table contains information about the configured physical
        communication channels at the CO-IWF."

        --DEFAULT next-function-async     fsnV5PhyComChanEntryNext
        --DEFAULT get-function-async      fsnV5PhyComChanEntryGet


  ::= { vgwV5Objects 3 }

  vgwV5PhyComChanEntry OBJECT-TYPE
    SYNTAX  VgwV5PhyComChanEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "An entry in the vgwV5PhyComChanTable[] that represents a single
       physical communication channel."
    INDEX { vgwV5PcPhyComChanIndex }
  ::= { vgwV5PhyComChanTable 1 }

  VgwV5PhyComChanEntry ::=
    SEQUENCE
    {
       vgwV5PcPhyComChanIndex     VgwV5PhyComChanIndexType,
       vgwV5PcProtectionGroup     VgwV5ProtectionGroupType,
       vgwV5PcLinkIndex           VgwV5LinkIndexType,
       vgwV5PcLinkTimeSlot        VgwV5TimeSlotType,
       vgwV5PcAlarmState          VgwV5AlarmStateType,
       vgwV5PcV5InterfaceIndex    VgwV5InterfaceIndexType,
       vgwV5PcAlarmSeverityIndex  VgwV5AlarmSeverityIndexType
     }

  -- *************************************************************************
  -- The items of a vgwV5PhyComChanEntry
  -- *************************************************************************
  vgwV5PcPhyComChanIndex OBJECT-TYPE
    SYNTAX  VgwV5PhyComChanIndexType
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "The index of the physical communication channel in the
      vgwV5PhyComChanTable[]."
  ::= { vgwV5PhyComChanEntry 1 }

  vgwV5PcProtectionGroup OBJECT-TYPE
    SYNTAX  VgwV5ProtectionGroupType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The protection group of the physical communication channel."
  ::= { vgwV5PhyComChanEntry 3 }

  vgwV5PcLinkIndex OBJECT-TYPE
    SYNTAX  VgwV5LinkIndexType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index of the logical link in the vgwV5LinkTable[]  which is used
       by this physical C-channel."
  ::= { vgwV5PhyComChanEntry 4 }

  vgwV5PcLinkTimeSlot OBJECT-TYPE
    SYNTAX  VgwV5TimeSlotType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The used timeslot on the assigned V5 link.
       If the physical C-channel belongs to protection group 1 only the timeslot
       16 is allowed."
  ::= { vgwV5PhyComChanEntry 5 }

  vgwV5PcAlarmState OBJECT-TYPE
    SYNTAX  VgwV5AlarmStateType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The state of the flag monitoring at the physical communication channel.
       An alarm 'loss of flags' is reported if no flags are detected. Note,
       the loss of flags reporting can be disabled by V5-profile if an LE
       does not send flags."
