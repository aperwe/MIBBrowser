  -- The operating state of an entity.
  VgwAAL2OperState ::=
     INTEGER {
        enabled(1),
        disabled(2)
     }

  -- The administrative state of an entity.
  VgwAAL2AdminState ::=
     INTEGER {
        unlocked(1),
        locked(2)
     }

  PrintString ::= OCTET STRING (SIZE (0..255))

  -- The Index of vgwAAL2IfTable
  -- (14 SU_VGW) * (1024 AAL2If per SU_VGW) = 14336
  VgwAAL2IfIndex ::= INTEGER (1..14336)

  -- The index in xldv20VccTpTable, and attribute in vgwAAL2IfTable
  VgwAAL2VccIndex ::= INTEGER

  -- The index in xldv20HwUnitTable, and attribute in vgwAAL2IfTable
  VgwAAL2HwUnitIndex ::= INTEGER

  -- Determines the selection algorithm for the used profile entry
  VgwAAL2SelectionMode ::=
     INTEGER {
        independent  (1),
        master-slave (2)
     }

  -- Application identifier specifies protocol combinations used between IWFs.
  -- Value of this object should be one of those values given in the
  -- administrated list of AppIDs in section 5 of the ATM Forum document
  -- entitled "ATM Forum Well-known Addresses and Assigned Codes".
  -- !! For the VGW version 2.0 this value is hardcoded to 0x000a for
  -- LES using PSTN and DSS1 with ELCP
  VgwAAL2AppId ::= INTEGER

  -- Maximum CPS-SDU size given in octets.
  -- !! For the VGW version 2.0 this value is hardcoded to 45.
  VgwAAL2CPSSDUSize ::=
     INTEGER {
        sdusize45 (45),
        sdusize64 (64)
     }

  -- Describes method used for multiplexing CPS-Packets into ATM cells.
  -- There are three optimizing methods:
  -- single CPS packet per SCP-PDU without overlapping
  -- multiple CPS packets per CPS-PDU with overlapping
  -- multiple CPS packets per CPS-PDU without overlapping
  -- !! For the VGW version 2.0 this value is hardcoded to 1.
  VgwAAL2Optimization ::=
     INTEGER {
        singlePacketNoOverlap (1),
        multiplePacketsOverlap (2),
        multiplePacketsNoOverlap (3)
     }


  -- The value of this object identifies the entry in vgwAAL2ProfileTable,
  -- which determines profile used for specific AAL2 connection.
  VgwAAL2ProfileIndex ::= INTEGER (1..4)

  -- Name of the organization that defined the profile
  VgwAAL2ProfileSource ::=
     INTEGER {
        atm-forum (1),
        itu-t (2)
     }

  -- Number that identify the profile (together with vgwAAL2ProfileSource)
  VgwAAL2ProfileNumber ::= INTEGER

  -- The state of a reported alarm. Each bit represents an alarm.
  -- The alarm is active if the bit is set.
  --  -bit 0: SigCErr: signaling channal error
  --  -bit 1: LoELCPDL: loss of ELCP data link
  --  -bit 2: LBC: loss of B-channel
  VgwAAL2AlarmState ::= INTEGER (0..7)



