  -- **********************************************************************
  --                          Commands
  -- **********************************************************************

  -- This type contains commands, error codes and states, which are
  -- used in vgwAAL2CgControlReq and vgwV5CgControlRequestResult.

  VgwAAL2CgCommandErrorStateType ::=
    INTEGER
    {
      -- states of command execution
      vgwAAL2NoRequest (1),          -- no request received
      vgwAAL2CommExecStarted (5),    -- command started
      vgwAAL2CommandSucceeded (16),  -- command executed

      -- general error codes ***********************************************
        -- Not enough parameters set for the command to be executed.
      vgwAAL2ErrParamFaulty (11),

        -- Command cannot be executed while another command is still executed.
      vgwAAL2ErrRequestBusy (12),

        -- The command could not be executed because some parameters contain
        -- invalid values (not allowed in the context of the command)
      vgwAAL2ErrSetDataFailed (13),

        -- Internal error (e.g. loss of communication with SU_VGW)
      vgwAAL2ErrAswTimeout (14),

        -- The requested command is not supported
      vgwAAL2ErrCommandFailed (15),

      -- command specific errors *******************************************

        -- too many AAL2 interfaces for this SU_VGW 
        -- the sum of ISDN D-channels and AAL2 interfaces cann be max. 2048
      vgwAAL2ErrLackOfResources	(50),

        -- specified VccIndex is not valid (out of range, or entry with
        -- this index does not exist)
      vgwAAL2ErrWrongVccIndex (51),

        -- specified HwUnitIndex is not valid (out of range, or entry
        -- with this index does not exist)
      vgwAAL2ErrWrongHwUnitIndex (52),

        -- selected HwUnit is not SU_VGW
      vgwAAL2ErrWrongHwUnitType (53),

        -- specified ProfileIndex is out of range
      vgwAAL2ErrWrongProfileIndex (54),

        -- HwUnit associated to VccTp is not the same to HwUnit specified
        -- in create operation
      vgwAAL2ErrVccNotMatchHwUnit (55),

        -- specified AAL2IfIndex is not valid (out of range, or entry with
        -- this index does not exist)
      vgwAAL2ErrWrongAAL2IfIndex (56),

        -- try to lock AAL2 interface that is already locked
      vgwAAL2ErrIfAlreadyLocked (57),

        -- try to unlock AAL2 interface that is already unlocked
      vgwAAL2ErrIfAlreadyUnlocked (58),

        -- selection mode of AAL2 interface is not valid
      vgwAAL2ErrWrongSelectionMode (59),

        -- when try to delete AAL2 interface that is use by V5.2
      vgwAAL2ErrIfInUse (60),


      -- commands ********************************************************

        -- create a new AAL2 interface
        -- parameters: vgwAAL2CgVccIndex, vgwAAL2CgHwUnitIndex,
        --             vgwAAL2CgProfileIndex, vgwAAL2CgSelectionMode
      vgwAAL2CreateIf 		        (100),

        -- delete a AAL2 interface
        -- parameters: vgwAAL2CgIfIndex
      vgwAAL2DeleteIf 		        (101),

        -- modify a AAL2 interface
        -- parameters: vgwAAL2CgIfIndex, vgwAAL2CgProfileIndex,
        --             vgwAAL2CgSelectionMode
      vgwAAL2ModifyIf                  (102),

        -- lock AAL2 interface
        -- parameters: vgwAAL2CgIfIndex
      vgwAAL2LockIf                    (103),

        -- unlock AAL2 interface
        -- parameters: vgwAAL2CgIfIndex
      vgwAAL2UnlockIf                  (104)

    }


  -- The control state of the control group
  VgwAAL2CgControlStatusType ::=
