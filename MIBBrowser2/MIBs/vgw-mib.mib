  -- *************************************************************************
  -- *************************************************************************
  -- vgwV5ControlGroup
  -- *************************************************************************
  -- *************************************************************************
  vgwV5ControlGroup OBJECT IDENTIFIER ::= { vgwV5Objects 8 }

  -- The commands and error codes. Note, vgwV5CgControlReq can contain
  -- a command as well as an error/state code. If it contains an error/state
  -- code, this value is only delivered in the getResponse. It is only
  -- allowed to write commands to this attribute.
  VgwV5CgCommandErrorStateType ::=
    INTEGER
    {
      -- *** states **********************************************************
      vgwV5NoRequest (1),          -- no request received
      vgwV5CommExecStarted (5),    -- command started
      vgwV5CommandSucceeded (16),  -- command executed

      -- *** general error codes *********************************************
        -- Not enough parameters set for the command to be executed.
      vgwV5ErrParamFaulty (11),
        -- Command cannot be executed while another command is still executed.
      vgwV5ErrRequestBusy (12),
        -- The command could not be executed because some parameters contain
        -- invalid values (not allowed in the context of the command)
      vgwV5ErrSetDataFailed (13),
        -- Internal error (e.g. loss of communication with SU_VGW)
      vgwV5ErrAswTimeout (14),
        -- The requested command is not supported
      vgwV5ErrCommandFailed (15),

      -- *** command specific errors *****************************************
        -- The number of V5.2 interfaces exceeds the max. number.
      vgwV5ErrTooManyV52Interfaces (50),
        -- The given HwUnitIndex contains an invalid value.
      vgwV5ErrWrongHwUnitIndex (51),
        -- The selected PIU is not a SU_VGW.
      vgwV5ErrWrongHwUnitType (52),
        -- The given link-id is not unambiguous for the V5.2 interface.
      vgwV5ErrWrongLinkId (53),
        -- The given E1 interface index contains an invalid value.
      vgwV5ErrWrongIfIndex (54),
        -- The selected interface is not an E1 interface.
      vgwV5ErrWrongIfType (55),
        -- The selected E1 interface is located on a wrong PIU.
      vgwV5ErrWrongIfLocation (56),
        -- The selected E1 interface is already assigned to another
        -- V5.2 interface.
      vgwV5ErrWrongPhyLinkState (57),
        -- The given V5.2 interface index contains an invalid value.
      vgwV5ErrWrongV5IfIndex (58),
        -- The given V5.2 interface is not in the right state
        -- (activated or deactivated)
      vgwV5ErrWrongIfState (59),
        -- There are too many links for the V5.2 interface configured.
      vgwV5ErrTooManyLinks (60),
        -- There are still physical C-channels which are using this
        -- logical link.
      vgwV5ErrLogLinkDep (61),
        -- The given index for the logical link contains an invalid value
      vgwV5ErrWrongLogLinkIndex (62),
        -- The logical link is in the wrong state (locked/unlocked).
      vgwV5ErrWrongLogLinkState (63),
        -- The given value of the timeslot is not allowed.
      vgwV5ErrWrongTimeSlot (64),
        -- The selected C-channel is of wrong type.
      vgwV5ErrWrongChannelType (65),
        -- The given index for the physical C-channel contains an invalid
        -- value
      vgwV5ErrWrongPhyChanIndex (66),
        -- The physical C-channel is still in use.
      vgwV5ErrPhyChanDep (67),
        -- The selected logical link does not belong to the right
        -- V5.2 interface.
      vgwV5ErrWrongLinkLocation (68),
        -- The given index for the logical channel contains an invalid value.
      vgwV5ErrWrongLogChanIndex (69),
        -- There is already a PSTN C-path for this V5.2 interface.
      vgwV5ErrPstnExist (70),
        -- The given index for the C-path contains an invalid value.
      vgwV5ErrWrongComPathIndex (71),
        -- There are too many user ports configured for this SU_VGW.
        -- (limitation: max. 4096)
      vgwV5ErrTooManyUserPorts (72),
        -- There are too many configured SSTED (to many ISDN user ports)
        -- for this SU_VGW.
        -- (limitation: max. 2048)
      vgwV5ErrTooManySSTED (73),
        -- The L3 address or EFA is invalid (out of range or not
        -- unambiguous for this V5.2 interface).
      vgwV5ErrWrongAddress (74),
        -- The given index for the user port interface (AN side) contains
        -- an invalid value.
      vgwV5ErrWrongUserPortIfIndex (75),
        -- The user port number is out of range or not unambiguous for
        -- one IAD and user port type.
      vgwV5ErrWrongUserPortId (76),
        -- The given index for the user port contains an invalid value.
      vgwV5ErrWrongUserPortIndex (77),
        -- The given channel-id is not unambiguous for this V5.2 interface.
      vgwV5ErrWrongChannelId (78),
        -- The given user port interface type is not supported.
      vgwV5ErrWrongUserPortIfType (79),
        -- The given index for the profile contains an invalid value.
      vgwV5ErrWrongProfileIndex (80),
        -- There are too many profiles
      vgwV5ErrTooManyProfiles (81),
        -- The given profile string is invalid (not a V5.2 profile or
        -- wrong checksum)
      vgwV5ErrWrongProfileString (82),
        -- The given index for the alarm severity contains an invalid value.
      vgwV5ErrWrongAlarmSeverityIndex (83),
        -- The given type of a user port is not supported.
      vgwV5ErrWrongUserPortType (84),
        -- The logical link is not in the operational state 'enabled'
      vgwV5ErrWrongLogLinkOST (85),
        -- No response from LE.
      vgwV5ErrTimeoutByLE (86),
        -- The link ID check cannot be executed because a link ID check
        -- is still in process.
      vgwV5ErrLinkIdCheckInProcress (87),
        -- The link check was rejected by AN or LE side.
      vgwV5ErrLinkIdCheckRejected (88),
        -- link ID check detected an error.
      vgwV5ErrLinkIdCheck (89),
        -- the PSTN C-path is still in use
      vgwV5ErrComPathDep (90),
        -- the interface cannot be removed as long as log. links, C-paths,
        -- C-channels or user ports configured for this interface
      vgwV5ErrIfDep (91),
        -- profile cannot be removed because it is still in use
        -- by a V5.2 interface
      vgwV5ErrProfileDep (92),
        -- internal error in SDT application (e.g. data base read error)
      vgwV5ErrInternal (93),

      -- *** the commands ****************************************************
        -- create a new V5.2 interface
        -- parameters: vgwV5CgInterfaceId, vgwV5CgVariantNo,
        --             vgwV5CgPrimLinkId, vgwV5CgPrimIfIndex,
        --             vgwV5CgSecLinkId, vgwV5CgSecIfIndex,
        --             vgwV5CgChannelId, vgwV5CgHwUnitIndex,
        --             vgwV5CgProfileIndex
      vgwV5CreateInterface (101),
        -- delete a V5.2 interface
        -- parameters: vgwV5CgV5IfIndex
      vgwV5DeleteInterface (102),
        -- activate a V5.2 interface
        -- parameters: vgwV5CgV5IfIndex
      vgwV5ActivateInterface (103),
        -- deactivate a V5.2 interface
        -- parameters: vgwV5CgV5IfIndex
      vgwV5DeactivateInterface (104),
        -- change the data of a V5.2 interface
        -- parameters: vgwV5CgV5IfIndex, vgwV5CgInterfaceId,
        --             vgwV5CgVariantNo, vgwV5CgAlarmSeverityIndex,
        --             vgwV5CgProfileIndex
      vgwV5ChangeInterfaceData (105),
        -- add a link to a V5.2 interface
        -- parameters: vgwV5CgV5IfIndex, vgwV5CgLinkId, vgwV5CgIfIndex,
        --             vgwV5CgLogLinkAdminState
      vgwV5AddLogLink (106),
        -- delete a link from a V5.2 interface
        -- parameters: vgwV5CgLinkIndex
      vgwV5DeleteLogLink (107),
        -- change the data of a logical link
        -- parameters: vgwV5CgLinkIndex, vgwV5CgLinkId, vgwV5CgIfIndex,
        --             vgwV5CgAlarmSeverityIndex
      vgwV5ChangeLogLinkData (108),
        -- lock a logical link
        -- parameters: vgwV5CgLinkIndex
      vgwV5LockLogLink (109),
        -- unlock a logical link
        -- parameters: vgwV5CgLinkIndex
      vgwV5UnlockLogLink (110),
        -- shutdown a logical link
        -- parameters: vgwV5CgLinkIndex
      vgwV5ShutdownLogLink (111),
        -- link-id check
        -- parameters: vgwV5CgLinkIndex
      vgwV5LinkIdCheck (112),
        -- create a C-channel
        -- parameters: vgwV5CgLinkIndex, vgwV5CgTimeSlot,
        --             vgwV5CgCChannelType, vgwV5CgChannelId
      vgwV5CreateCChannel (113),
        -- delete a C-channel
        -- parameters: vgwV5CgPhyChanIndex
      vgwV5DeleteCChannel (114),
        -- change the data of a C-channel
        -- parameters: vgwV5CgPhyChanIndex, vgwV5CgLinkIndex,
        --             vgwV5CgTimeSlot, vgwV5CgChannelId,
        --             vgwV5CgAlarmSeverityIndex
      vgwV5ChangeCCData (115),
        -- force a protection switch for C-channel (of protection group 2)
        -- parameters: vgwV5CgLogChanIndex, vgwV5CgPhyChanIndex
      vgwV5SwitchOver (116),
        -- create a PSTN C-path
        -- parameters: vgwV5CpLogChanIndex
      vgwV5CreatePstnPath (117),
        -- delete a PSTN C-path
        -- parameters: vgwV5CgComPathIndex
      vgwV5DeletePstnPath (118),
        -- change the data of a C-path
        -- parameters: vgwV5CgComPathIndex, vgwV5CgLogChanIndex,
        --             vgwV5CgAlarmSeverityIndex
      vgwV5ChangeCPathData (119),
        -- create a user port
        -- parameters: vgwV5CgV5IfIndex, vgwV5CgTypeOfPort,
        --             vgwV5CgAddress, vgwV5CgUserPortState,
        --             vgwV5CgUserPortIfIndex, vgwV5CgUserPortIfType,
        --             vgwV5CgUserPortId
        --             and for ISDN user port only...
        --             vgwV5CgDsTypePath, vgwV5CgPTypePath, vgwV5CgFTypePath
      vgwV5CreateUserPort (120),
        -- delete a user port
        -- parameters: vgwV5CgUserPortIndex
      vgwV5DeleteUserPort (121),
        -- lock a user port
        -- parameters: vgwV5CgUserPortIndex
      vgwV5LockUserPort (122),
        -- unlock a user port
        -- parameters: vgwV5CgUserPortIndex
      vgwV5UnlockUserPort (123),
        -- create a new profile
        -- parameters: vgwV5CgProfileStr
      vgwV5CreateProfile (124),
        -- delete a profile
        -- parameters: vgwV5CgProfileIndex
      vgwV5DeleteProfile (125)
    }

  -- The control state of the control group
  VgwV5CgControlStatusType ::=
    INTEGER
    {
      vgwV5RequestBusy (2),           -- command is just executing
      vgwV5RequestPassed (3),         -- command executed w/o error
      vgwV5RequestFailed (4)          -- command executed with error
    }

