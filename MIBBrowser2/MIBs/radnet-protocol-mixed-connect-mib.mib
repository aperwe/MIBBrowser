-- Revision   :   V2.101
--
-- Description:   Definition of Connection between two end points of
--		  different types.
--		  The connection defined in the module are:
--		  - ATM to CES connection.
--		  - ATM to Frame Relay connection.
--                - ATM to SMDS connection.
--
-- ----------------------------------------------------------------------
RADNET-PROTOCOL-MIXED-CONNECT-MIB DEFINITIONS ::= BEGIN

	IMPORTS
       		OBJECT-TYPE, MODULE-IDENTITY,
		OBJECT-IDENTITY,
		Integer32, Counter32            	FROM SNMPv2-SMI
	
		InterfaceIndex		FROM IF-MIB
	            
--      AtmAddr         FROM ATM-TC-MIB

		DisplayString,
		RowStatus, TruthValue,
		TimeStamp,
		TEXTUAL-CONVENTION	FROM SNMPv2-TC

		rnetProtocol		
				FROM RADNET-PROTOCOL-MIB;


rnetProtocolMixedConnections MODULE-IDENTITY
                    LAST-UPDATED "9709021300Z"
                    ORGANIZATION "RADNET"
                    CONTACT-INFO
                      "          Eitan Mardiks
                        Postal:  RADNET Ltd.
				24 Raul Wallenberg St.
				Tel-Aviv 69719, Israel.
                        Tel:     +972 3 6455700
                        Fax:     +972 3 6480582
                        E-Mail:  eitan@gate.radnet.co.il"

                    DESCRIPTION
                    "Definition of Connection between two end points of
					      different types.
					      The connection defined in the module are:
					      - ATM to CES connection.
					      - ATM to Frame Relay connection.
							- ATM to SMDS connection.
							- ATM to LAN connection."
                    ::= { rnetProtocol 5 }

		
-- --------------------------------------------------------------
-- RADNET Connections Objects
-- --------------------------------------------------------------

rnetProtocolAtmCeConnection	
		OBJECT IDENTIFIER ::= { rnetProtocolMixedConnections 1 }
rnetProtocolAtmFrConnection	
		OBJECT IDENTIFIER ::= { rnetProtocolMixedConnections 2 }
rnetProtocolAtmSmdsConnection   
      OBJECT IDENTIFIER ::= { rnetProtocolMixedConnections 3 }
rnetProtocolAtmLanConnection   
      OBJECT IDENTIFIER ::= { rnetProtocolMixedConnections 4 }


AtmAddr ::= TEXTUAL-CONVENTION
--			DISPLAY_HINT "1x"
			STATUS  current
			DESCRIPTION
								""
			SYNTAX  OCTET  STRING (SIZE(0|8|20))



-- --------------------------------------------------------------
-- RADNET ATM to CE endpoints Connections group
-- --------------------------------------------------------------
	
rnetProtocolAtmCeConnectIndexNext   OBJECT-TYPE
        SYNTAX INTEGER (1..2147483647)
	MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
               "The next available value of rnetProtocolAtmCeConnectIndex,
		when creating entries in the rnetProtocolAtmCeConnectTable.
		To obtain the rnetProtocolAtmCeConnectIndex value for a new 
		entry, the manager performs a READ operation to 
		obtain the current value of this object. After each 
		READ operation of this object, the agent must 
		modify it to the next available value. "
	::= { rnetProtocolAtmCeConnection 1 }


-- --------------------------------------------------------------
--  RADNET ATM to CES connection table.
-- --------------------------------------------------------------

rnetProtocolAtmCeConnectTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF RnetProtocolAtmCeConnectEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"This table defines virtual connections between an ATM 
		 end point and a CES interface." 
	::= { rnetProtocolAtmCeConnection 2 }


rnetProtocolAtmCeConnectEntry	OBJECT-TYPE
	SYNTAX	RnetProtocolAtmCeConnectEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
	        "An entry defines a virtual connection between an ATM 
		 end point and a CES interface." 
	INDEX	{ rnetProtocolAtmCeConnectIndex,
		  rnetProtocolAtmCeConnectProtocolIndex,
		  rnetProtocolAtmCeConnectAtmVpi,
		  rnetProtocolAtmCeConnectAtmVci,
		  rnetProtocolAtmCeConnectCeIfIndex }
	::= { rnetProtocolAtmCeConnectTable 1 }

RnetProtocolAtmCeConnectEntry ::= SEQUENCE {
	rnetProtocolAtmCeConnectIndex
		INTEGER,
	rnetProtocolAtmCeConnectProtocolIndex
		InterfaceIndex,
	rnetProtocolAtmCeConnectAtmVpi
		INTEGER,
	rnetProtocolAtmCeConnectAtmVci
		INTEGER,
	rnetProtocolAtmCeConnectCeIfIndex
		InterfaceIndex,
	rnetProtocolAtmCeConnectAdminStatus
		INTEGER,
	rnetProtocolAtmCeConnectAtm2CeOperStatus
		INTEGER,
	rnetProtocolAtmCeConnectCe2AtmOperStatus
		INTEGER,
	rnetProtocolAtmCeConnectAtm2CeLastChange
		TimeStamp,
	rnetProtocolAtmCeConnectCe2AtmLastChange
		TimeStamp,
	rnetProtocolAtmCeConnectBusSelection
		INTEGER,
	rnetProtocolAtmCeConnectRowStatus
		RowStatus,
    rnetProtocolAtmCeConnectCbrService                 
		INTEGER,
    rnetProtocolAtmCeConnectCbrClockMode               
		INTEGER,
    rnetProtocolAtmCeConnectCas                        
		INTEGER,
    rnetProtocolAtmCeConnectPartialFill                
		INTEGER,
    rnetProtocolAtmCeConnectBufMaxSize                 
		INTEGER,
    rnetProtocolAtmCeConnectCdvRxT                     
		INTEGER,
    rnetProtocolAtmCeConnectCellLossIntegrationPeriod  
		INTEGER,
    rnetProtocolAtmCeConnectConnType                   
		INTEGER,
    rnetProtocolAtmCeConnectLocalAddr                  
		AtmAddr
	}

rnetProtocolAtmCeConnectIndex   OBJECT-TYPE
        SYNTAX INTEGER (1..2147483647)
	MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
		"The index of this table."
	::= { rnetProtocolAtmCeConnectEntry 1 }

rnetProtocolAtmCeConnectProtocolIndex OBJECT-TYPE
        SYNTAX InterfaceIndex
	MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
		"The ifIndex associated to the ATM interface."
	::= { rnetProtocolAtmCeConnectEntry 2 }

rnetProtocolAtmCeConnectAtmVpi   OBJECT-TYPE
        SYNTAX INTEGER (1..4095)
	MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
		"The Vpi number of the ATM end point."
	::= { rnetProtocolAtmCeConnectEntry 3 }

rnetProtocolAtmCeConnectAtmVci   OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
	MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
		"The Vci number of the ATM end point."
	::= { rnetProtocolAtmCeConnectEntry 4 }

rnetProtocolAtmCeConnectCeIfIndex   OBJECT-TYPE
        SYNTAX InterfaceIndex
	MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
		"The ifIndex associated to the CES interface."
	::= { rnetProtocolAtmCeConnectEntry 5 }

rnetProtocolAtmCeConnectAdminStatus   OBJECT-TYPE
        SYNTAX INTEGER {
         up(1),
			down(2)
	}
	MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
		"The desired state of the ATM to CES connection."
	::= { rnetProtocolAtmCeConnectEntry 6 }

rnetProtocolAtmCeConnectAtm2CeOperStatus   OBJECT-TYPE
        SYNTAX INTEGER {
			up(1),
			down(2),
			unknown(3)
	}
	MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
		"The current operational state of the ATM to CES
		 connection."
	::= { rnetProtocolAtmCeConnectEntry 7 }

rnetProtocolAtmCeConnectCe2AtmOperStatus   OBJECT-TYPE
        SYNTAX INTEGER {
			up(1),
			down(2),
			unknown(3)
	}
	MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
		"The current operational state of the CES to ATM
		 connection."
	::= { rnetProtocolAtmCeConnectEntry 8 }

rnetProtocolAtmCeConnectAtm2CeLastChange   OBJECT-TYPE
	SYNTAX TimeStamp
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The value of sysUpTime at the time the ATM to CES
                 connection entered its current operational state.  If 
	         the current state was entered prior to the last 
	         re-initialization of the local network management 
	         subsystem, then this object contains a zero value."
	::= { rnetProtocolAtmCeConnectEntry 9 }

rnetProtocolAtmCeConnectCe2AtmLastChange   OBJECT-TYPE
        SYNTAX TimeStamp
	MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
		"The value of sysUpTime at the time the CES to ATM
                 connection entered its current operational state.  If 
	         the current state was entered prior to the last 
	         re-initialization of the local network management 
	         subsystem, then this object contains a zero value."
	::= { rnetProtocolAtmCeConnectEntry 10 }

rnetProtocolAtmCeConnectBusSelection   OBJECT-TYPE
        SYNTAX INTEGER {
			first(1),
			second(2),
			automatic(3)
	}
	MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
		"An internal bus entry."
        DEFVAL { automatic }
	::= { rnetProtocolAtmCeConnectEntry 11 }

rnetProtocolAtmCeConnectRowStatus   OBJECT-TYPE
        SYNTAX RowStatus
	MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
		"This object is used to create
		 a new row or modify or delete an
		 existing row in this table."
	::= { rnetProtocolAtmCeConnectEntry 12 }


rnetProtocolAtmCeConnectCbrService       OBJECT-TYPE
    SYNTAX        INTEGER {
                      unstructured(1),
                      structured(2)
                      }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Define if DSx/Ex service isas structured or unstructurednot. A
         structured(2) interface is some nx64kbKbps. An unstructured
         (1) interface is 1.544Mbps, 2.048Mbps, 6.312Mbps, 44.736 Mbps,
         or 34.368 Mbps. unstructured(1) passes all bits through the
         ATM network.  strucutured(2) passes data bits through the
         ATM network, and may also pass signalling bits
         
         At this time, only unstructured mode is defined for the
         44.736 Mbps and 34.368 Mbps services.

			The same parameter is defined in the rnetProtocolDSX1ConfigTable  with read			-create access"
    ::= { rnetProtocolAtmCeConnectEntry 13 }

rnetProtocolAtmCeConnectCbrClockMode     OBJECT-TYPE
    SYNTAX        INTEGER {
                      synchronous(1),
                      srts(2),
                      adaptive(3)
                      }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Define if DSx/Ex service clocking mode. This maps into
         transmit clock source of CBR interface.

         For structured modes this value, if present, must be set to
         synchronous(1).
			
			The same parameter is defined in the rnetProtocolDSX1ConfigTable with read			-create access"
    DEFVAL { synchronous } 
    ::= { rnetProtocolAtmCeConnectEntry 14 }

rnetProtocolAtmCeConnectCas      OBJECT-TYPE
    SYNTAX        INTEGER {
                      basic(1),
                      e1Cas(2),
                      ds1SfCas(3),
                      ds1EsfCas(4),
                      j2Cas(5)
                      }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This parameter selects which AAL1 Format should be used:
         Basic does not carry CAS bits, and uses a single 125 usec frame.
         e1Cas, ds1SfCas, dS1EsfCas and j2Cas carry CAS bits in a
         multiframe structure for E1, DS1 SF, DS1 ESF and J2 
         respectively.
         This applies to structured interfaces only. Default is
         basic (1). For unstructured interfaces this value, if present,
         must be set to the default of basic (1).

			
			The same parameter is defined in the rnetProtocolDSX1ConfigTable with read			-create access."
    DEFVAL { basic } 
    ::= { rnetProtocolAtmCeConnectEntry 15 }

rnetProtocolAtmCeConnectPartialFill     OBJECT-TYPE
    SYNTAX        INTEGER (0 .. 47)
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "If partial cell fill is used, the number of user octets per
         cell must be set with this parameter.  Setting this parameter
         to zero disables partial cell fill, and causes all cells to
         be completely filled before they are sent."
    DEFVAL { 0 } -- Partial Cell Fill not used
    ::= { rnetProtocolAtmCeConnectEntry 16 }

rnetProtocolAtmCeConnectBufMaxSize      OBJECT-TYPE
    SYNTAX        INTEGER  (1..65536)
    UNITS         "10 usec"
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
         "Define maximum size in 10 microsecond increments of the
          reassembly buffer. Some implementations may want allow the
          maximum buffer size to be programmed to a size less than the
          physical limit to reduce the maximum delay through a circuit."
    DEFVAL { 210 } 
    ::= { rnetProtocolAtmCeConnectEntry 17 }

rnetProtocolAtmCeConnectCdvRxT         OBJECT-TYPE
    SYNTAX        INTEGER  (1..65535)
    UNITS         "10 usec"
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "The maximum cell arrival jitter in 10 usec increments that 
         the reassembly process will tolerate in the cell stream
         without producing errors on the CBR service interface."
    DEFVAL { 100 } 
    ::= { rnetProtocolAtmCeConnectEntry 18 }

rnetProtocolAtmCeConnectCellLossIntegrationPeriod   OBJECT-TYPE
    SYNTAX        INTEGER  (1000 .. 65535)
    UNITS         "msec"
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "The time in milliseconds for the cell loss integration period. 
         If a cells are continuously lost for this period of time,
         rnetProtocolAtmCeConnectCellLossStatus is set to loss (2). The
         default definition is 2500."
    DEFVAL { 2500 } 
    ::= { rnetProtocolAtmCeConnectEntry 19 }

rnetProtocolAtmCeConnectConnType              OBJECT-TYPE
    SYNTAX        INTEGER {
                      other(1),
                      pvc(2),
                      activeSvc(3),
                      passiveSvc(4)
                      }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The type of ATM connectivity between associated CES IWF's.
         Valid values are:
            other      - none of the types specified below
            pvc        - supporting connectivity is a permanent
                         virtual connection
            activeSvc  - attempt calls whenever none established
            passiveSvc - accept calls"
    ::= { rnetProtocolAtmCeConnectEntry 20 }

rnetProtocolAtmCeConnectLocalAddr              OBJECT-TYPE
    SYNTAX        AtmAddr
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "The ATM address of the local CES IWF process. This address
         may be used by the automatic SVC establishment procedures to
         identify the intended recipient CES IWF of an incoming automatic
         SVC call request. Optionally, the MAX-ACCESS of this object
         may be read-only, for those implementations where it is
         not desired to manually configure this address. 
			This field is currently N/A."
    ::= { rnetProtocolAtmCeConnectEntry 21 }


-- CES Statistics Table

rnetProtocolAtmCeStatsTable    OBJECT-TYPE
    SYNTAX        SEQUENCE OF RnetProtocolAtmCeStatsEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The CES AAL1 statistical data table."
    ::= { rnetProtocolAtmCeConnection 3 }

rnetProtocolAtmCeStatsEntry    OBJECT-TYPE
    SYNTAX        RnetProtocolAtmCeStatsEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "An entry in the CES AAL1 Stats table.  There is one
         entry in this table corresponding to each entry in the
         rnetProtocolAtmCeConnectConfTable."
    AUGMENTS  { rnetProtocolAtmCeConnectEntry }
    ::= { rnetProtocolAtmCeStatsTable 1 }

RnetProtocolAtmCeStatsEntry ::= SEQUENCE  {
    rnetProtocolAtmCeStatsReassCells           Counter32,
    rnetProtocolAtmCeStatsHdrErrors            Counter32,
    rnetProtocolAtmCeStatsPointerReframes      Counter32,
    rnetProtocolAtmCeStatsPointerParityErrors  Counter32,
    rnetProtocolAtmCeStatsAal1SeqErrors        Counter32,
    rnetProtocolAtmCeStatsLostCells            Counter32,
    rnetProtocolAtmCeStatsMisinsertedCells     Counter32,
    rnetProtocolAtmCeStatsBufUnderflows        Counter32,
    rnetProtocolAtmCeStatsBufOverflows         Counter32,
    rnetProtocolAtmCeStatsCellLossStatus       INTEGER
    }

rnetProtocolAtmCeStatsReassCells         OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This count gives the number of cells played out to the
         CES Service Interface. It excludes cells that were
         discarded for any reason, including cells that were not used
         due to being declared misinserted, or discarded while the
         reassembler was waiting to achieve synchronization."
    ::= { rnetProtocolAtmCeStatsEntry 1 }

rnetProtocolAtmCeStatsHdrErrors         OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The count of the number of AAL1 header errors detected,
         including those corrected.  Header errors include correctable
         and uncorrectable CRC, plus bad parity."
    ::= { rnetProtocolAtmCeStatsEntry 2 }

rnetProtocolAtmCeStatsPointerReframes    OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This records the count of the number of events in which the
         AAL1 reassembler found that an SDT pointer is not where it is
         expected, and the pointer must be reacquired. This count is
         only meaningful for structured CES modes, as unstructured CES
         modes do not use pointers. For unstructured CES modes, this
         count, if present, should indicate zero."
    ::= { rnetProtocolAtmCeStatsEntry 3 }

rnetProtocolAtmCeStatsPointerParityErrors    OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This records the count of the number of events in which the
         AAL1 reassembler detects a parity check failure at the point
         where a structured data pointer is expected. This count is only
         meaningful for structured CES modes, as unstructured CES modes
         do not use pointers. For unstructured CES modes, this count, if
         present, should indicate zero."
    ::= { rnetProtocolAtmCeStatsEntry 4 }

rnetProtocolAtmCeStatsAal1SeqErrors      OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Number of times that the sequence number of an incoming AAL1
         Type 1 SAR-PDU causes a transition from the 'sync' state to
         the 'out of sequence' state, as defined by ITU-T I.363.1."
    ::= { rnetProtocolAtmCeStatsEntry 5 }

rnetProtocolAtmCeStatsLostCells   OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Number of lost cells, as detected by the AAL1 sequence number
         processing, for example. This records the count of the number
         of cells detected as lost in the network prior to the
         destination CES IWF AAL1 layer processing."
    ::= { rnetProtocolAtmCeStatsEntry 6 }

rnetProtocolAtmCeStatsMisinsertedCells   OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Number of AAL1 sequence violations which the AAL Convergence
         sublayer interprets as a misinserted cell, as defined by
         ITU-T I.363.1."
    ::= { rnetProtocolAtmCeStatsEntry 7 }

rnetProtocolAtmCeStatsBufUnderflows      OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Number of buffer underflows. This records the count of the
         number of times the CES reassembly buffer underflows. In the
         case of a continuous underflow caused by a loss of ATM cell
         flow, a single buffer underflow should be counted. If the CES
         IWF is implemented with multiple buffers (such as a cell level
         buffer and a bit level buffer), then either buffer underflow
         will cause this count to be incremented."
    ::= { rnetProtocolAtmCeStatsEntry 8 }

rnetProtocolAtmCeStatsBufOverflows      OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Number of buffer overflows. This records the count of the
         number of times the CES reassembly buffer overflows. If the CES
         IWF is implemented with multiple buffers (such as a cell level
         buffer and a bit level buffer, then either buffer overflow will
         cause this count to be incremented "
    ::= { rnetProtocolAtmCeStatsEntry 9 }

rnetProtocolAtmCeStatsCellLossStatus      OBJECT-TYPE
    SYNTAX        INTEGER {
                     noLoss(1),
                     loss(2)
                     }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "When cells are continuously lost for the number of milliseconds
         specified by rnetProtocolAtmCeStatsCellLossIntegrationPeriod, the value is set
         to loss (2). When cells are no longer lost, the value is set 
         to noLoss (1)."
    ::= { rnetProtocolAtmCeStatsEntry 10 }


