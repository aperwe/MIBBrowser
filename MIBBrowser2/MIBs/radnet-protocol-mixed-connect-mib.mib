    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This counter counts the total number of frames that 
         are transmitted to the ATM fabric from the 
         DXI interface"
    ::= { rnetProtocolAtmSmdsStatsEntry 1 }

rnetProtocolAtmSmdsStatsEgressFrames         OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This counter counts the total number of frames 
         received on the ATM fabric to be transmitted onto
         the DXI interface"
    ::= { rnetProtocolAtmSmdsStatsEntry 2 }

rnetProtocolAtmSmdsStatsTotalErrorCount         OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This counter counts the total number of errors
         detected on the end point"
    ::= { rnetProtocolAtmSmdsStatsEntry 3 }

rnetProtocolAtmSmdsStatsInvalidLength         OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "This counter counts the number of AAL3/4 frames
         received with invalid length indication"
    ::= { rnetProtocolAtmSmdsStatsEntry 4 }

