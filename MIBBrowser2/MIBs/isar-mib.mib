isarIfrAtmNiIwfConnFramesInError OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
	   "A Statistical counter of number of Frames
            received in error over this PVC."
    ::= { isarIfrAtmNiIwfConnectionEntry  18 }

isarIfrAtmNiIwfConnFramesDiscard OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
	   "A statistical counter of number of Frames
            Discarded by FR-SSCS for this PVC."
    ::= { isarIfrAtmNiIwfConnectionEntry  19 }

isarIfrAtmNiIwfConnSscsVCSigProtocol OBJECT-TYPE
    SYNTAX  INTEGER {
                      none(1),
                      lmi(2),
                      ansiT1617D(3),
                      ccittQ933A(4)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The value of this object identifies the Local
            In-Channel Signaling Protocol that is used at
            the sscs level for this interworking connection.
            To be compliant with FRF.5 the value should be ccittQ933A.
            Modification of this value is not allowed.
            The SNMPv2 MAX-ACCESS is read(only)-create."
