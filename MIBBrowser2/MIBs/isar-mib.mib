    DESCRIPTION
           "One row in the isarIhdlcLogPortMappingTable."
    INDEX   { ifIndex }
    ::= { isarIhdlcLogPortMappingTable  1 }


IsarIhdlcLogPortMappingEntry ::=
    SEQUENCE { 
      isarIhdlcLogPortMappedIndex
        INTEGER
    }
    
isarIhdlcLogPortMappedIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the HDLC logical port in
            isarIhdlcLogPortTable."
    ::= { isarIhdlcLogPortMappingEntry  1 }


