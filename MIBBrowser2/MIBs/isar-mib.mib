    ::= { isarIimaLinkEntry  24 }

isarIimaLinkFeTxNumFailures OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  optional
    DESCRIPTION
           "The number of times a far-end transmit failure alarm 
            condition has been entered on this link. This is an 
            optional attribute."
    ::= { isarIimaLinkEntry  25 }

isarIimaLinkFeRxNumFailures OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
