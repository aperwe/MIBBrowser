    INDEX   { isarIphysSlotId }
    ::= { isarIbriConfigTable  1 }

IsarIbriConfigEntry ::=
    SEQUENCE {

    isarIbriConfigNumberS0Ports
        INTEGER,

    isarIbriConfigS0Version
        OCTET STRING,

    isarIbriConfigIfType
        INTEGER,

    isarIbriConfigLineTopology
        INTEGER,

    isarIbriConfigIfMode
        INTEGER,

    isarIbriConfigSignalMode
        INTEGER,

    isarIbriConfigPowerSupplyAvailable
        INTEGER,

    isarIbriConfigClockReferenceSelect
        INTEGER,

    isarIbriConfigNumberPoweredLinesIAD
        INTEGER
    }

isarIbriConfigNumberS0Ports OBJECT-TYPE
    SYNTAX  INTEGER ( 4 | 8 )
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Shows the number of equipped S0 ports for this
            type of DPM:BRI."
    ::= { isarIbriConfigEntry  1 }

isarIbriConfigS0Version OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The HW version of the S0 port components equipped on
            this type of DPM:BRI (all S0 ports have the same type
            of HW component)."
    ::= { isarIbriConfigEntry  2 }

isarIbriConfigIfType OBJECT-TYPE
    SYNTAX  INTEGER {
                      isdns(75),
                      isdnu(76)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The physical interface type. For 'S/T' interfaces,
            also called 'Four-wire Basic Access Interface',
            the value of this object is isdns(75).
            For 'U' interfaces, also called 'Two-wire Basic
            Access Interface', the value of this object is
            isdnu(76).
            IAD4700 is always active on the S/T interface. (There
            is no corresponding entry in the ifTable.)"
    DEFVAL { isdns }
    ::= { isarIbriConfigEntry  3 }

isarIbriConfigLineTopology OBJECT-TYPE
    SYNTAX  INTEGER {
                      pointToPoint(1),
                      pointToMultipoint(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The line topology to be used for this interface.
            Note that setting isarIbriConfigIfType to isdns(75)
            does not necessarily mean a line topology of
            point-to-multipoint.
            For IAD4700 it is always point-to-point:
             1. in case connected to PBX -> p-t-p
             2. in case connected to TE : the IAD4700 is configured
                in nt mode -> p-t-p."
    DEFVAL { pointToPoint }
    ::= { isarIbriConfigEntry  4 }

isarIbriConfigIfMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      te(1),
                      nt(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The physical interface mode. For TE mode, the value
            of this object is te(1). For NT mode, the value
            of this object is nt(2)."
    DEFVAL { te }
    ::= { isarIbriConfigEntry  5 }

isarIbriConfigSignalMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      active(1),
                      inactive(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The signaling channel operational mode for this interface.
            If active(1) there is a signaling channel on this
            interface. If inactive(2) a signaling channel is
            not available.
            For IAD4700, the D channel is always present."
    DEFVAL { active }
    ::= { isarIbriConfigEntry  6 }

isarIbriConfigPowerSupplyAvailable OBJECT-TYPE
    SYNTAX  INTEGER {
                      true(1),
                      false(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Indicates the current status of the Power Supply.
            false: power supply not connected to this DPM
            true: power supply connected to this DPM."
    DEFVAL { false }
    ::= { isarIbriConfigEntry  7 }

isarIbriConfigClockReferenceSelect OBJECT-TYPE
    SYNTAX  INTEGER (0..7)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "If the IAD4700 clock is set to slave on this DPM:BRI (only
            meaningfull if isarIbriConfigIfMode equals 'te'), the clock
            of the indicated S0 channel is used as synchronisation clock."
    DEFVAL { 0 }
    ::= { isarIbriConfigEntry  8 }

isarIbriConfigNumberPoweredLinesIAD OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Integer representing the number of lines for which 'line
            power' is enabled."
    DEFVAL { 0 }
    ::= { isarIbriConfigEntry  9 }




