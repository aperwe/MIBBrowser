--
-- ISAR tftpMIBObjects : begin
--  
-- enterprise MIB for configuration TFTP
-- The MIB is written for SNMPv1.



-- For TFTP a server specific userid and password are not needed.
-- TFTP MIB objects are not persistent


isarItftpLock OBJECT-TYPE
    SYNTAX  INTEGER {
                      true(1),
                      false(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This flag indicates if the IAD4700 file system is locked for
            TFTP actions by an SNMP manager (based on IpAddress).
            If the IAD4700 file system is not locked for TFTP actions 
            (isarItftpLock = false), one SNMP manager can lock it by 
            setting isarItftpLock to true.  TFTP actions can be done by  
            this SNMP manager. TFTP actions from other SNMP managers (= 
            other IpAddress) will be refuced.
            The IAD4700 file system can be unlocked for TFTP actions on 3 
            ways :
            1. The SNMP manager who has set isarItftpLock to true can set 
               isarItftpLock to false.
            2. automatically after end of TFTP actions
            2. after a time out (timer value larger than time to download 
               all selected load palettes)."
    ::= { isarItftpMIBObjects 1 }


-- The following objects can only be changed when isarItftpLock=true
-- (reading is always possible)

isarItftpIpAddress OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "IP address of TFTP server platform from where download/upload
            action will take place.
            Default=0.0.0.0 ."
    ::= { isarItftpMIBObjects  2 }

isarItftpPort OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "A non-zero value specifies the portnumber of the TFTP server.
            Zero means that the default port number (=69 for TFTP) will be
            used."
    ::= { isarItftpMIBObjects  3 }


isarItftpError OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "A string that gives more information about the reason why
            tftp failed (e.g. lack of resources)."
    ::= { isarItftpMIBObjects  12 }





-- It should be possible to upload (from IAD4700 to TFTP server) or download
-- (from TFTP server to IAD4700) a single file of a primary or secondary MAMO
-- or active PM load palette.
-- This single file can be :
-- for the MAMO : initialisation file, MLS file, SNMP configuration file
--                and the configuration file (= persistent store of the
--                running load)
-- for an active PM : initialisation file

isarItftpFileLoad OBJECT-TYPE
    SYNTAX  INTEGER {
                      primaryMamo(1),
                      secondaryMamo(2),
                      primaryPM1(3),
                      secondaryPM1(4),
                      primaryPM2(5),
                      secondaryPM2(6),
                      primaryPM3(7),
                      secondaryPM3(8)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "MAMO or PM Load Palette from/to where the file will be
            uploaded/downloaded.
            PMx is the PM in slotx.
            Initial value = secondaryMamo"
    ::= { isarItftpMIBObjects 4 }

isarItftpFileLocation OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (0..100))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Location (directory and filename) where TFTP server 
            will look for or put the file. This location must be
            relative to the location where the TFTP server is 
            started. 
            E.g. '/tftp/upload' is used as directory for upload; 
                 TFTP server is started in '/tftp';
                 isarItftpFileLocation must be set to 
                 '/upload/filename'.
            Initial value = an empty string."
    ::= { isarItftpMIBObjects 5 }

isarItftpFileAction OBJECT-TYPE
    SYNTAX  INTEGER {
                      noAction(1),
                      uploadConfigFile(2),
                      downloadConfigFile(3),
                      uploadInifile(4),
                      downloadInifile(5),
                      uploadMlsfile(6),
                      downloadMlsfile(7),
                      uploadSnmpConfigfile(8),
                      downloadSnmpConfigfile(9)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "1. read-only value (SET of this value is not allowed)
            2. TFTP upload (IAD4700 -> TFTP server) of configuration file
            3. TFTP download (TFTP server -> IAD4700) of configuration file.
            4. TFTP upload (IAD4700 -> TFTP server) of initialisation file
            5. TFTP download (TFTP server -> IAD4700) of initialisation file.
            6. TFTP upload (IAD4700 -> TFTP server) of MLS file
            7. TFTP download (TFTP server -> IAD4700) of MLS file.
            8. TFTP upload (IAD4700 -> TFTP server) of SNMP config file
            9. TFTP download (TFTP server -> IAD4700) of SNMP config file.
            Initial value = noAction (reset by IAD4700)."
    ::= { isarItftpMIBObjects 6 }

isarItftpFileStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      initial(1),
                      tftpActionStarted(2),
                      tftpActionSucceeded(3),
                      tftpActionFailed(4),
                      fileNotFound(7),
                      emptySlot(8),
                      unknownModule(9),
                      invalidModule(10),
                      notArmedModule(11),
                      passiveModule(12),
                      slotEntryNotFound(13),
                      actionNotAllowed(14)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           " 1. initial status
             2. download/upload of file started
             3. download/upload of file successful
             4. download/upload of file unsuccessful
             7. unable to open or create the file on IAD4700
             8. action not started because isarIslotType = empty
             9. action not started because isarIslotType = unknown
            10. action not started because isarIslotType = invalid
            11. action not started because isarIisArmed = false
            12. action not started because isarIssCompatibility = 
                notApplicable
            13. entry in slot table not found
            14. invalid combination of isarItftpFileAction and 
                isarItftpFileLoad (active PM has no configuration files 
                and no MLS file)
            Initial value = initial (reset by IAD4700 at the start of a 
            new action)."
    ::= { isarItftpMIBObjects 7 }

isarItftpFileTimeStamp OBJECT-TYPE
    SYNTAX  TimeTicks
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Time when TFTP action has ended.
            Initial value = 0 (reset by IAD4700 at the start of a 
            new action)."
    ::= { isarItftpMIBObjects 8 }






-- The MAMO and each armed active PM can have both a primary and a 
-- secondary load palette. It should be possible to download (from TFTP 
-- server to IAD4700) such a load palette (without configuration file). 
-- The load palettes are stored on the corresponding module and this means
-- that when an active PM is not armed, it is not possible to download a 
-- load palette for this PM.
-- The isarItftpLoadTable contains for each slot, information about 
-- load palettes to be downloaded.


