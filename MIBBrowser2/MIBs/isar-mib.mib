-- Call and connection control information


isarIatmInterfaceCcConfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIatmInterfaceCcConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "These objects are not used by the IAD4700 software.
            They are needed for path management via the 46020.  Path management
            of the 46020 selects the VPI, VCI itself.  The information in this
            table will be used to tell the 46020 not to choose a value within 
            the indicated range because they are reserved for CCC SVCs.
            The information of this table is not visible in the LCT.
            Rows are created at startup of the IAD4700.
            Values can be changed at any time."
    ::= { isarIatmMIBObjects  18 }

isarIatmInterfaceCcConfEntry OBJECT-TYPE
    SYNTAX  IsarIatmInterfaceCcConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIatmInterfaceCcConfTable.
            
            INDEX : ifIndex is the index of the ATM layer."
    INDEX   { ifIndex }
    ::= { isarIatmInterfaceCcConfTable  1 }

IsarIatmInterfaceCcConfEntry ::=
    SEQUENCE {
      isarIatmInterfaceCcResBw
        INTEGER,

      isarIatmInterfaceCcResVpiVciRange
        DisplayString
    }

isarIatmInterfaceCcResBw OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Reserved bandwidth at the associated ATM interface.
            UNITS = kBits/sec"
    DEFVAL { 0 }
    ::= { isarIatmInterfaceCcConfEntry  1 }


isarIatmInterfaceCcResVpiVciRange OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (0..255))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Reserved VPI/VCI range for CCC SVCs. 
            The VPI/VCI range is a string with one or more 'VPI/VCI 
            ranges' separated by a ';'.
            A VPI/VCI range is a VPI range and a VCI range separated by 
            a ':'.
            The maximum number of VPI/VCI ranges is 4. 
            There can be no overlap of VPI ranges for multiple VPI/VCI 
            ranges. 
            The VCIs have to be between 32 and 1024.
            E.g. 10-15:100-200;20:32-40.  Thsi means: for VPIs 10 to 15
                 the VCIs 100 to 200 are reserved and for VPI = 20 the 
                 VCIs 32 to 40 are reserved."
    DEFVAL { "" }
    ::= { isarIatmInterfaceCcConfEntry  3 }



--  The ATM Interface VCL read-only Table

