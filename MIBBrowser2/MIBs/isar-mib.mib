isarIspeechBundleEnableEchoCancel OBJECT-TYPE
    SYNTAX  INTEGER {
                      true(1),
                      false(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enable echo cancellation.
            The SNMPv2 MAX-ACCESS is read-create."
    DEFVAL { true }
    ::= { isarIspeechBundleEntry  8 }

isarIspeechBundleEnableSilenceCompression OBJECT-TYPE
    SYNTAX  INTEGER {
                      true(1),
                      false(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enable echo cancellation.
            The SNMPv2 MAX-ACCESS is read-create."
    DEFVAL { false }
    ::= { isarIspeechBundleEntry  9 }

isarIspeechBundleInputGain OBJECT-TYPE
    SYNTAX  INTEGER (-31..31)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The voice input gain in db.
            
            The SNMPv2 MAX-ACCESS is read-create."
    DEFVAL { 0 }
    ::= { isarIspeechBundleEntry  10 }

isarIspeechBundleLowerLayerIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the layer on which the speech bundle is
            defined.
            Modify of this value is not allowed.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIspeechBundleEntry  11 }

isarIspeechBundleAdminStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),
                      down(2)
            }
    ACCESS  read-write
    STATUS  optional
    DESCRIPTION
           "The desired administrative status of the speech bundle.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIspeechBundleEntry  12 }

isarIspeechBundleOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),        -- ready to pass packets
                      down(2),
                      testing(3),   -- in some test mode
                      unknown(4),   -- status can not be determined
                                    -- for some reason.
                      dormant(5),
                      notPresent(6),    -- some component is missing
                      lowerLayerDown(7) -- down due to state of
                                        -- lower-layer interface(s)
            }
    ACCESS  read-only
    STATUS  optional
    DESCRIPTION
           "The operational status of the speech bundle.
            Only 'up' and 'down' will be used. (Admin state is immediately
            reflected in Operational state)"
    ::= { isarIspeechBundleEntry  13 }

isarIspeechBundleRowStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),
 
                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in
            this table.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIspeechBundleEntry  14 }

isarIspeechBundleConnectionIdentifier OBJECT-TYPE
    SYNTAX  INTEGER (0..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The identification of the connection is filled in when an 
            entry in a connection is created.
            A value different from zero indicates that the interface
            is in use by a higher layer interface or by a service (IWF).
            The value itself has no meaning for the operator.
            For IAD4700 intern the value 0-1024 indicates the number of
            connection.  A value > 1024 is one specific connection."
    ::= { isarIspeechBundleEntry  15 }

isarIspeechBundlePackingFactor OBJECT-TYPE
    SYNTAX  INTEGER (1..2)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Defines the number of samples to put into a single frame
            before sending it to the other side.
            Depending on isarIspeechBundleCoder this value can 
            be :
                 for g729 the value can be 1 to 2
                 for g723lo and g723hi it must be 1
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIspeechBundleEntry  16 }


