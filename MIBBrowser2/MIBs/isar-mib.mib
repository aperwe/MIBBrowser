    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the vcsIwf mapping table."
    INDEX   { isarIvcsIwfConnIndex }
    ::= { isarIvcsIwfMappingTable  1 }

IsarIvcsIwfMappingEntry ::=
    SEQUENCE {
      isarIvcsIwfMapVoiceBundle
        INTEGER
    }

isarIvcsIwfMapVoiceBundle OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Mapped index."
    ::= { isarIvcsIwfMappingEntry  1 }


-- statistics

