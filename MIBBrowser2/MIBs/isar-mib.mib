--
-- ISAR inbandIpMIBObjects : begin
--
-- enterprise MIB for ISAR
-- The MIB is written for SNMPv1.


-- ISAR inband IP protocol table
-- extension of the protocol handler table
-- table is optional


isarIinbandIpProtocolHandlerTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIinbandIpProtocolHandlerEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains the inband IP protocol handlers present in
            the system.
            It must be possible to dynamically add and delete entries.
            Creation of an entry in this table results in the creation of
            an entry in the isarIprotocolHandlerTable (done by IAD4700).
            It is the isarIprotocolHandlerTable who determines the index. 
            
            An entry cannot be deleted when it is in use by (an) inband IP
            channel(s)"
    ::= { isarIinbandIpMIBObjects  1 }

isarIinbandIpProtocolHandlerEntry OBJECT-TYPE
    SYNTAX  IsarIinbandIpProtocolHandlerEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Description of the inband IP protocol handlers present in the 
            IAD4700."
    INDEX   { isarIinbandIpProtocolHandlerIndex }
    ::= { isarIinbandIpProtocolHandlerTable  1 }


IsarIinbandIpProtocolHandlerEntry ::=
    SEQUENCE {
      isarIinbandIpProtocolHandlerIndex
        INTEGER,

      isarIinbandIpProtocolHandlerMultiplexingMethod
        INTEGER,

      isarIinbandIpProtocolHandlerIpInterfaceId
        INTEGER,

      isarIinbandIpProtocolHandlerRowStatus
        INTEGER
    }

isarIinbandIpProtocolHandlerIndex OBJECT-TYPE
    SYNTAX  INTEGER (0..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Unique protocol handler identification.
            This is the same index as isarIprotocolHandlerIndex in
            isarIprotocolHandlerTable.  
            The manager has to GET the isarIprotocolHandlerNextIndex before
            creating a row in isarIinbandIpProtocolHandlerTable."
    ::= { isarIinbandIpProtocolHandlerEntry  1 }

isarIinbandIpProtocolHandlerMultiplexingMethod OBJECT-TYPE
    SYNTAX  INTEGER {
                      llcEncapsulation(1),
                      vcBasedMultiplexing(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Multiplexing method supported by the protocol handler.
            See RFC 1483 for a description of the methods for carrying
            connectionless network interconnect traffic, routed and
            bridged Protocol Data Units, over an ATM network.
            Modify is not allowed.
            The SNMPv2 MAX-ACCESS is read(-only)-create."
    ::= { isarIinbandIpProtocolHandlerEntry  2 }

isarIinbandIpProtocolHandlerIpInterfaceId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the IP channel that uses this protocol handler.
            A value 0 indicates: no IP channels.
            A value 1 indicates: several IP channels.
            Otherwise, the index is the same as 
            isarIinbandIpChannelIpInterfaceIndex in the 
            isarIipInterfaceTable.
            The value of this object is initialized by the agent after the
            associated entry in the isarIinbandIpChannelTable has been
            created."
    ::= { isarIinbandIpProtocolHandlerEntry  3 }

isarIinbandIpProtocolHandlerRowStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),

                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in
            this table.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIinbandIpProtocolHandlerEntry 4 }



-- inband IP channel table
-- table is optional


isarIinbandIpChannelTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIinbandIpChannelEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains the inband IP channels present in the
            system.
            An entry defines the relation between an inband IP protocol 
            handler and an IP interface.
            It must be possible to dynamically add and delete entries.
            An entry cannot be deleted when it is in use by an inband IP
            interworking function."
    ::= { isarIinbandIpMIBObjects  2 }

isarIinbandIpChannelEntry OBJECT-TYPE
    SYNTAX  IsarIinbandIpChannelEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIinbandIpChannelTable."
    INDEX   { isarIinbandIpChannelIpInterfaceIndex }
    ::= { isarIinbandIpChannelTable  1 }


IsarIinbandIpChannelEntry ::=
    SEQUENCE {
      isarIinbandIpChannelIpInterfaceIndex
        INTEGER,

      isarIinbandIpChannelProtocolHandlerIndex
        INTEGER,

      isarIinbandIpChannelRowStatus
        INTEGER,

      isarIinbandIpChannelConnectionIndication
        INTEGER
    }

isarIinbandIpChannelIpInterfaceIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the IP interface of this channel.
            The index is the same as isarIipInterfaceId in the 
            isarIipInterfaceTable."
    ::= { isarIinbandIpChannelEntry  1 }

isarIinbandIpChannelProtocolHandlerIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The index of the inband IP protocol handler of this channel.
            The index is the same as isarIinbandIpProtocolHandlerIndex in
            the isarIinbandIpProtocolHandlerTable.
            Modify is not allowed.
            The SNMPv2 MAX-ACCESS is read(-only)-create."
    ::= { isarIinbandIpChannelEntry  2 }

isarIinbandIpChannelRowStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),

                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in this table.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIinbandIpChannelEntry  3 }

isarIinbandIpChannelConnectionIndication OBJECT-TYPE
    SYNTAX  INTEGER {
                      inUse(1),
                      free(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object is used to indicate whether the IP channel is in
            use by an inband IP interworking function.
            The value of this object is initialized by the agent after the
            associated entry in the isarIinbandIpSignallingIwfTable has
            been created."
    ::= { isarIinbandIpChannelEntry  4 }



