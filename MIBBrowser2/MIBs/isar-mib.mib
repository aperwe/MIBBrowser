    DESCRIPTION
           "An entry in the CES IWF mapping table."
    INDEX   { isarIatmfCESConfIwfConnIndex }
    ::= { isarIatmfCESIwfMappingTable  1 }

IsarIatmfCESIwfMappingEntry ::=
    SEQUENCE {
      isarIatmfCESIwfMapCbrIndex
        INTEGER
    }

isarIatmfCESIwfMapCbrIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
