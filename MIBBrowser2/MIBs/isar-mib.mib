    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),

                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in
            this table.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIccNbLogPortEntry  2 }

isarIccNbLogPortLowerLayerIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex value of the E1 or S0 port on which the NbLogPort
            is defined.  The value is determined by the SNMP manager.
            The operator is not allowed to change this value.
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIccNbLogPortEntry  3 }

isarIccNbLogPortMap OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This is a bitmap that indicates which B-channels are used on
            the lower layer for the dynamic trunking service of Call and
            Connection Control.
            The field is defined as the sum of (2 to the power of 
            ChannelNumber) with ChannelNumber (0..31).
            It is not allowed to select the D-channel.  In case of QSIG and
            a E1 interface, the D-channel is always located at timeslot 16.
            In case of S0 (only 2 bits are allowed):
              B1 channel is the same as ChannelNumber 1 in case of E1
              B2 channel is the same as ChannelNumber 2 in case of E1
            
            REMARK : isarIccNbLogPortMap is not to be used by the 45020
            The agent will refuse a SET that contains a value for both
            isarIccNbLogPortMap and isarIccNbLogPortMapString.

            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIccNbLogPortEntry  4 }

isarIccNbLogPortMapString OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE (4))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This OCTET STRING is for 45020 use only ! (However, the agent
            cannot prevent other management stations to use this object.)
            It contains the same information as isarIccNbLogPortMap.
            
            - isarIccNbLogPortMapString will be converted by the agent into
              isarIccNbLogPortMap at time of SET (of the string).
            - isarIccNbLogPortMap will be converted by the agent into
              isarIccNbLogPortMapString at time of GET (of the string).
              
            The agent will refuse a SET that contains a value for both
            isarIccNbLogPortMap and isarIccNbLogPortMapString.
            
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIccNbLogPortEntry  5 }

isarIccNbLogPortAdminStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),
                      down(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The desired administrative status of the NbLogPort.
            
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIccNbLogPortEntry  6 }

isarIccNbLogPortShutdownMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      forced(1),
                      notForced(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The desired shutdown mode of the NbLogPort.
           
            forced: all active calls are released
            notForced: new calls are not accepted; existing calls stay
                       active till they are released according the normal
                       procedure
            
            The shutdown mode is linked with the admin status.
            If admin status = up: changing of this field has no effect.
            If admin status is changed from up to down: active calls will
                                     be released according to this field.
            If admin status = down: changing of this field from notForced
                                    to forced will cause the release of the
                                    active calls.  Changing from forced to
                                    notForced has no effect."
    DEFVAL  { notForced }
    ::= { isarIccNbLogPortEntry  7 }

isarIccNbLogPortOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),        -- ready to pass packets
                      down(2),
                      testing(3),   -- in some test mode
                      unknown(4),   -- status can not be determined
                                    -- for some reason.
                      dormant(5),
                      notPresent(6),    -- some component is missing
                      lowerLayerDown(7) -- down due to state of
                                        -- lower-layer interface(s)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the current operational status of the
            layer-3 protocol stack.  The status of the layer-3 protocol
            stack also reflects the status of the lower layers.
            The oper status = dormant as long as the peer-to-peer layer 3
            connection is still under construction.  If this construction
            fails, the oper status is set to lowerLayerDown."
    ::= { isarIccNbLogPortEntry  8 }

isarIccNbLogPortLocNodeIndex OBJECT-TYPE
    SYNTAX  INTEGER (2..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Index to an entry in the isarIccLocNodeTable.
            
