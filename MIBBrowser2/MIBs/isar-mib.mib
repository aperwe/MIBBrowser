-- The Frame Relay/ATM PVC Service Interworking Protocol Table

isarIfrAtmSiIwfProtocolTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIfrAtmSiIwfProtTblEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "A table in which each row represents an enabled mapping for
            an encapsulated protocol for a Frame Relay/ATM Service
            Interworking connection.
            A connection can occur several times is the table.
            A connection can only occur in this table when in the 
            corresponding entry in the isarIfrAtmSiIwfConnTable the 
            EncapsulationMappingMode is set to translationMode.
            Entries in this table are created when corresponding bits
            are set in isarIfrAtmSiIwfConnEncapsulationMappings
            in the isarIfrAtmSiIwfConnTable."
    ::= { isarIfrAtmIwfMIBObjects  2 }

isarIfrAtmSiIwfProtTblEntry OBJECT-TYPE
    SYNTAX  IsarIfrAtmSiIwfProtTblEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry for an encapsulated protocol."
    INDEX   { isarIfrAtmSiIwfProtTblAtmIfIndex,
              isarIfrAtmSiIwfProtTblAtmVpi,
              isarIfrAtmSiIwfProtTblAtmVci,
              isarIfrAtmSiIwfProtTblProtocol }
    ::= { isarIfrAtmSiIwfProtocolTable  1 }


IsarIfrAtmSiIwfProtTblEntry ::=
    SEQUENCE {
      isarIfrAtmSiIwfProtTblAtmIfIndex
        INTEGER,

      isarIfrAtmSiIwfProtTblAtmVpi
        INTEGER,

      isarIfrAtmSiIwfProtTblAtmVci
        INTEGER,

      isarIfrAtmSiIwfProtTblProtocol
        INTEGER,

      isarIfrAtmSiIwfProtTblAAL5InFrames
        Counter,

      isarIfrAtmSiIwfProtTblAAL5OutFrames
        Counter,

      isarIfrAtmSiIwfProtTblAAL5InOctets
        Counter,

      isarIfrAtmSiIwfProtTblAAL5OutOctets
        Counter,

      isarIfrAtmSiIwfProtTblAAL5OutDiscards
        Counter,

      isarIfrAtmSiIwfProtTblFrInFrames
        Counter,

      isarIfrAtmSiIwfProtTblFrOutFrames
        Counter,

      isarIfrAtmSiIwfProtTblFrInOctets
        Counter,

      isarIfrAtmSiIwfProtTblFrOutOctets
        Counter,

      isarIfrAtmSiIwfProtTblFrOutDiscards
      	Counter

    }

isarIfrAtmSiIwfProtTblAtmIfIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index in the ifTable that identifies the ATM
            port for this interworking connection."
