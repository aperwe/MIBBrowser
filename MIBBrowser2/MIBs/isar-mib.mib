-- implementation is mandatory

isarIdsx1IntervalTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIdsx1IntervalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The DS1 Interval Table contains various
            statistics collected by each DS1 Interface over
            the previous 24 hours of operation.  The past 24
            hours are broken into 96 completed 15 minute
            intervals.  Each row in this table represents one
            such interval (identified by isarIdsx1IntervalNumber)
            for one specific instance (identified by
            isarIdsx1IntervalIndex)."
    ::= { isarIds1MIBObjects  8 }

isarIdsx1IntervalEntry OBJECT-TYPE
    SYNTAX  IsarIdsx1IntervalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the DS1 Interval table."
    INDEX   { isarIdsx1IntervalIndex, isarIdsx1IntervalNumber }
    ::= { isarIdsx1IntervalTable  1 }

IsarIdsx1IntervalEntry ::=
    SEQUENCE {
      isarIdsx1IntervalIndex
        INTEGER,

      isarIdsx1IntervalNumber
        INTEGER,

      isarIdsx1IntervalESs
        Gauge,

      isarIdsx1IntervalSESs
        Gauge,

      isarIdsx1IntervalSEFSs
        Gauge,

      isarIdsx1IntervalUASs
        Gauge,

      isarIdsx1IntervalCSSs
        Gauge,

      isarIdsx1IntervalPCVs
        Gauge,

      isarIdsx1IntervalLESs
        Gauge,

      isarIdsx1IntervalBESs
        Gauge,

      isarIdsx1IntervalDMs
        Gauge,

      isarIdsx1IntervalLCVs
        Gauge,

      isarIdsx1IntervalValidData
        INTEGER,

      isarIdsx1IntervalTimeStamp
        TimeTicks
    }

isarIdsx1IntervalIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index value which uniquely identifies the DS1
            interface to which this entry is applicable.  The
            interface identified by a particular value of this
            index is the same interface as identified by the
            same value as an isarIdsx1LineIndex object instance."
    ::= { isarIdsx1IntervalEntry  1 }

isarIdsx1IntervalNumber OBJECT-TYPE
    SYNTAX  INTEGER (1..96)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "A number between 1 and 96, where 1 is the most
            recently completed 15 minute interval and 96 is
            the 15 minutes interval completed 23 hours and 45
            minutes prior to interval 1."
    ::= { isarIdsx1IntervalEntry  2 }

isarIdsx1IntervalESs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Errored Seconds."
    ::= { isarIdsx1IntervalEntry  3 }

isarIdsx1IntervalSESs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Severely Errored Seconds."
    ::= { isarIdsx1IntervalEntry  4 }

isarIdsx1IntervalSEFSs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Severely Errored Framing Seconds."
    ::= { isarIdsx1IntervalEntry  5 }

isarIdsx1IntervalUASs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Unavailable Seconds.  This object
            may decrease if the occurance of unavailable
            seconds occurs across an interval boundary."
    ::= { isarIdsx1IntervalEntry  6 }

isarIdsx1IntervalCSSs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Controlled Slip Seconds."
    ::= { isarIdsx1IntervalEntry  7 }

isarIdsx1IntervalPCVs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Path Coding Violations."
    ::= { isarIdsx1IntervalEntry  8 }

isarIdsx1IntervalLESs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Line Errored Seconds."
    ::= { isarIdsx1IntervalEntry  9 }

isarIdsx1IntervalBESs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Bursty Errored Seconds."
    ::= { isarIdsx1IntervalEntry  10 }

isarIdsx1IntervalDMs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Degraded Minutes."
    ::= { isarIdsx1IntervalEntry  11 }

isarIdsx1IntervalLCVs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Line Code Violations."
    ::= { isarIdsx1IntervalEntry  12 }

isarIdsx1IntervalValidData OBJECT-TYPE
    SYNTAX  INTEGER { true(1), false(2) }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if there is valid data
            for this interval."
    ::= { isarIdsx1IntervalEntry  13 }

isarIdsx1IntervalTimeStamp OBJECT-TYPE
    SYNTAX  TimeTicks
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the start of 
            this interval."
    ::= { isarIdsx1IntervalEntry  14 }




