-- severity trap threshold

isarIalarmTrapThreshold OBJECT-TYPE
    SYNTAX  INTEGER (0..3)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Indicates when an alarm results into a trap 
              0 = only critical alarms generate a trap
              1 = critical and major alarms generate a trap
              2 = critical, major and minor alarms generate a trap
              3 = all alarms generate a trap"
    ::= { isarIalarmLogMIBObjects  4 }

-- highest current severity

isarIcurrentAlarmHighestSeverity OBJECT-TYPE
    SYNTAX  INTEGER { 
                      critical(1), 
                      major(2),
                      minor(3),
                      diagnostic(4) ,
                      none(5)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Indicates the highest severity in the list of current alarms 
              1 = a critical alarm is present
              2 = a major alarm but no critical alarm is present
              3 = a minor alarm but no critical/major alarm is present
              4 = a diagnostic alarm but no critical/major/minor alarm 
                  is present
              5 = there is no current alarm
            This object is used by the 45020 to determine the color state
            of the system."
    ::= { isarIalarmLogMIBObjects  5 }



