-- CES Statistics Table

isarIatmfCESStatsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIatmfCESStatsEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The CES AAL1 statistical data table.
            
            IAD4700 : the counters are the counters of the port on which
                   the AAL1 channel is located.  The information
                   is not CES specific when more than one channel is 
                   located on the port."
    ::= { isarIatmfCESObjects  3 }

isarIatmfCESStatsEntry OBJECT-TYPE
    SYNTAX  IsarIatmfCESStatsEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the CES AAL1 Stats table.  There is one
            entry in this table corresponding to each entry in the
            atmfCESConfTable."
    INDEX   { isarIatmfCESCbrIndex }
-- AUGMENTS { atmfCESConfEntry } 
    ::= { isarIatmfCESStatsTable  1 }

IsarIatmfCESStatsEntry ::=
    SEQUENCE {
      isarIatmfCESReassCells
        Counter,

      isarIatmfCESHdrErrors
        Counter,

      isarIatmfCESPointerReframes
        Counter,

      isarIatmfCESPointerParityErrors
        Counter,

      isarIatmfCESAal1SeqErrors
        Counter,

      isarIatmfCESLostCells
        Counter,

      isarIatmfCESMisinsertedCells
        Counter,

      isarIatmfCESBufUnderflows
        Counter,

      isarIatmfCESBufOverflows
        Counter,

      isarIatmfCESCellLossStatus
        INTEGER
    }

isarIatmfCESReassCells OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This count gives the number of cells played out to the
            CES Service Interface. It excludes cells that were
            discarded for any reason, including cells that were not used
            due to being declared misinserted, or discarded while the
            reassembler was waiting to achieve synchronization."
    ::= { isarIatmfCESStatsEntry  1 }

isarIatmfCESHdrErrors OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The count of the number of AAL1 header errors detected,
            including those corrected.  Header errors include 
            correctable and uncorrectable CRC, plus bad parity."
    ::= { isarIatmfCESStatsEntry  2 }

isarIatmfCESPointerReframes OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This records the count of the number of events in which the
            AAL1 reassembler found that an SDT pointer is not where it 
            is expected, and the pointer must be reacquired. This count 
            is only meaningful for structured CES modes, as unstructured
            CES modes do not use pointers. For unstructured CES modes, 
            this count, if present, should indicate zero."
    ::= { isarIatmfCESStatsEntry  3 }

isarIatmfCESPointerParityErrors OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This records the count of the number of events in which the
            AAL1 reassembler detects a parity check failure at the point
            where a structured data pointer is expected. This count is 
            only meaningful for structured CES modes, as unstructured 
            CES modes do not use pointers. For unstructured CES modes, 
            this count, if present, should indicate zero."
    ::= { isarIatmfCESStatsEntry  4 }

isarIatmfCESAal1SeqErrors OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  optional
    DESCRIPTION
           "Number of times that the sequence number of an incoming AAL1
            Type 1 SAR-PDU causes a transition from the 'sync' state to
            the 'out of sequence' state, as defined by ITU-T I.363.1."
    ::= { isarIatmfCESStatsEntry  5 }

isarIatmfCESLostCells OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  optional
    DESCRIPTION
           "Number of lost cells, as detected by the AAL1 sequence 
            number processing, for example. This records the count of 
            the number of cells detected as lost in the network prior 
            to the destination CES IWF AAL1 layer processing."
    ::= { isarIatmfCESStatsEntry  6 }

isarIatmfCESMisinsertedCells OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  optional
    DESCRIPTION
           "Number of AAL1 sequence violations which the AAL Convergence
            sublayer interprets as a misinserted cell, as defined by
            ITU-T I.363.1."
    ::= { isarIatmfCESStatsEntry  7 }

isarIatmfCESBufUnderflows OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of buffer underflows. This records the count of the
            number of times the CES reassembly buffer underflows. In the
            case of a continuous underflow caused by a loss of ATM cell
            flow, a single buffer underflow should be counted. If the
            CES IWF is implemented with multiple buffers (such as a cell
            level buffer and a bit level buffer), then either buffer 
            underflow will cause this count to be incremented."
    ::= { isarIatmfCESStatsEntry  8 }

isarIatmfCESBufOverflows OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of buffer overflows. This records the count of the
            number of times the CES reassembly buffer overflows. If the 
            CES IWF is implemented with multiple buffers (such as a cell
            level buffer and a bit level buffer), then either buffer 
            overflow will cause this count to be incremented."
    ::= { isarIatmfCESStatsEntry  9 }

isarIatmfCESCellLossStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      noLoss(1),
                      loss(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "When cells are continuously lost for the number of 
            milliseconds specified by 
            isarIatmfCESCellLossIntegrationPeriod, the value is set to 
            loss (2). When cells are no longer lost, the value is set 
            to noLoss (1)."
    ::= { isarIatmfCESStatsEntry  10 }




-- CES Active SVC Table

isarIatmfCESActiveSvcTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIatmfCESActiveSvcEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The table used to manage active SVCs established across ATM
            networks between CES entities."
    ::= { isarIatmfCESObjects  4 }

isarIatmfCESActiveSvcEntry OBJECT-TYPE
    SYNTAX  IsarIatmfCESActiveSvcEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the CES active SVC table.  There is one
            entry in the table corresponding to each active row in
            the isarIatmfCESConfTable for which the atmfCESConnType 
            takes the value 'activeSvc'."
    INDEX   { isarIatmfCESCbrIndex }
    ::= { isarIatmfCESActiveSvcTable  1 }

IsarIatmfCESActiveSvcEntry ::=
    SEQUENCE {
      isarIatmfCESRemoteAddr
        OCTET STRING,

      isarIatmfCESFirstRetryInterval
        INTEGER,

      isarIatmfCESRetryTimer
        INTEGER,

      isarIatmfCESRetryLimit
        INTEGER,

      isarIatmfCESRetryFailures
        Gauge,

      isarIatmfCESActiveSvcRestart
        INTEGER,

      isarIatmfCESActiveSvcOperStatus
        INTEGER,

      isarIatmfCESLastReleaseCause
        INTEGER,

      isarIatmfCESLastReleaseDiagnostics
        OCTET STRING
    }

isarIatmfCESRemoteAddr OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE(0|8|20))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The ATM address supporting the corresponding far end
            CES IWF process.  If no address is supplied, no attempts
            to establish the active SVC are initiated.
            The ATM address used by the network entity.  The address
            types are: no address (0 octets), E.164 (8 octets), and
            NSAP-encoded ATM Endsystem Address (20 octets).
            Note: The E.164 address is encoded in BCD format."
    ::= { isarIatmfCESActiveSvcEntry  1 }

isarIatmfCESFirstRetryInterval OBJECT-TYPE
    SYNTAX  INTEGER (1..3600)
    -- UNITS seconds
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The time (in seconds) to wait before attempting to establish
            the SVC after the first failed call attempt.  The time to 
            to wait between subsequent call attempts may differ to
            implement a backoff scheme.  Zero represents an infinite
            interval indicating no retries."
    DEFVAL  { 10 }
    ::= { isarIatmfCESActiveSvcEntry  2 }

isarIatmfCESRetryTimer OBJECT-TYPE
    SYNTAX  INTEGER (0..86400)
    -- UNITS seconds
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Indicates the current value (in seconds) of the retry timer 
            for this connection.  When the value reaches zero an 
            attempt will be made to establish the active SVC.  When the 
            timer is not running, the value zero shall be returned."
    ::= { isarIatmfCESActiveSvcEntry  3 }

isarIatmfCESRetryLimit OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Sets a maximum limit on how many consecutive unsuccessful
            call setup attempts can be made before stopping the attempt
            to set up the connection.  If this limit is reached then
            management action will be required (e.g. setting
            isarIatmfCESActiveSvcRestart to 'restart') to initiate a new
            attempt to establish the connection.  A value of zero
            indicates no limit - the attempts will continue until
            successful. If this object is not present, no limit on call
            attempts is assumed."
    DEFVAL  { 0 }
    ::= { isarIatmfCESActiveSvcEntry  4 }

isarIatmfCESRetryFailures OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Indicates how many attempts to establish the connection
            have failed.  This count is reset whenever a connection
            is successfully established or the active SVC is restarted."
    ::= { isarIatmfCESActiveSvcEntry  5 }

isarIatmfCESActiveSvcRestart OBJECT-TYPE
    SYNTAX  INTEGER {
                      restart(1),
                      noop(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "When the value is set to 'restart' the active SVC
            is released if necessary and a new setup procedure
            is begun.  As a result of this action, the
            isarIatmfCESActiveSvcOperStatus object transitions to
            'establishmentInProgress' (if not already in this state)
            and the isarIatmfCESRetryFailures object is cleared.

            When the value is set to 'noop' no operation is
            performed.  When read, the value 'noop' is returned."
    ::= { isarIatmfCESActiveSvcEntry  6 }

isarIatmfCESActiveSvcOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      other(1),
                      establishmentInProgress(2),
                      connected(3),
                      retriesExhausted(4),
                      noAddressSupplied(5),
                      lowerLayerDown(6)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Describes the status of the active SVC.  Valid values are:
               other              - none of the types specified below
               establishmentInProgress - connection is not operational,
                                         but call attempts are ongoing
               connected          - connection is currently operational
               retriesExhausted   - retry limit has been reached and 
                                    call attempts have ceased
               noAddressSupplied  - no remote address has been 
                                    configured, so no call attempts are
                                    initiated
               lowerLayerDown     - underlying CES IWF is not 
                                    operational

            When the row is not 'active', the value of this object is
            'other'."
    ::= { isarIatmfCESActiveSvcEntry  7 }

isarIatmfCESLastReleaseCause OBJECT-TYPE
    SYNTAX  INTEGER (1..127)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Value of the Cause field of the Cause information element
            in the last RELEASE signalling message received for this
            active SVC.  Indicates the reason for the release."
    ::= { isarIatmfCESActiveSvcEntry  8 }

isarIatmfCESLastReleaseDiagnostics OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE(0..8))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Value of the first 8 bytes of diagnostic information
            from the Cause field of the Cause information element
            in the last RELEASE signalling message received for this
            active SVC."
    ::= { isarIatmfCESActiveSvcEntry  9 }


-- CES Conf read-only table

isarIatmfCESConfRoTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIatmfCESConfRoEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The CES configuration table used to manage interworking
            between CBR interfaces or channels and ATM Virtual Channel
            Links (VCLs).  The reverse mapping is shown in the
            isarIatmfCESMappingRoTable.

            When a row in this table is created, an entry in the
            isarIatmfCESIwfMappingRoTable is made by IAD4700 (inverse 
            mapping of index).
            
            At the same time another table must be updated :
             - set field isarIatmVclRoCrossConnectIdentifier of the 
               corresponding entry in IsarIatmVclRoEntry with the
               value of isarIatmfCESRoConfIwfConnIndex and
             - set field isarIatmVclRoServiceType of the 
               corresponding entry in IsarIatmVclRoEntry to 'ces'.

            This table is a duplicate of the isarIatmfCESConfTable with all
            fields, except row status, set to read-only.  The operator
            is not allowed to modify entries in this table, only delete
            is permitted.
            Duplication is done for Call and connection control because
            this software creates its own CESs.
            Rows are created/deleted by IAD4700."
    ::= { isarIatmfCESObjects  6 }

isarIatmfCESConfRoEntry OBJECT-TYPE
    SYNTAX  IsarIatmfCESConfRoEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the CES configuration read-only table.  There is 
            one entry in the table per CES entity, mapping one CBR
            interface, channel, or bundle to an ATM VCL."
    INDEX   { isarIatmfCESRoCbrIndex }
    ::= { isarIatmfCESConfRoTable  1 }

IsarIatmfCESConfRoEntry ::=
    SEQUENCE {
      isarIatmfCESRoCbrIndex
        INTEGER,

      isarIatmfCESRoAtmIndex
        INTEGER,

      isarIatmfCESRoAtmVpi
        INTEGER,

      isarIatmfCESRoAtmVci
        INTEGER,

      isarIatmfCESRoCbrService
        INTEGER,

      isarIatmfCESRoCbrClockMode
        INTEGER,

      isarIatmfCESRoCas
        INTEGER,

      isarIatmfCESRoPartialFill
        INTEGER,

      isarIatmfCESRoBufMaxSize
        INTEGER,

      isarIatmfCESRoCdvRxT
        INTEGER,

      isarIatmfCESRoCellLossIntegrationPeriod
        INTEGER,

      isarIatmfCESRoConnType
        INTEGER,

      isarIatmfCESRoLocalAddr
        OCTET STRING,

      isarIatmfCESRoOperStatus
        INTEGER,

      isarIatmfCESRoConfRowStatus
        INTEGER,

      isarIatmfCESRoConfServiceIndex
        INTEGER,

      isarIatmfCESRoConfIwfConnIndex
        INTEGER,

      isarIatmfCESRoConfUserDefData
        INTEGER,

      isarIatmfCESRoConfUserDefSigCode1
        INTEGER,

      isarIatmfCESRoConfUserDefSigCode2
        INTEGER,

      isarIatmfCESRoConfTCEnabled
        INTEGER,

      isarIatmfCESRoConfSlotNumber
        INTEGER,

      isarIatmfCESRoConfPhysPortId
        INTEGER,

      isarIatmfCESRoConfDsx0BundleMap
        INTEGER
    }

isarIatmfCESRoCbrIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The value of this object is equal to MIB II's ifIndex value
            of the CBR interface, channel, or bundle that is being
            cross-connected to an ATM VCL."
    ::= { isarIatmfCESConfRoEntry  1 }

isarIatmfCESRoAtmIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object is equal to MIB II's
            ifIndex value of the ATM Port interface mapped 
            through this CES IWF to a CBR interface.

            The distinguished value zero indicates that no ATM
            interface has been specified.

            IAD4700 : The ifIndex of the ATM interface to which this
            interworking function belongs.  The value 0 is not allowed
            for IAD4700."
    ::= { isarIatmfCESConfRoEntry  2 }

isarIatmfCESRoAtmVpi OBJECT-TYPE
    SYNTAX  INTEGER (0..4095)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object is equal to the VPI used
            by the ATM VCL mapped through this CES IWF to a CBR
            interface.
            
            The value is not relevant if no ATM interface has been
            specified (i.e., isarIatmfCESAtmIndex is set to zero).
            
            IAD4700 : The VPI value of the VCL used for this interworking
            function."
    ::= { isarIatmfCESConfRoEntry  3 }

isarIatmfCESRoAtmVci OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object is equal to the VCI used
            by the ATM VCL mapped through this CES IWF to a CBR
            interface.

            The distinguished value zero indicates that no ATM
            VCL has been specified.
            
            IAD4700 : The VCI value of the CVL used for this interworking
            function."
    ::= { isarIatmfCESConfRoEntry  4 }

isarIatmfCESRoCbrService OBJECT-TYPE
    SYNTAX  INTEGER {
                      unstructured(1),
                      structured(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Define if DSx/Ex service is structured or unstructured. A
            structured(2) interface is some nx64kbKbps. An unstructured
            (1) interface is 1.544Mbps, 2.048Mbps, 6.312Mbps, 
            44.736 Mbps, or 34.368 Mbps. unstructured(1) passes all bits
            through the ATM network.  strucutured(2) passes data bits 
            through the ATM network, and may also pass signalling bits.
            
            At this time, only unstructured mode is defined for the
            44.736 Mbps and 34.368 Mbps services.
            
            IAD4700 v1 / v2.0 : only the value 'structured' is supported."
    DEFVAL { structured }
    ::= { isarIatmfCESConfRoEntry  5 }

isarIatmfCESRoCbrClockMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      synchronous(1),
                      srts(2),
                      adaptive(3)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Define if DSx/Ex service clocking mode. This maps into
            transmit clock source of CBR interface.

            For structured modes this value, if present, must be set to
            synchronous(1).
            IAD4700 v1 / v2.0 : only the value 'synchronous' is supported."
    DEFVAL  { synchronous }
    ::= { isarIatmfCESConfRoEntry  6 }

isarIatmfCESRoCas OBJECT-TYPE
    SYNTAX  INTEGER {
                      basic(1),
                      e1Cas(2),
                      ds1SfCas(3),
                      ds1EsfCas(4),
                      j2Cas(5)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This parameter selects which AAL1 Format should be used:
            Basic does not carry CAS bits, and uses a single 125 usec 
            frame.
            e1Cas, ds1SfCas, dS1EsfCas and j2Cas carry CAS bits in a
            multiframe structure for E1, DS1 SF, DS1 ESF and J2 
            respectively.
            This applies to structured interfaces only. Default is
            basic (1). For unstructured interfaces this value, if 
            present, must be set to the default of basic (1).

            IAD4700 v1 / v2.0 : only the values 'basic' and 'e1Cas' are
            allowed."
    DEFVAL  { basic }
    ::= { isarIatmfCESConfRoEntry  7 }

isarIatmfCESRoPartialFill OBJECT-TYPE
    SYNTAX  INTEGER (0 | 4 .. 47)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "If partial cell fill is used, the number of user octets per
            cell must be set with this parameter.  Setting this 
            parameter to zero disables partial cell fill, and causes all
            cells to be completely filled before they are sent."
    DEFVAL  { 0 }
    ::= { isarIatmfCESConfRoEntry  8 }

isarIatmfCESRoBufMaxSize OBJECT-TYPE
    SYNTAX  INTEGER  (1..65536)
    -- UNITS 10 usec
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Define maximum size in 10 microsecond increments of the
            reassembly buffer. Some implementations may want allow the
            maximum buffer size to be programmed to a size less than
            the physical limit to reduce the maximum delay through a 
            circuit."
    DEFVAL  { 128 }
    ::= { isarIatmfCESConfRoEntry  9 }

isarIatmfCESRoCdvRxT OBJECT-TYPE
    SYNTAX  INTEGER (1..65535)
    -- UNITS 10 usec
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum cell arrival jitter in 10 usec increments that 
            the reassembly process will tolerate in the cell stream
            without producing errors on the CBR service interface.
            IAD4700 : req. spec : CDV = 1 cell, so this depends on cell
            rate."
    DEFVAL  { 100 }
    ::= { isarIatmfCESConfRoEntry  10 }

isarIatmfCESRoCellLossIntegrationPeriod OBJECT-TYPE
    SYNTAX  INTEGER (1000 .. 65535)
    -- UNITS msec
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The time in milliseconds for the cell loss integration 
            period.  If cells are continuously lost for this period 
            of time, isarIatmfCESCellLossStatus is set to loss (2). 
            The default definition is 2500.
            IAD4700 : this value is constant."
    DEFVAL  { 2500 }
    ::= { isarIatmfCESConfRoEntry  11 }

isarIatmfCESRoConnType OBJECT-TYPE
    SYNTAX  INTEGER {
                      other(1),
                      pvc(2),
                      activeSvc(3),
                      passiveSvc(4)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of ATM connectivity between associated CES IWF's.
            Valid values are:
               other      - none of the types specified below
               pvc        - supporting connectivity is a permanent
                            virtual connection
               activeSvc  - attempt calls whenever none established
               passiveSvc - accept calls.

            IAD4700 v2.0 : only 'passiveSvc' is supported."
    DEFVAL { pvc }
    ::= { isarIatmfCESConfRoEntry  12 }

isarIatmfCESRoLocalAddr OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE(0|8|20))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ATM address of the local CES IWF process. This address
            may be used by the automatic SVC establishment procedures to
            identify the intended recipient CES IWF of an incoming 
            automatic SVC call request.
            The ATM address used by the network entity.  The address
            types are: no address (0 octets), E.164 (8 octets), and
            NSAP-encoded ATM Endsystem Address (20 octets).
            Note: The E.164 address is encoded in BCD format."
    ::= { isarIatmfCESConfRoEntry  13 }

isarIatmfCESRoOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),        -- ready to pass packets
                      down(2),
                      testing(3),   -- in some test mode
                      unknown(4),   -- status can not be determined
                                    -- for some reason.
                      dormant(5),
                      notPresent(6),    -- some component is missing
                      lowerLayerDown(7) -- down due to state of
                                        -- lower-layer interface(s)
            }
    ACCESS  read-only
    STATUS  optional
    DESCRIPTION
