-- THDLCS interworking function table
-- table is optional


isarIthdlcsIwfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIthdlcsIwfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains the THDLCS interworking functions
            present in the system.
            It must be possible to dynamically add and delete entries.
            Addition (or deletion) of an entry will automatically add
            (or delete) a corresponding entry in the 
            isarIthdlcsIwfStatsTable.

            When a row in this table is created, an entry in the
            isarIthdlcsIwfMappingTable is made by IAD4700 (inverse mapping
            of index).
            
            At the same time another table must be updated :
             - set field isarIatmVclCrossConnectIdentifier of the 
               corresponding entry in IsarIatmVclEntry with the
               value of isarIthdlcsIwfConnIndex and
             - set field isarIatmVclServiceType of the 
               corresponding entry in IsarIatmVclEntry to 'thdlcs'."
    ::= { isarIthdlcsMIBObjects  1 }

isarIthdlcsIwfEntry OBJECT-TYPE
    SYNTAX  IsarIthdlcsIwfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIthdlcsIwfTable."
    INDEX   { isarIthdlcsIwfServiceIndex, isarIthdlcsIwfHdlcIfIndex }
    ::= { isarIthdlcsIwfTable  1 }


IsarIthdlcsIwfEntry ::=
    SEQUENCE { 
      isarIthdlcsIwfServiceIndex
        INTEGER,
        
      isarIthdlcsIwfConnIndex
        INTEGER,
        
      isarIthdlcsIwfHdlcIfIndex
        INTEGER,

      isarIthdlcsIwfAtmIfIndex
        INTEGER,

      isarIthdlcsIwfAtmVpi
        INTEGER,

      isarIthdlcsIwfAtmVci
        INTEGER,

      isarIthdlcsIwfAdminStatus
        INTEGER,

      isarIthdlcsIwfOperStatus
        INTEGER,

      isarIthdlcsIwfRowStatus
        INTEGER

    }
    
isarIthdlcsIwfServiceIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the service to which this interworking function
            belongs.
            This is the same index as serviceIndex of isarIserviceTable."
    ::= { isarIthdlcsIwfEntry  1 }

isarIthdlcsIwfConnIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object is equal to the 
            isarIiwfNextConnIndex obtained (by IAD4700) to uniqely identify 
            this IWF."
    ::= { isarIthdlcsIwfEntry  2 }

isarIthdlcsIwfHdlcIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the logical HDLC-port of this interworking 
            function (index of isarIhdlcLogPortMappingTable).
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIthdlcsIwfEntry  3 }

isarIthdlcsIwfAtmIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the ATM interface of this interworking 
            function.
            (it must be possible to add rows but it must be 
            impossible to modify this index)
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIthdlcsIwfEntry  4 }

isarIthdlcsIwfAtmVpi OBJECT-TYPE
    SYNTAX  INTEGER (0..4095)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The VPI value of the VCL used for this interworking 
            function.
            (it must be possible to add rows but it must be 
            impossible to modify this index)
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIthdlcsIwfEntry  5 }

isarIthdlcsIwfAtmVci OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
