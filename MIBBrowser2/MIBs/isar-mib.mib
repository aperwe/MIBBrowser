isarIs0ConfigGroup OBJECT IDENTIFIER ::= { isarIs0MIBObjects  2 }

isarIs0ConfigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIs0ConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Supports configuration parameters of one physical interface.
            Rows are created at startup of IAD4700."
    ::= { isarIs0ConfigGroup  1 }

isarIs0ConfigEntry OBJECT-TYPE
    SYNTAX  IsarIs0ConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in isarIs0ConfigTable."
    INDEX   { ifIndex }
    ::= { isarIs0ConfigTable  1 }

IsarIs0ConfigEntry ::=
    SEQUENCE {
    isarIs0ConfigLoopMode
        INTEGER,

    isarIs0ConfigLineStatus
        INTEGER,

    isarIs0ConfigPowerSupplyStatus
        INTEGER,

    isarIs0ConfigLinePowering
        INTEGER,

    isarIs0ConfigServiceMode
        INTEGER
    }

isarIs0ConfigLoopMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      noLoop(1),
                      iomAnalogLoop(3)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable represents the desired loopback configuration of
            the S0 interface.  The values mean:
            noLoop : not in the loopback state.  A device that is not
                     capable of performing a loopback on the interface
                     shall allways return this as its value.
            iomAnalogLoop : The iomAnalogLoop will loop back the analog
                            output at the S/T interface to the analog input
                            at the S/T interface, so data is reflected 
                            towards the IOM-2 bus.  In other words, in LT-T
                            mode INFO3 is returned, and in LT-S mode INFO4
                            is returned to the analog input at the S/T
                            interface."
    DEFVAL { noLoop }
    ::= { isarIs0ConfigEntry  1 }

isarIs0ConfigLineStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      other(1),
                      f3PendingDeactivation(2),
                      f4PendingActivation(3),
                      f5Unsynchronized(4),
                      f6Synchronized(5),
                      f7Activated(6),
                      f8LostFraming(7),
                      g1Deactivated(8),
                      g2Synchronized(9),
                      g3Activated(10),
                      g4PendingDeactivated(11),
                      f0Deactivated(12)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Line status of this interface."
    ::= { isarIs0ConfigEntry  2 }

isarIs0ConfigPowerSupplyStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      disabled(1),
                      operational(2),
                      overloaded(3)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Indicates the current status of the power supply.
            disabled and overloaded: no line powering possible (problems 
                                     with the power supply)
            operational: line powering is possible."
    ::= { isarIs0ConfigEntry  3 }

isarIs0ConfigLinePowering OBJECT-TYPE
    SYNTAX  INTEGER {
                      enabled(1),
                      disabled(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Indicates if 'line powering' for this interface is to be
            enabled/disabled.
            A maximum of 16 S0 interfaces can be line powered in 
            the IAD4700.
            This configuration is only meaningful if the port is 
            configured in 'nt' mode."
    DEFVAL { disabled }
    ::= { isarIs0ConfigEntry  4 }

isarIs0ConfigServiceMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      normalMode(1),
                      transparentMode(2),
                      undefined(3)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "IAD4700 DPM:BRI can be applied in 2 different configurations:
            1. normal mode - as an emulated leased line interface
            2. transparent mode - remote S0 devices
            3. undefined - no service mode specified"
    DEFVAL { undefined }
    ::= { isarIs0ConfigEntry  5 }





