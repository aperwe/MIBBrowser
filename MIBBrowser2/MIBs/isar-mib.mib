-- isarIvcsMIBObjects       OBJECT IDENTIFIER ::= { isar  22 }


--
-- ISAR vcsMIBObjects : begin
--
-- enterprise MIB for ISAR
-- The MIB is written for SNMPv1.


-- VCS interworking function table


isarIvcsIwfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIvcsIwfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains the VCS interworking functions
            present in the system.
            It must be possible to dynamically add and delete entries.
            
            When a row in this table is created, an entry in the
            isarIvcsIwfMappingTable is made by IAD4700 (inverse mapping
            of index).
            
            At the same time another table must be updated :
             - set field isarIatmVclCrossConnectIdentifier of the 
               corresponding entry in IsarIatmVclEntry with the
               value of isarIvcsIwfConnIndex and
             - set field isarIatmVclServiceType of the 
               corresponding entry in IsarIatmVclEntry to 'vcs'."
    ::= { isarIvcsMIBObjects  1 }

isarIvcsIwfEntry OBJECT-TYPE
    SYNTAX  IsarIvcsIwfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIvcsIwfTable."
    INDEX   { isarIvcsIwfVoiceBundle }
    ::= { isarIvcsIwfTable  1 }


IsarIvcsIwfEntry ::=
    SEQUENCE { 
      isarIvcsIwfServiceIndex
        INTEGER,

      isarIvcsIwfConnIndex
        INTEGER,
                
      isarIvcsIwfVoiceBundle
        INTEGER,

      isarIvcsIwfVclIfIndex
        INTEGER,

      isarIvcsIwfVclVpi
        INTEGER,

      isarIvcsIwfVclVci
        INTEGER,

      isarIvcsIwfCodingAlgorithm
        INTEGER,

      isarIvcsIwfPackingFactor
        INTEGER,

      isarIvcsIwfSendFrHeader
        INTEGER,

      isarIvcsIwfPhysSlotId
        INTEGER,

      isarIvcsIwfPhysStackNo
        INTEGER,

      isarIvcsIwfAdminStatus
        INTEGER,

      isarIvcsIwfOperStatus
        INTEGER,

      isarIvcsIwfRowStatus
        INTEGER

    }
    
isarIvcsIwfServiceIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The index of the service to which this interworking function
            belongs.
            This is the same index as serviceIndex of isarIserviceTable.
            (it must be possible to add rows but it must be 
            impossible to modify this index)
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIvcsIwfEntry  1 }

isarIvcsIwfConnIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object is equal to the 
            isarIiwfNextConnIndex obtained (by IAD4700) to uniqely identify 
            this IWF."
    ::= { isarIvcsIwfEntry  2 }

isarIvcsIwfVoiceBundle OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index in the isarIspeechBundleTable of the bundle carrying
            the voice data to be compressed."
    ::= { isarIvcsIwfEntry  3 }

isarIvcsIwfVclIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Identification of the ATM port to be used.
            The combination of VclIfIndex, VclVpi and VclVpi identifies 
            the VCL on which to send/receive compressed voice data.
  
            (it must be possible to add rows but it must be 
            impossible to modify this index)
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIvcsIwfEntry  4 }

isarIvcsIwfVclVpi OBJECT-TYPE
    SYNTAX  INTEGER (0..4095)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The VPI value of the VCL used for this interworking 
            function.
            The combination of VclIfIndex, VclVpi and VclVpi identifies 
            the VCL on which to send/receive compressed voice data.          
            isarIvcsIwfVclVpi corresponds to isarIatmVclVpi of 
            isarIatmVclTable.
            (it must be possible to add rows but it must be 
            impossible to modify this index)
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIvcsIwfEntry  5 }

isarIvcsIwfVclVci OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The VCI value of the VCL used for this interworking 
            function.
            The combination of VclIfIndex, VclVpi and VclVpi identifies 
            the VCL on which to send/receive compressed voice data.
            isarIvcsIwfVclVci corresponds to isarIatmVclVci of 
            isarIatmVclTable
            (it must be possible to add rows but it must be 
            impossible to modify this index)
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIvcsIwfEntry  6 }

isarIvcsIwfCodingAlgorithm OBJECT-TYPE
    SYNTAX  INTEGER { 
                      g723Lo(1), 
                      g723Hi(2),
                      g729(3)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The compression method as defined in the SpeechBundle."
    ::= { isarIvcsIwfEntry  7 }

isarIvcsIwfPackingFactor OBJECT-TYPE
    SYNTAX  INTEGER (1..2)
    ACCESS  read-write
    STATUS  obsolete
    DESCRIPTION
           "Defines the number of samples to put into a single frame
            before sending it to the other side.
            The value is defined in the SpeechBundle."
    ::= { isarIvcsIwfEntry  8 }

isarIvcsIwfSendFrHeader OBJECT-TYPE
    SYNTAX  INTEGER {
                      noFrHeaderSend(1),
                      frHeaderSend(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Depending on the value a frame relay header will be composed
            and put in front of the compressed data.  The value is not
            used at the moment and will allways be noFrHeaderSend.
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    DEFVAL { noFrHeaderSend }
    ::= { isarIvcsIwfEntry  9 }

isarIvcsIwfPhysSlotId OBJECT-TYPE
    SYNTAX  INTEGER (1..3)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Slot Id of the Voice Compression Module (VCM) where the
            (de)compression will take place.
            
            When an Iwf is created on the E1/T1 port of a particular VCM,
            only the Slot Id of this VCM can be selected.
            
            When an Iwf is created on the E1/T1 port of the MAMO,
            any valid VCM Slot Id can be selected.
            
            Information about the number of available channels per VCM can
            be found in isarIspeechProcTable MIB.
            
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIvcsIwfEntry  10 }

isarIvcsIwfPhysStackNo OBJECT-TYPE
    SYNTAX  INTEGER (0..63)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Stack number of the Voice Compression Module (VCM) where the
            (de)compression will take place.
            The SNMPv2 MAX-ACCESS is read-(only-)create."
    ::= { isarIvcsIwfEntry  11 }

isarIvcsIwfAdminStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),
                      down(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The administrative state of the interworking function.
            The SNMPv2 MAX-ACCESS is read-create."
    DEFVAL { down }
    ::= { isarIvcsIwfEntry  12 }

isarIvcsIwfOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),        -- ready to pass packets
                      down(2),
                      testing(3),   -- in some test mode
                      unknown(4),   -- status can not be determined
                                    -- for some reason.
                      dormant(5),
                      notPresent(6),    -- some component is missing
                      lowerLayerDown(7) -- down due to state of
                                        -- lower-layer interface(s)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The operational state of the interworking function.
            Refer to RFC 2233 for the different states."
    ::= { isarIvcsIwfEntry  13 }


isarIvcsIwfRowStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),
 
                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in
            this table.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIvcsIwfEntry  14 }


-- vcs iwf mapping

isarIvcsIwfMappingTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIvcsIwfMappingEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The vcsIwf mapping table is used to map the IWF index
            into the index of the corresponding entry in the
            isarIvcsIwfTable."
    ::= { isarIvcsMIBObjects  2 }

isarIvcsIwfMappingEntry OBJECT-TYPE
