-- ISAR service table


isarIserviceTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIserviceEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains all services that are present in IAD4700.
            When a row in this table is created (by IAD4700), an entry in the
            isarIserviceMappingTable is made by IAD4700 (inverse mapping
            of index)."
    ::= { isarIserviceMIBObjects  1 }

isarIserviceEntry OBJECT-TYPE
    SYNTAX  IsarIserviceEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIserviceTable."
    INDEX   { isarIserviceIndex }
    ::= { isarIserviceTable  1 }


IsarIserviceEntry ::=
    SEQUENCE { 
      isarIserviceIndex
        INTEGER,
        
      isarIserviceName
        DisplayString,
        
      isarIserviceType
        INTEGER,
        
      isarIserviceStackNumber
        INTEGER,
        
      isarIserviceSlotNumber
        INTEGER,

      isarIserviceBufferNum
        INTEGER,

      isarIserviceBufferLength
        INTEGER,        

      isarIserviceAdminStatus
        INTEGER,

      isarIserviceOperStatus
        INTEGER

    }
    
isarIserviceIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the service."
    ::= { isarIserviceEntry  1 }

isarIserviceName OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (0..255))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The operator given name of the service."
    ::= { isarIserviceEntry  2 }

isarIserviceType OBJECT-TYPE
    SYNTAX  INTEGER {
                      ces(1),
                      frs(2),
                      thdlcs(3),
                      vcs(4),
                      lanis(5),
                      signalling(6)
                --    iwfIwfSignalling(7)   for CCC: will be used in VCLs
                --    broadbandSignalling(8)   for CCC: will be used in VCLs
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The name of the service."
    ::= { isarIserviceEntry  3 }

isarIserviceStackNumber OBJECT-TYPE
    SYNTAX  INTEGER (0..63)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of the IAD4700 in which the service physically
            resides (when IAD4700s are stacked)."
    ::= { isarIserviceEntry  4 }

isarIserviceSlotNumber OBJECT-TYPE
    SYNTAX  INTEGER (0..3)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of the slot in which the service physically
            resides.  The slot number corresponds to isarIphysSlotId
            of isarIslotTable."
    ::= { isarIserviceEntry  5 }

isarIserviceBufferNum OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of buffers for store and forward that are
            dedicated to this service. (will probably be defined
            in a .INI-file)"
    ::= { isarIserviceEntry  6 }

isarIserviceBufferLength OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The lentgh of the buffers."
    ::= { isarIserviceEntry  7 }

isarIserviceAdminStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),
                      down(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The administrative status of this service. It allows the
            operator to disable/enable the data flow for a service."
    ::= { isarIserviceEntry  8 }

isarIserviceOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      up(1),        -- ready to pass packets
                      down(2),
                      testing(3),   -- in some test mode
                      unknown(4),   -- status can not be determined
                                    -- for some reason.
                      dormant(5),
                      notPresent(6),    -- some component is missing
                      lowerLayerDown(7) -- down due to state of
                                        -- lower-layer interface(s)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The operational status of this service. It allows the
            operator to determine the availability of the module
            where this service task runs on.
            Refer to RFC 2233 for the different states."
    ::= { isarIserviceEntry  9 }



-- ISAR service mapping table


isarIserviceMappingTable OBJECT-TYPE
