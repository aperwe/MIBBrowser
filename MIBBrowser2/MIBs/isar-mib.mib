        INTEGER,

      isarIfrMgtVCSigUserT391
        INTEGER,

      isarIfrMgtVCSigNetN392
        INTEGER,

      isarIfrMgtVCSigNetN393
        INTEGER,

      isarIfrMgtVCSigNetT392
        INTEGER,

      isarIfrMgtVCSigNetnN4
        INTEGER,

      isarIfrMgtVCSigNetnT3
        INTEGER,

      isarIfrMgtVCSigUserLinkRelErrors
