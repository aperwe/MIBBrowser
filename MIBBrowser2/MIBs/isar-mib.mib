isarIsaveNow OBJECT-TYPE
    SYNTAX  INTEGER { 
                      true(1), 
                      false(2) 
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "When true(1), the IAD4700 will automatically regenerate the
            configuration file.  The 'configuration update log' will
            be 'recreated' (deleted and created)."
    ::= { isarIsystemMIBObjects  2 }


isarIperformanceStatistics OBJECT-TYPE
    SYNTAX  INTEGER { 
                      disabled(1),
                      enabled(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Indicates whether the collection of performance parameters
            is enabled or not and this for the complete IAD4700. This field
            can be used to align the intervals throughout the network.
            15 minutes after the enable is sent to the agent, the 
            statistics for the first (next) interval are collected. The
            previous intervals are discarded."
    DEFVAL { disabled }
    ::= { isarIsystemMIBObjects  3 }

isarIrestoreFactoryDefault OBJECT-TYPE
    SYNTAX  INTEGER { 
                      true(1),
                      false(2)
            }
    ACCESS  read-write
    STATUS  optional
    DESCRIPTION
           "The persistent store is cleared when this object is set to 
            'true'. It has to be set back to 'false' after clearing the 
            persistent store."
    DEFVAL { false }
    ::= { isarIsystemMIBObjects  7 }



