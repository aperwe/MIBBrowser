    ::= { isarIfrAtmNiIwfConnectionEntry  20 }

isarIfrAtmNiIwfConnSscsUserN391 OBJECT-TYPE
    SYNTAX  INTEGER (1..255)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The value of this object identifies the User-side
            N391 full status polling cycle value.
            This object applies to Q.933 Annex A and T1.617 Annex D."
    DEFVAL  { 1 }
    ::= { isarIfrAtmNiIwfConnectionEntry  21 }

isarIfrAtmNiIwfConnSscsUserT391 OBJECT-TYPE
    SYNTAX  INTEGER (5..180)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The value of this object identifies the User-side
            T391 link integrity verification polling timer value.
            This object applies to Q.933 Annex A and T1.617 Annex D."
    DEFVAL  { 180 }
    ::= { isarIfrAtmNiIwfConnectionEntry  22 }

isarIfrAtmNiIwfConnSscsNetT392 OBJECT-TYPE
    SYNTAX  INTEGER (5..200)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The value of this object identifies the Network-side
            T392 polling verification timer value.
            T392 should be greater than T391.
            This object applies to Q.933 Annex A, T1.617 Annex D and LMI."
    DEFVAL  { 200 }
    ::= { isarIfrAtmNiIwfConnectionEntry  23 }


