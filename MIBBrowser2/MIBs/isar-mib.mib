-- isarIpowerOnSTMIBObjects      OBJECT IDENTIFIER ::= { isar  25 }


--
-- ISAR powerOnSTMIBObjects : begin
--
-- enterprise MIB for ISAR
-- The MIB is written for SNMPv1.


isarItestLockedForRead OBJECT-TYPE
    SYNTAX  INTEGER { 
                      true(1), 
                      false(2) 
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Indicates if the test buffer is currently locked for 
            reading. If locked, other snmp managers are not able to read
            the data. 
            When the value is set from true to false, a check has to be
            made to verify that the same snmp manager is resetting the
            value. There are three possible ways this variable can be set
            to false.
            a. set to false by the same manager who set it previously to
               true,
            b. automatically when during sequential get, the test buffer
               is sent completely,
            c. via a time out in order not to block the test buffer
               indefinite."
    ::= { isarIpowerOnSTMIBObjects  1 }

isarItestResultLevel OBJECT-TYPE
    SYNTAX  INTEGER { 
                      allResults(1), 
                      onlyPassed(2),
                      onlyFailed(3)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Indicates which results are returned via GET."
    DEFVAL { onlyFailed }
    ::= { isarIpowerOnSTMIBObjects  2 }



-- ISAR power on self test : results


isarIpowerOnSTTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIpowerOnSTEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "."
    ::= { isarIpowerOnSTMIBObjects  3 }

isarIpowerOnSTEntry OBJECT-TYPE
    SYNTAX  IsarIpowerOnSTEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIpowerOnSTTable."
    INDEX   { isarIpowerOnSTIndex }
    ::= { isarIpowerOnSTTable  1 }


IsarIpowerOnSTEntry ::=
    SEQUENCE { 
      isarIpowerOnSTIndex
        INTEGER,
        
      isarIpowerOnSTMessage
        OCTET STRING
    }
    
isarIpowerOnSTIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Dummy index not used to access the table.
            The table will always be sequentially read."
    ::= { isarIpowerOnSTEntry  1 }

isarIpowerOnSTMessage OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "During a 'get', the snmp agent will verify whether the 
            calling manager is the same as the one who has set the
            isarItestLockedForRead variable. Each 'get' will return the 
            next test result message that is conform with the value of 
            isarItestResultLevel.
            The message contains the following information :
               incrementing number for ID of the test,
               name of the component,
               description of the test,
               the test level,
               the test type,
               the test group,
               the test result (passed or failed),
               fail data when the test failed.
            E.g. '1.PLD_MAMO. Register Access Test. Component. Basic. 
            Core. Failed - 0x10 (16), 0x20 (32), 0x30 (48).'
            '2. PLD_VCM. Register Access Test. Component. Basic. Core. 
            Passed.'"
    ::= { isarIpowerOnSTEntry  2 }





