isarIfrAtmSiIwfProtTblAtmVpi  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The VPI of the ATM PVC end point for this interworking
            connection."
    ::= { isarIfrAtmSiIwfProtTblEntry 2 }

isarIfrAtmSiIwfProtTblAtmVci  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The VCI of the ATM PVC end point for this interworking
            connection."
    ::= { isarIfrAtmSiIwfProtTblEntry 3 }

isarIfrAtmSiIwfProtTblProtocol  OBJECT-TYPE
    SYNTAX  INTEGER {
                      bridged8023Fcs(1),   --  PID: 0x00-01
                      bridged8023(2),      --  PID: 0x00-07
                      bridged8025Fcs(3),   --  PID: 0x00-03
                      bridged8025(4),      --  PID: 0x00-09
