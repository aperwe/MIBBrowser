    STATUS  mandatory
    DESCRIPTION
           "Each row describes one ifIndex --> isarIimaGroupIndex 
            mapping."
    INDEX   { ifIndex }
    ::= { isarIimaGroupMappingTable  1 }

IsarIimaGroupMappingEntry ::=
    SEQUENCE {
      isarIimaGroupMappingIndex
        INTEGER
    }

isarIimaGroupMappingIndex OBJECT-TYPE
