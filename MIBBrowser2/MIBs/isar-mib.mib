           "IEEE 802.1D-1990: Section 4.5.5.4"
    ::= { isarIdot1dStpPortEntry 6 }

isarIdot1dStpPortDesignatedCost OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
