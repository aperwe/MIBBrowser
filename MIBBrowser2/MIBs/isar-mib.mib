isarItftpLoadStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      initial(1),
                      loadInUse(2),
                      tftpDownloadStarted(5),
                      tftpDownloadSucceeded(6),
                      tftpDownloadFailed(7),
                      emptySlot(8),
                      unknownModule(9),
                      invalidModule(10),
                      notArmedModule(11),
                      passiveModule(12)
            }
    ACCESS  read-only
    STATUS  mandatory
