--
-- ISAR frnetservMIBObjects : begin
-- 
-- This MIB is based on the MIB described in IETF draft
-- draft-ietf-frnetmib-frs-mib-02.
-- The MIB is written for SNMPv1.
-- The textual conventions have been replaced by their base types
-- (because the 45020 does not know these textual conventions).

-- Replaced textual conventions

-- imported textual conventions

-- TimeStamp is replaced by TimeTicks


-- RowStatus : The RowStatus textual convention is used to manage the
--             creation and deletion of conceptual rows. See RFC1903.
-- is replaced by INTEGER {...}
-- originally used in : frPVCEndptRowStatus

-- next logical port index value

isarIfrLportNextIndex OBJECT-TYPE
    SYNTAX  INTEGER (0..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object contains an appropriate value to be used for a
            isarIfrLportIndex when creating entries in the
            isarIfrLportTable.
            The value 0 indicates that no unassigned entries are
            available. To obtain the isarIfrLportIndex  value for
            a new entry, the manager issues a management  protocol
            retrieval operation to obtain the current value of this
            object. After creation of the row, the agent will
            modify the value to the next unassigned index."
    ::= { isarIfrnetservMIBObjects  1 }


-- The Frame Relay Service Logical Port Group

--  the Frame Relay Logical Port Group
--  This table is an interface specific addendum
--  to the generic ifTable from MIB-II.

isarIfrLportTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIfrLportEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Frame Relay Logical Port Information table.
            When an entry in this table is created it is necessary to
            create an entry in the ifTable.  When an entry of this table
            is deleted, it is necessary to delete the corresponding
            entry in the ifTable."
    ::= { isarIfrnetservMIBObjects  2 }

isarIfrLportEntry OBJECT-TYPE
    SYNTAX  IsarIfrLportEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the Frame Relay Logical Port
            Information table."
    INDEX   { isarIfrLportIndex }
    ::= { isarIfrLportTable  1 }

IsarIfrLportEntry ::=
    SEQUENCE {
      isarIfrLportIndex
        INTEGER,

      isarIfrLportIfIndex
        INTEGER,

      isarIfrLportLowerLayerIfIndex
        INTEGER,

      isarIfrLportNumPlan
        INTEGER,

      isarIfrLportContact
        DisplayString,

      isarIfrLportLocation
        DisplayString,

      isarIfrLportType
        INTEGER,

      isarIfrLportAddrDLCILen
        INTEGER,

      isarIfrLportVCSigProtocol
        INTEGER,

      isarIfrLportServiceIndex
        INTEGER,

      isarIfrLportMaxFrameSize
        INTEGER,

      isarIfrLportRowStatus
        INTEGER,

      isarIfrLportInterworkingType
        INTEGER

    }

isarIfrLportIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the frame relay logical port."
    ::= { isarIfrLportEntry  1 }

isarIfrLportIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the frame relay logical port.  This index is
            determined after creation of a row in the ifTable."
    ::= { isarIfrLportEntry  2 }

isarIfrLportLowerLayerIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the layer on which this frame relay logical 
            port is defined.
            Modify of this value is not allowed.
            The SNMPv2 MAX-ACCESS is read(-only)-create."
    ::= { isarIfrLportEntry  3 }

isarIfrLportNumPlan OBJECT-TYPE
    SYNTAX  INTEGER {
                      other(1),
                      e164(2),
                      x121(3),
                      none(4)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object identifies the network
            address numbering plan for this UNI/NNI logical
            port. The network address is the object
            ifPhysAddress. The value none implies that there
            is no ifPhysAddress.  The SNMP proxy-agent will
            return an octet string of zero length for
            ifPhysAddress. The value other means that an
            address has been assigned to this interface, but
            the numbering plan is not enumerated here.

            IAD4700 : will always be equal to 'none'."
    ::= { isarIfrLportEntry  4 }

isarIfrLportContact OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..255))
