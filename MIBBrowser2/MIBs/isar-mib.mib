--
-- ISAR hssiPortMIBObjects : begin
--  
-- enterprise MIB for configuration of HSSI (V.35/X.21) port
-- The MIB is written for SNMPv1.

-- REMARK : HSSI is named SI in management applications


-- High speed serial interface (V.35/X.21) port table


isarIhssiConfigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIhssiConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Configuration of HSSI (V.35/X.21) port.
            The rows in this table are created by IAD4700 after creation of
            an entry in the ifTable."
    ::= { isarIhssiPortMIBObjects  1 }

isarIhssiConfigEntry OBJECT-TYPE
    SYNTAX  IsarIhssiConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIhssiConfigTable."
    INDEX   { ifIndex }
    ::= { isarIhssiConfigTable  1 }


IsarIhssiConfigEntry ::=
    SEQUENCE { 
      isarIhssiSpeed
        INTEGER,
        
      isarIhssiClockMode
        INTEGER,

      isarIhssiPortMode
        INTEGER,

      isarIhssiPinConfMode
        INTEGER,

      isarIhssiCtrlLeads
        INTEGER,

      isarIhssiCurrentSpeed
        INTEGER
        
    }
    
isarIhssiSpeed OBJECT-TYPE
    SYNTAX  INTEGER (0..8000000)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Required Speed (in bits/sec) of HSSI (V.35/X.21) port. The maximum 
            value is 8 Mbits/sec.  Look at isarIhssiCurrentSpeed for the actual
            speed."
    ::= { isarIhssiConfigEntry  1 }

isarIhssiClockMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      dte(1),
                      dce(2),
                      normal(3)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Clock coming from DTE or DCE side.
            For isarIhssiPinConfMode=x21 only normal clock mode is
            supported."
    DEFVAL  { normal }
    ::= { isarIhssiConfigEntry  2 }

isarIhssiPortMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      dte(1),
                      dce(2)
            }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Port mode is always DCE."
    DEFVAL  { dce }
    ::= { isarIhssiConfigEntry  3 }

isarIhssiPinConfMode OBJECT-TYPE
    SYNTAX  INTEGER {
                      x21(1),
                      eia530A(2),
                      eia530(3),
                      v35(4),
                      v36(5)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Pin configuration mode."
    DEFVAL  { eia530A }
    ::= { isarIhssiConfigEntry  4 }

isarIhssiCtrlLeads OBJECT-TYPE
    SYNTAX  INTEGER {
                      enabled(1),
                      disabled(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Control leads enabled means that control signals taht are 
            relevant for the selected pin configuration mode are 
            supported
            For EIA-530A, EIA-530, V35,V36 these are: RTS(105), CTS(106),
            DSR(107), DTR(108), DCD(109).
            For X21 these are: RTS(105), DCD(109).
            When the DTE or cable does not support any of these controls,
            the control leads should be disabled, otherwise, an error is
            assumed (not app. for X21)."
    DEFVAL  { enabled }
    ::= { isarIhssiConfigEntry  5 }

isarIhssiCurrentSpeed OBJECT-TYPE
    SYNTAX  INTEGER (0..8000000)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Current Speed (in bits/sec) of HSSI (V.35/X.21) port. The maximum 
            value is 8 Mbits/sec.  Look at isarIhssiSpeed for the required
            speed."
    ::= { isarIhssiConfigEntry  6 }




