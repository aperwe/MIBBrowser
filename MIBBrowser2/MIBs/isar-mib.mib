--
-- ISAR oamMIBObjects : begin
--  
-- enterprise MIB for OAM
-- The MIB is written for SNMPv1.

isarIoamNodeId OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE (16))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Value used in the OAM loopback cells to indicate the source
            or the loopback location.
            The default value is 'all ones'."
    ::= { isarIoamMIBObjects  1 }



-- continuity check table
-- This table is optional

isarIoamContCheckTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIoamContCheckEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The continuity check table offers the operator the
            possibility to engage OAM Continuity Check on selected VPC
            (F4 flow) or VCC (F5 flow) connections or connection
            segments.  The operator can determine the flow
            (isarIoamContCheckVci equal to zero means F4 flow, every
            other value means F5 flow), the type (value different from
            none(1) activates the flow) and the direction of the flow.
            Loss of continuity will result in an alarm.
            INDEX : ifIndex is the index of the ATM layer."
    ::= { isarIoamMIBObjects  2 }

isarIoamContCheckEntry OBJECT-TYPE
    SYNTAX  IsarIoamContCheckEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIoamContCheckTable."
    INDEX   { ifIndex, isarIoamContCheckVpi, isarIoamContCheckVci }
    ::= { isarIoamContCheckTable  1 }


IsarIoamContCheckEntry ::=
    SEQUENCE { 
      isarIoamContCheckVpi
        INTEGER,
        
      isarIoamContCheckVci
        INTEGER,
        
      isarIoamContCheckType
        INTEGER,
        
      isarIoamContCheckDirection
        INTEGER,
        
      isarIoamContCheckActivProc
        INTEGER,
        
      isarIoamContCheckRowStatus
        INTEGER

    }


isarIoamContCheckVpi OBJECT-TYPE
    SYNTAX  INTEGER (0..4095)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Vpi has to be specified in case of VC or VP end-to-end
            or segment continuity check."
    ::= { isarIoamContCheckEntry  1 }

isarIoamContCheckVci OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object has the value zero in case of VP end-to-end or
            segment continuity check (not in IAD4700 v1.0/v1.1/v2.0).
            It is the Vci in case of VC end-to-end or segment
            continuity check."
    ::= { isarIoamContCheckEntry  2 }

isarIoamContCheckType OBJECT-TYPE
    SYNTAX  INTEGER {
                      none(1),
                      activeSegment(2),
                      activeEnd2End(3),
                      activeBoth(4)
                    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The type of the continuity check."
    ::= { isarIoamContCheckEntry  3 }

isarIoamContCheckDirection OBJECT-TYPE
    SYNTAX  INTEGER {
                      directionBA(1),
                      directionAB(2),
                      directionBoth(3)
                    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The B-A direction means that the endpoint B is commanded
            to activate the source-process of the continuity check
            function and the endpoint A is commanded to activate the
            sink-process of the continuity check function.
            The A-B direction means that the endpoint A is commanded
            to activate the source-process of the continuity check
            function and the endpoint B is commanded to activate the
            sink-process of the continuity check function."
    ::= { isarIoamContCheckEntry  4 }

isarIoamContCheckActivProc OBJECT-TYPE
    SYNTAX  INTEGER {
                      disabled(1),
                      enabled(2)
                    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Continuity check activation procedure.
            disabled : the operator has to configure both endpoints
            enabled : the operator has to configure this endpoint, the
                      other endpoint is configured via OAM activation
                      cells.  Traps are generated for success or 
                      failure."
    ::= { isarIoamContCheckEntry  5 }


isarIoamContCheckRowStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),

                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
                    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in the 
            isarIoamContCheckTable.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIoamContCheckEntry  6 }



-- loopback table
-- This table is optional

isarIoamLoopbackTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIoamLoopbackEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The loopback table offers the operator the possibility to
            engage OAM Loopback on selected VPC (F4 flow) or VCC (F5
            flow) connections or connection segments.  The operator
            can determine the flow (isarIoamLoopbackVci equal to zero
            means F4 flow, every other value means F5 flow), the type
            (value different from none(1) activate the flow) and
            optional the loopback location.  No loopback location 
            specified means the next connection end-point or segment
            end-point.
           
            INDEX : ifIndex is the index of the ATM layer."
    ::= { isarIoamMIBObjects  3 }

isarIoamLoopbackEntry OBJECT-TYPE
    SYNTAX  IsarIoamLoopbackEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIoamLoopbackTable."
    INDEX   { ifIndex, isarIoamLoopbackVpi, isarIoamLoopbackVci }
    ::= { isarIoamLoopbackTable  1 }


IsarIoamLoopbackEntry ::=
    SEQUENCE { 
      isarIoamLoopbackVpi
        INTEGER,
        
      isarIoamLoopbackVci
        INTEGER,
        
      isarIoamLoopbackType
        INTEGER,
        
      isarIoamLoopbackLocationId
        OCTET STRING,
        
      isarIoamLoopbackStatus
        INTEGER,

      isarIoamLoopbackRowStatus
        INTEGER
    }
    
isarIoamLoopbackVpi OBJECT-TYPE
    SYNTAX  INTEGER (0..4095)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Vpi has to be specified in case of VC or VP end-to-end
            or segment loopback."
    ::= { isarIoamLoopbackEntry  1 }

isarIoamLoopbackVci OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object has the value zero in case of VP end-to-end or
            segment loopback.  
            It is the Vci in case of VC end-to-end or segment loopback."
    ::= { isarIoamLoopbackEntry  2 }

isarIoamLoopbackType OBJECT-TYPE
    SYNTAX  INTEGER {
                      none(1),
                      segmentOam(2),
                      end2EndOam(3)
                    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The type of the loopback.
            Loopback is activated when type is changed into segmentOam
            or end2EndOam.
            Changes are not allowed when isarIoamLoopbackStatus = 
            initiated."
    ::= { isarIoamLoopbackEntry  3 }

isarIoamLoopbackLocationId OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE (16))
    ACCESS  read-write
    STATUS  optional
    DESCRIPTION
           "Identifies the connection point along the virtual
            connection or connection segment where the loopback is to
            occur.
            The default value is 'all ones'. (Not done via DEFVAL because
            EMANATE generates the wrong code)."
    ::= { isarIoamLoopbackEntry  4 }

isarIoamLoopbackStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      none(1),
                      initiated(2),
                      success(3),
                      failed(4)
            }
    ACCESS  read-only
    STATUS  optional
    DESCRIPTION
           "Status of loopback.
              none : row created and loopback not started
              initiated : loopback cell has been sent
              success : loopback cell received
              failed : loopback cell not received within x seconds."
    DEFVAL { none }
    ::= { isarIoamLoopbackEntry  5 }

isarIoamLoopbackRowStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),

                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
                    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in the 
            isarIoamLoopbackTable.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIoamLoopbackEntry  6 }



isarIoamTraps OBJECT IDENTIFIER     ::= { isarIoamMIBObjects 4 }
                                    
isarIoamCCActivationConfirmed TRAP-TYPE
    ENTERPRISE  isarIoamTraps
    VARIABLES   { ifIndex, 
                  isarIoamContCheckVpi, 
                  isarIoamContCheckVci 
                 }
    DESCRIPTION
           "isarIoamContCheckActivProc = enabled and continuity check
            is activated."
    ::= 1

isarIoamCCActivationDenied TRAP-TYPE
    ENTERPRISE  isarIoamTraps
    VARIABLES   { ifIndex, 
                  isarIoamContCheckVpi, 
                  isarIoamContCheckVci 
                 }
    DESCRIPTION
           "isarIoamContCheckActivProc = enabled and continuity check
            is denied."
    ::= 2

isarIoamLoopbackSuccess TRAP-TYPE
    ENTERPRISE  isarIoamTraps
    VARIABLES   { ifIndex,
                  isarIoamLoopbackVpi, 
                  isarIoamLoopbackVci 
                 }
    DESCRIPTION
           "isarIoamLoopbackStatus = success."
    ::= 3

isarIoamLoopbackFailed TRAP-TYPE
    ENTERPRISE  isarIoamTraps
    VARIABLES   { ifIndex,
                  isarIoamLoopbackVpi, 
                  isarIoamLoopbackVci 
                 }
    DESCRIPTION
           "isarIoamLoopbackStatus = failed."
    ::= 4



