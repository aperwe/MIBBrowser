--
-- ISAR hdlcLogPortMIBObjects : begin
--
-- enterprise MIB for ISAR
-- The MIB is written for SNMPv1.

-- next logical port index value

isarIhdlcLogPortNextIndex OBJECT-TYPE
    SYNTAX  INTEGER (0..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object is used to assist the manager in selecting
            a value for isarIhdlcLogPortIndex.  
            The manager has to GET this index before creating a row in 
            isarIhdlcLogPortTable."
    ::= { isarIhdlcLogPortMIBObjects  1 }


-- HDLC logical port table


isarIhdlcLogPortTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIhdlcLogPortEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "When an entry in this table is created it is necessary to 
            create an entry in the ifTable, in the ifIndex mapping
            table and in the isarIhdlcLogPortStatsTable.  When an entry of 
            this table is deleted, it is necessary to delete the corresponding
            entry in the ifTable, the mapping table and 
            isarIhdlcLogPortStatsTable.
            Once a row is created it is not allowed to modify it.  The 
            operator must delete and re-create the row.
            The operator is not allowed to delete HDLC logical ports with an 
            ifName that starts with Internal_."
    ::= { isarIhdlcLogPortMIBObjects  2 }

isarIhdlcLogPortEntry OBJECT-TYPE
    SYNTAX  IsarIhdlcLogPortEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One row in the isarIhdlcLogPortTable."
    INDEX   { isarIhdlcLogPortIndex }
    ::= { isarIhdlcLogPortTable  1 }


IsarIhdlcLogPortEntry ::=
    SEQUENCE { 
      isarIhdlcLogPortIndex
        INTEGER,

      isarIhdlcLogPortIfIndex
        INTEGER,

      isarIhdlcLogPortLowerLayerIfIndex
        INTEGER,

      isarIhdlcLogPortCrcType
        INTEGER,

      isarIhdlcLogPortNumberOfFlags
        INTEGER,

      isarIhdlcLogPortRowStatus
        INTEGER
    }
    


isarIhdlcLogPortIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the HDLC logical port."
    ::= { isarIhdlcLogPortEntry  1 }

isarIhdlcLogPortIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the HDLC logical port.  This index is 
            determined after creation of a row in the ifTable."
    ::= { isarIhdlcLogPortEntry  2 }

isarIhdlcLogPortLowerLayerIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the layer on which this HDLC logical port
            is defined.  This lower layer can be a ds0Bundle or a
            HSSI port.
            Modify of this value is not allowed.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIhdlcLogPortEntry  3 }

isarIhdlcLogPortCrcType OBJECT-TYPE
    SYNTAX  INTEGER {
                      crc16Bit(1),
                      crc32Bit(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The type of CRC that will be applied to frames through this
            port.
            Modify of value is not allowed.
            The SNMPv2 MAX-ACCESS is read-create."
    DEFVAL { crc16Bit }
    ::= { isarIhdlcLogPortEntry  4 }

isarIhdlcLogPortNumberOfFlags OBJECT-TYPE
    SYNTAX  INTEGER (1..7)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The number of flags used for synchronisation.
            Modify of value is not allowed.
            The SNMPv2 MAX-ACCESS is read-create."
    DEFVAL { 2 }
    ::= { isarIhdlcLogPortEntry  5 }

isarIhdlcLogPortRowStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      -- the following two values are states:
                      -- these values may be read or written
                      active(1),
                      notInService(2),

                      -- the following value is a state:
                      -- this value may be read, but not written
                      notReady(3),

                      -- the following three values are
                      -- actions: these values may be written,
                      --   but are never read
                      createAndGo(4),
                      createAndWait(5),
                      destroy(6)
                    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object is used to create and delete rows in the 
            isarIhdlcLogPortTable.
            The SNMPv2 MAX-ACCESS is read-create."
    ::= { isarIhdlcLogPortEntry  6 }



-- HDLC logical port ifIndex mapping table


isarIhdlcLogPortMappingTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IsarIhdlcLogPortMappingEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Mapping of ifIndex into isarIhdlcLogPortIndex."
    ::= { isarIhdlcLogPortMIBObjects  3 }

isarIhdlcLogPortMappingEntry OBJECT-TYPE
    SYNTAX  IsarIhdlcLogPortMappingEntry
