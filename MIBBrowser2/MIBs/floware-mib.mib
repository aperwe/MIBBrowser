-- ####################################################################
-- Basic Unit Interface Reset Control Table
-- ####################################################################

buIfResetControlTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BuIfResetControlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
	"The Basic Unit Interface Card Reset control table."
    ::= { buInterface 5 }

buIfResetControlEntry OBJECT-TYPE
    SYNTAX      BuIfResetControlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "Entry in the buIfResetControlTable."
    INDEX   { ifIndex }
    ::= { buIfResetControlTable 1 }

BuIfResetControlEntry ::= SEQUENCE {
    buIfResetControl     INTEGER
    }


buIfResetControl  OBJECT-TYPE
    SYNTAX     INTEGER {
		    running(1),
		    reset(2),
		    switchOver(3),
			halt(4)
	      }
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"Setting this object to	reset(2) causes the Interface Card to reinitialize
	configuration parameters in non-volatile memory to
	default values and to restart the application software.
	Setting this object to switchOver(3) causes the interface card
	to switch between the stand-by and active software.
	When the device is running normally,
	this variable has a value of running(1)."
    ::= { buIfResetControlEntry 1 }									







-- ####################################################################
-- Basic Unit Traps group
-- ####################################################################


buTrapDestTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BuTrapDestEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table contains IP addresses of the management stations
     to transmit traps to.It resides in the BS BU only."
    ::= { buTraps 3 }

buTrapDestEntry OBJECT-TYPE
    SYNTAX      BuTrapDestEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "Entry in the buTrapDestTable."
    INDEX   { buTrapDestIpAddress }
    ::= { buTrapDestTable 1 }

BuTrapDestEntry ::= SEQUENCE {
    buTrapDestIpAddress         IpAddress,
    buTrapDestState             INTEGER,
    buTrapDestAuthorizedMngr    INTEGER,
    buTrapDestCommunity         DisplayString,
    buTrapDestRowStatus         RowStatus
    }

buTrapDestIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "IP address of the Management Station to send trap
     Also an index to the Trap Destination Table."
    ::= { buTrapDestEntry 2 }

buTrapDestState OBJECT-TYPE
    SYNTAX      INTEGER {
                          disable(1),
                          enable(2)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "When set to enable (2) trap will be sent to the Management Station
     with corresponding IP address , otherwize it will be not "
    ::= { buTrapDestEntry 3 }

buTrapDestAuthorizedMngr OBJECT-TYPE
    SYNTAX      INTEGER {
                          disable(1),
                          enable(2)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "When set to enable (2) and trap is alarm trap ,the same value 
     of buAlarmType variable will be sent for the On and Off alarm,
     otherwize different values of buAlarmTypes will be sent "
    ::= { buTrapDestEntry 4 }

buTrapDestCommunity OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..10))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Community string for trap transmitting by BS-BU and it's TS_BUs"
    ::= { buTrapDestEntry 5 }

buTrapDestRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The status of the row in the Trap Dest Table."
    ::= { buTrapDestEntry 6 }

buTrapSource  OBJECT-TYPE
    SYNTAX    DisplayString (SIZE(3..12)) 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This string should be BS for BS and the TS<customer id> TS transmitting trap"
    ::= { buTraps 4 }

