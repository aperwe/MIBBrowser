-- ####################################################################
-- Basic Unit Interface Table
-- ####################################################################

buIfTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BuIfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table represents the telecomm interfaces types configured by user
     for the Base Station and for the Terminal Station registered to it"
    ::= { buInterface 1 }

buIfEntry OBJECT-TYPE
    SYNTAX      BuIfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "Entry in the buIfTable."
    INDEX   { buIfIndex }
    ::= { buIfTable 1 }

BuIfEntry ::= SEQUENCE {
    buIfIndex             INTEGER,
    buIfType              IANAifType,
	buIfLoopbackType      INTEGER,
    buIfMaxBandwidth 	  INTEGER
    }

buIfIndex OBJECT-TYPE
    SYNTAX      INTEGER (2..4)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The Identification of the BU telecomm interface"
    ::= { buIfEntry 1 }

buIfType OBJECT-TYPE
    SYNTAX      IANAifType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The type of the BU telecomm interface. ds1(18) should be used for
     the E1 , v35(45) for V35, x213(93) for X21 , other (1) for not configured"
    ::= { buIfEntry 2 }

buIfLoopbackType OBJECT-TYPE
    SYNTAX      INTEGER	{
                          none    (1),
                          remote  (2),
						  local   (3),
						  payload (4),
						  board   (5),  -- not supported, future use
						  matrix  (6),  -- not supported
						  other   (7)
                        }

    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The type of the loopback applied for the BU telecomm interface
     Note that board and matrix looback is NOT supported."
    ::= { buIfEntry 3 }

buIfMaxBandwidth OBJECT-TYPE
    SYNTAX      INTEGER	{
                          none       (1),
                          bw2Mbps    (2),
                          bw4Mbps    (3)
                        }

    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The maximum bandwidth supported by BS-BU telecomm interface"
    ::= { buIfEntry 4 }


