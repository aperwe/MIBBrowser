    SYNTAX      BuLeasedLineEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "Entry in the buLeasedLineTable."
    INDEX   { buLLnIndex }
    ::= { buLeasedLineTable 1 }

BuLeasedLineEntry ::= SEQUENCE {
    buLLnIndex                    INTEGER,
    buLLnBsIfIndex                INTEGER,
	buLLnBsFirstE1Channel	      INTEGER,
