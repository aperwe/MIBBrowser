    SYNTAX      BuSwDownloadLogEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "Entry in the buSwDownloadLogTable."
    INDEX   { buSwDownloadLogDevice }
    ::= { buSwDownloadLogTable 1 }

BuSwDownloadLogEntry ::= SEQUENCE {
   	buSwDownloadLogDevice       DeviceId,
	buSwDownloadLogFile			FileName,
	buSwDownloadLogTFTPServer	IpAddress,
	buSwDownloadLogAction		DownloadAction,
	buSwDownloadLogStartTime	TimeTicks,
	buSwDownloadLogEndTime		TimeTicks,
	buSwDownloadLogStatus		INTEGER
    }

buSwDownloadLogDevice OBJECT-TYPE
    SYNTAX      DeviceId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Specification of the device. It is 1 to 16 for the Terminal Station,
     65536 (0x00010000) for BS"
    ::= { buSwDownloadLogEntry 1 }


buSwDownloadLogFile  OBJECT-TYPE
    SYNTAX     FileName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	"The name of the file which was downloaded to the device" 
    ::= { buSwDownloadLogEntry 2 }


buSwDownloadLogTFTPServer  OBJECT-TYPE
    SYNTAX     IpAddress
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	"The IP address of the TFTP server which was used for the download to 
	the device" 
    ::= { buSwDownloadLogEntry 3 }

buSwDownloadLogAction   OBJECT-TYPE
    SYNTAX     DownloadAction
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	"The programm image : current or stand by which was updated"
    ::= { buSwDownloadLogEntry 4 }


buSwDownloadLogStartTime	OBJECT-TYPE
    SYNTAX     TimeTicks
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	"The time when the download process was started"
    ::= { buSwDownloadLogEntry 5 }

buSwDownloadLogEndTime	OBJECT-TYPE
    SYNTAX     TimeTicks
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	"The time when the download process was finished"
    ::= { buSwDownloadLogEntry 6 }


buSwDownloadLogStatus  OBJECT-TYPE
    SYNTAX     INTEGER {
	        none(1),
		    downloadSuccess(2),
			downloadInProgress(3),
			downloadFileChecksumError(4),
			downloadFileAddressExceedsMaxValue(5),
			downloadFileAddressNotInOrder(6),
			downloadFileFormatError(7),
			downloadCongestion(8),
			downloadFlashCrcError(9),
			downloadAirLinkCorruption(10),
			downloadTsNotRegistered(11),
			downloadTsNotResponding(12),
			downloadTsDisconnected(13),
			downloadTftpNoResponseFromServer(14),
			downloadTftpFileNotFound(15),
			downloadTftpAccessViolation(16),
		    downloadStatusUnknown(17),
		    downloadGeneralError(18),
			downloadAborted(19),
			downloadFileCorrupted(20),
			downloadSearchingServer(21)
	       }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	"The status of the last download procedure, if any.
	This object will have a value of downloadStatusUnknown(2)
	if no download process has been performed."
    ::= {buSwDownloadLogEntry 7  }

buSwResetControl  OBJECT-TYPE
    SYNTAX     INTEGER {
		    running(1),
		    reset(2),
		    switchOver(3)
	      }
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"Setting this object to	reset(2) causes the BU to reinitialize
	configuration parameters in non-volatile memory to
	default values and restart the application software.
	Setting this object to switchOver(3) causes the BU
	to switch between the stand-by and active software.
	When the device is running normally,
	this variable has a value of running(1)."
    ::= { buSwConfig 3 }

buConfigLoad        OBJECT IDENTIFIER ::= { buSwConfig 4 }
-- always is destined to BS - community should be BS0 

buConfigLoadAction  OBJECT-TYPE
    SYNTAX     INTEGER {
	        none(1),
		    configLoadUpload(2),
			configLoadDownload(3),
			configLoadAbort(4),
			configLoadOverwrite(5)
	       }
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"The last configuration upload/download operation, if any.
	This object will have a value none(1) if no configuration
	upload/download process has been performed. If action is configLoadDownload
	RF calibration parameters are not overwritten during the download
	process. If action is configLoadOverwrite , all configuration parameters are
	overwritten "
    ::= {buConfigLoad 1  }

buConfigLoadStatus  OBJECT-TYPE
    SYNTAX     INTEGER {
	        none(1),
			configLoadInProgress(2),
		    configLoadSuccess(3),
			configLoadAborted(4),
			configLoadSoftwareVersionError(5),
			configLoadFileCRCError(6),
			configLoadFileFormatError(7),
		    configLoadGeneralError(8),
			configLoadFileCorrupted(9)
	       }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	"The status of the last configuration upload/download procedure, 
	if any.	This object will have a value none(1) if no configuration
	upload/download process has been performed."
    ::= {buConfigLoad 2  }

buConfigLoadTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BuConfigLoadEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
	"The Basic Unit Configuration Upload/Download table. The configuration data is
	accessed as consecutive blocks of 512 bytes. A table row corresponds to a block"
    ::= { buConfigLoad 3 }

