-- The Air Performance Monitoring Interval Table

buApmIntervalTs OBJECT IDENTIFIER ::= { buApmInterval 1  }


           buApmValidIntervalsTs OBJECT-TYPE
               SYNTAX INTEGER (0..96)
               MAX-ACCESS read-only
               STATUS current
               DESCRIPTION
                      "The number of  previous  intervals  for  which
                      valid data was collected by TS.  The value will be 96
                      unless the interface was brought on-line within
                      the last 24 hours, in which case the value will
                      be the number of complete 15  minute  intervals
                      the since interface has been online."
               ::= { buApmIntervalTs 1 }



          buApmIntervalTsTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF BuApmIntervalTsEntry
               MAX-ACCESS  not-accessible
               STATUS  current
               DESCRIPTION
                      "The Air Performance Monitoring Interval Table contains various
                      statistics collected by  TS Air Interface over
                      the previous 24 hours of operation.  The past 24
                      hours are broken into 96 completed 15 minute
                      intervals."
               ::= { buApmIntervalTs 2 }

          buApmIntervalTsEntry OBJECT-TYPE
               SYNTAX  BuApmIntervalTsEntry
               MAX-ACCESS  not-accessible
               STATUS  current
               DESCRIPTION
                      "An entry in the Ts Air Performance Monitoring Interval table."
               INDEX   { buApmIntervalTsNumber }
               ::= { buApmIntervalTsTable 1 }

          BuApmIntervalTsEntry ::=
               SEQUENCE {
                   buApmIntervalTsNumber            INTEGER,
                   buApmIntervalTsDlESs             Counter32,
                   buApmIntervalTsDlSESs            Counter32,
                   buApmIntervalTsDlUASs            Counter32,
                   buApmIntervalTsDlDMs             Counter32,
                   buApmIntervalTsDlSnrMin          INTEGER,
                   buApmIntervalTsDlSnrMax          INTEGER,
                   buApmIntervalTsDlSnrAvg          INTEGER,
                   buApmIntervalTsDlSigMin          INTEGER,
                   buApmIntervalTsDlSigMax          INTEGER,
                   buApmIntervalTsDlSigAvg          INTEGER,
				   buApmIntervalTsUlTxSigMin        INTEGER,
                   buApmIntervalTsUlTxSigMax        INTEGER,
                   buApmIntervalTsUlTxSigAvg        INTEGER,
                   buApmIntervalTsDlAfcMin          INTEGER,
                   buApmIntervalTsDlAfcMax          INTEGER,
                   buApmIntervalTsDlFer             Counter32
          }

          buApmIntervalTsNumber OBJECT-TYPE
               SYNTAX  INTEGER (1..96)
               MAX-ACCESS  read-only
               STATUS  current
               DESCRIPTION
                      "A number between 1 and 96, where 1 is the most
                       recently completed 15 minute interval and 96 is
                       the 15 minutes interval completed 23 hours and 45
                       minutes prior to interval 1."
               ::= { buApmIntervalTsEntry 1 }

          buApmIntervalTsDlESs OBJECT-TYPE
               SYNTAX  Counter32
               MAX-ACCESS  read-only
               STATUS  current
               DESCRIPTION
                      "The number of Errored Seconds encountered by the	TS
                       air interface in one of the previous 96,
                       individual 15 minute, intervals."
               ::= { buApmIntervalTsEntry 2 }

          buApmIntervalTsDlSESs OBJECT-TYPE
               SYNTAX  Counter32
               MAX-ACCESS  read-only
               STATUS  current
               DESCRIPTION
                      "The number of Severely Errored Seconds
                       encountered by the TS air interface in one of the
                       previous 96, individual 15 minute, intervals. "
               ::= { buApmIntervalTsEntry 3 }

          buApmIntervalTsDlUASs OBJECT-TYPE
               SYNTAX  Counter32
               MAX-ACCESS  read-only
               STATUS  current
               DESCRIPTION
                      "The number of Unavailable Seconds encountered by
                       the TS air interface in one of the previous 96,
                       individual 15 minute, intervals. In the case where
                       the agent is a proxy and valid data is not
                       available, return noSuchInstance.  This object may
                       decrease if the occurance of unavailable seconds
                       occurs across an inteval boundary."
               ::= { buApmIntervalTsEntry 4 }

          buApmIntervalTsDlDMs OBJECT-TYPE
               SYNTAX Counter32
               MAX-ACCESS read-only
               STATUS current
               DESCRIPTION
                      "The number of Degraded Minutes (DMs) encountered
                       by the TS air interface in one of the previous 96,
                       individual 15 minute, intervals."
               ::= { buApmIntervalTsEntry 5 }

