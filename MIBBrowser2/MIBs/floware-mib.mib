                      notExist(1),
                      mux8(2),
                      mux16(3)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Type of the mux "	
    ::= { muxIf 1 }

muxIfMuxOperStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                      up(1),
                      down(2),
                      testing(3)
                        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Mux operational status"
    ::= { muxIf 2 }

