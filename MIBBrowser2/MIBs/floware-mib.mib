buSwDownload        OBJECT IDENTIFIER ::= { buSwConfig 2 }
-- always is destined to BS - community should be BS0 

buSwDownloadDestDevice  OBJECT-TYPE
    SYNTAX     OCTET STRING (SIZE(3))
--     {
--		       baseStation(0),
--		       terminalStation1(1),
--		       terminalStation2(2),
--		       terminalStation3(3),
--		       terminalStation4(4),
--		       terminalStation5(5),
--		       terminalStation6(6),
--		       terminalStation7(7),
--		       terminalStation8(8),
--		       terminalStation9(9),
--		       terminalStation10(10),
--		       terminalStation11(11),
--		       terminalStation12(12),
--		       terminalStation13(13),
--		       terminalStation14(14),
--		       terminalStation15(15),
--		       terminalStation16(16)
--	           }
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	" 1 in the corresponding bit indicates the device to be downloaded from the TFTP server"
    ::= { buSwDownload 1 }

buSwDownloadFile  OBJECT-TYPE
    SYNTAX     FileName
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"The file name to be downloaded from the TFTP server
	when a download is next requested via this MIB." 
	DEFVAL     {""}
	::= { buSwDownload 2 }

buSwDownloadTFTPServer  OBJECT-TYPE
    SYNTAX     IpAddress
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"The IP address of the TFTP server that contains the
	boot image to load when a download is next requested
	via this MIB"
	DEFVAL     {0}
    ::= { buSwDownload 3 }

buSwDownloadAction  OBJECT-TYPE
    SYNTAX     DownloadAction
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"When this object is set to downloadToCurrent or
	downloadToStandBy, the BU will discontinue its
	normal operation and begin download of the image
	specified by buSwDownloadFile from the server
	specified by buSwDownloadTFTPServer using the TFTP
	protocol. If downloadToCurrent is specified the new
	image is written to the flash EPROM memory after
	its checksum has been verified to be correct. If
	downloadToStandBy is specified, the new image
	is downloaded to Stand-By area, while normal
	operation is still taking place. When the BU is not
	downloading, this object will have a value of
	none."
	DEFVAL	   {none}
    ::= { buSwDownload 4 }

buSwDownloadLogTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BuSwDownloadLogEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
	"The Basic Unit Download Log Table."
    ::= { buSwDownload 5 }

