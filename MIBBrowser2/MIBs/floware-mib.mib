-- ***##############################################################***
--              Frame Relay
-- ***##############################################################***

frPrvLportTable OBJECT-TYPE
   SYNTAX  SEQUENCE OF FrPrvLportEntry
   MAX-ACCESS  not-accessible
   STATUS  current
   DESCRIPTION
           "The Frame Relay Logical Port table.This table is an addition
            to the frLportTable defined in the rfc1604 "
   ::= { frameRelay 1 }
  
frPrvLportEntry OBJECT-TYPE
   SYNTAX  FrPrvLportEntry
   MAX-ACCESS  not-accessible
   STATUS  current
   DESCRIPTION
            "An entry in the Frame Relay Logical Port
             Information table."
   INDEX   { ifIndex }
   ::= { frPrvLportTable 1 }
  
FrPrvLportEntry ::=	SEQUENCE {
   frPrvLportRowStatus
            RowStatus
   }

frPrvLportRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The status of the current entry"
    ::= { frPrvLportEntry 1 }


frConcentrationRatio OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Concentration ratio defined in the system."
    ::= { frameRelay 2 }


frE1TsAssignTable OBJECT-TYPE
   SYNTAX  SEQUENCE OF FrE1TsAssignEntry
   MAX-ACCESS  not-accessible
   STATUS  current
   DESCRIPTION
   "The Frame Relay E1 Time Slots Assignment table.This table 
    contains the E1 Time Slots over which FR Logical Port is
    defined"
   ::= { frameRelay 3 }
  
frE1TsAssignEntry OBJECT-TYPE
   SYNTAX  FrE1TsAssignEntry
   MAX-ACCESS  not-accessible
   STATUS  current
   DESCRIPTION
   "An entry in the Framr Relay Time Slots Assignment table"
   INDEX   { ifIndex }
   ::= { frE1TsAssignTable 1 }
  
FrE1TsAssignEntry ::=	SEQUENCE {
   frTsAssignment  INTEGER
   }

frTsAssignment OBJECT-TYPE
    SYNTAX      INTEGER	(1..32)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The bitmap represnting the assignment of the E1 Time Slots.Each one
     of 32 bits represents 32 E1 time slots. Value 1 in the corresponding bit
     means that corresponding time slot is in use "
    ::= { frE1TsAssignEntry 1 }


-- ***##############################################################***
--              Testing
-- ***##############################################################***

berMeterTest     OBJECT IDENTIFIER ::= { testing 1 }

bmTestDestDevice  OBJECT-TYPE
    SYNTAX     INTEGER
--     {
--		       terminalStation1(0),
--		       terminalStation2(1),
--		       terminalStation3(2),
--		       terminalStation4(3),
--		       terminalStation5(4),
--		       terminalStation6(5),
--		       terminalStation7(6),
--		       terminalStation8(7),
--		       terminalStation9(8),
--		       terminalStation10(9),
--		       terminalStation11(10),
--		       terminalStation12(11),
--		       terminalStation13(12),
--		       terminalStation14(13),
--		       terminalStation15(14),
--		       terminalStation16(15)
--	           }
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	" 1 in the corresponding bit indicates the TS to be tested"
    ::= { berMeterTest 1 }

bmTestBerThreshold  OBJECT-TYPE
    SYNTAX     INTEGER
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"The BER threshold to transmit trap to the Management Station"
	DEFVAL     {0}
	::= { berMeterTest 2 }

bmTestUsedUpATS  OBJECT-TYPE
    SYNTAX     INTEGER
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"Number of Uplink Air Time Slots used in the test"
	DEFVAL     {0}
    ::= { berMeterTest 3 }


bmTestUsedDnATS  OBJECT-TYPE
    SYNTAX     INTEGER
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"Number of Downlink Air Time Slots used in the test"
	DEFVAL     {0}
    ::= { berMeterTest 4 }


bmTestDuration  OBJECT-TYPE
    SYNTAX     INTEGER
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"Duration of the BER test in minutes"
	DEFVAL	   {0}
    ::= { berMeterTest 5 }


bmTestIssueTrap  OBJECT-TYPE
    SYNTAX     INTEGER
	            {
				  disable(1),
				  enable(2)
				}
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"If this object is set to enable - trap is transmitted in case of BER threshold
	 is exceeded"
	DEFVAL	   {disable}
    ::= { berMeterTest 6 }

bmTestAction  OBJECT-TYPE
    SYNTAX     INTEGER
	            {
				  start(1),
				  stop(2),
				  none(3)
				}
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"Action to activate and to stop the test"
	DEFVAL	   {none}
    ::= { berMeterTest 7 }

bmTestType  OBJECT-TYPE
    SYNTAX     INTEGER
	            {
				  unicast(1),
				  multicast(2)
				}
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
	"During the Unicast test data is transmitted to TSs sequentially, otherwise
	 data is broadcasted to all TSs defined in the test "
	DEFVAL	   {unicast}
    ::= { berMeterTest 8 }


bmTestResultsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BmTestResultsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
	"BER Meter Test results."
    ::= { berMeterTest 9 }

bmTestResultsEntry OBJECT-TYPE
    SYNTAX      BmTestResultsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "Entry in the bmTestResultsTable."
    INDEX   { bmTestDeviceId }
    ::= { bmTestResultsTable 1 }

BmTestResultsEntry ::= SEQUENCE {
   	bmTestDeviceId       DeviceId,
	bmTestNumOfUpErrors  INTEGER,
	bmTestNumOfDnErrors  INTEGER,
	bmTestElapseTime     INTEGER,
	bmTestUsedATS        INTEGER,
	bmTestStatus	     INTEGER,
	bmTestNumOfCntOvfl	 INTEGER,
	bmTestNumOfStarv     INTEGER
    }

bmTestDeviceId OBJECT-TYPE
    SYNTAX      DeviceId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Specification of the device. It is 1 to 16 for the Terminal Station"
    ::= { bmTestResultsEntry 1 }

bmTestNumOfUpErrors OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number of errors in the Uplink occured since the beginning of the test"
    ::= { bmTestResultsEntry 2 }

bmTestNumOfDnErrors OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number of errors in the Downlink occured since the beginning of the test"
    ::= { bmTestResultsEntry 3 }

bmTestElapseTime OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Time in minutes elapced since the beginning of the test"
    ::= { bmTestResultsEntry 4 }

bmTestUsedATS OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number of ATS used in the test. Number	of transmitted bits is equal to 
     (Number of used ATS * 8 * Number of Frames * 40) "
    ::= { bmTestResultsEntry 5 }

bmTestStatus OBJECT-TYPE
    SYNTAX      INTEGER
	              {
				    active(1),	       
					complete(2),      
					idle(3),	       
				  	pend(4),	       
				  	stop(5),	       
				  	stopUser(6),	   
				  	stopLinkDown(7),	
				  	stopStartNack(8),	
				  	stopNoStartAck(9),
				  	stopOverflow(10),	
				  	stopTerminal(11),	
				  	stopReport(12)	   
				  }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Status of test procedure"
    ::= { bmTestResultsEntry 6 }

bmTestNumOfCntOvfl OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number of times the counter overflow was detected "
    ::= { bmTestResultsEntry 7 }

bmTestNumOfStarv OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number of times task starvation was detected "
    ::= { bmTestResultsEntry 8 }


