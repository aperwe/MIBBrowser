buSysLedStatus OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(8))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "LEDs status.
     Each byte represents particular LED:
     1   : external Alarm
     2   : internal Alarm
	 3   : ethernet ( only for BS_BU )
	 4-6 : telecomm leds
     7-8 : spare
     Within each byte, bits 0-3 are the color, 4-7 are 
     the blink rate.
     Supported color codes:
     0000 : Black (LED is off)
     0001 : Green
     0010 : Yellow
     0011 : Red
     Supported blink rates:
     0000 : no blink
     0001 : blink"
    ::= { buSystem 7 }

buSysStuffDelete OBJECT-TYPE
    SYNTAX      INTEGER {
                      disable(1),
                      enable(2)
			}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
	"Basic Unit operational mode, which depends on the
	clock accuracy type of the switch."
    DEFVAL { disable }
    ::= { buSystem 8 }

buSysLastRestart OBJECT-TYPE
    SYNTAX      INTEGER {
	                  powerOn(1),
                      unknown(2),
                      cpuOverload(3),
                      taskRequest(4),
                      userRequest(5)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The cause of last restart."
    ::= { buSystem 9 }


buSysAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                      disable(1),
                      enable(2)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Basic Unit Administration Status:
         Enable -     The TS BU is enabled
         Disable -    All telecom interfaces and functionality of the TS BU
                      are disabled.
                      The radio link to the BS is enabled thus the operator
                      can enable the TS using the EMS.
         In TS, disabling this causes total shutdown of radio channels without 
         possibility for remote recovery - so the manager does not set this
         variable (use buTsBuAdminStatus[] instead)."
    ::= { buSystem 10 }

buSysOperStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                      disconnected(1),
                      connected(2)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " BU's Operation Status. 
          For BS-BU : downlink channel status.
          For TS-BU : channel status "
    ::= { buSystem 11 }

 buSysFeaturesMask OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE(4))    
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " Features supported by system. Relevant for BS-BU only.
		  1 in corresponding bit means that feature is supported,
		  0 - feature is not supported. 
          The bits representation is as follows :
          bit0 (1) - v5 isdn
          bit1 (2) - not used
          bit2 (4) - not used
          bit3 (8) - not used
          bit4 (16) - not used
          bit5 (32) - not used
          bit6 (64) - not used
          bit7 (128) - not used
          bit8 (256) - not used
          bit9 (512) - not used
          bit10 (1024) - not used
          bit11 (2048) - not used
          bit12 (4096) - not used
          bit13 (8192) - not used
          bit14 (16384) - not used
          bit15 (32768) - not used" 
    ::= { buSystem 12 }

