SLMU-MIB DEFINITIONS ::= BEGIN

IMPORTS
MODULE-IDENTITY, OBJECT-TYPE, enterprises
        		FROM SNMPv2-SMI
    	TEXTUAL-CONVENTION, TimeInterval, TruthValue, DisplayString
        		FROM SNMPv2-TC
