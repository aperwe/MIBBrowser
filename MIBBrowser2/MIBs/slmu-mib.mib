        		FROM ADSL-LINE-MIB 
    	entPhysicalIndex
        		FROM ENTITY-MIB;

sni OBJECT IDENTIFIER ::= { enterprises 231 }

siemensUnits OBJECT IDENTIFIER ::= { sni 7 }

oenProductMibs OBJECT IDENTIFIER ::= { siemensUnits 1 }

sn OBJECT IDENTIFIER ::= { oenProductMibs 4 }

slmu MODULE-IDENTITY
    	LAST-UPDATED "9904220000Z"
    	ORGANIZATION "SIEMENS AG, ICN WN CS EP A14"
    	CONTACT-INFO
        		"   ICN WN CS EP A14
            		Hofmannstr. 27
            		81379 Munich, Germany

            		Claudio A. Lazzarotto
            		Tel:   +49 (0)89 722 47049
            		Fax:   +49 (0)89 722 62211
            		Email: ClaudioA.Lazzarotto@icn.siemens.de "
    	DESCRIPTION
        		"The MIB for the SLMU module.
The SLMU MIB shall complement the ADSL MIB, so that the most of parameters to be managed in the ITU-T recommendation G997.1 are covered.
This MIB provides objects  to enhance the port maintenance and performance monitoring."
