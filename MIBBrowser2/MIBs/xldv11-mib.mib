    -- info-VALUE:   trReplaceableUnitProblem (19)
    -- info-VALUE:   trReplaceableUnitNotInstalled (20)
    -- info-VALUE:   trReplaceableUnitSwMismatch(21)
    -- info-VALUE:   trHiBER(43)
    -- info-VALUE:   XLD V1.1U alarms :
    -- info-VALUE:   trLOMF(102) pon
    -- info-VALUE:   trFail(103) pon
    -- info-VALUE:   trLPS(104)  vdc, vdr
    -- info-VALUE:   trExternalAlarm(110),  external alarm
    -- info-VALUE:   trReplaceableUnitRemoved(113)
    -- info-VALUE:   trReplaceableUnitSwMissing(123)
    -- info-VALUE:   trInternalFault(124)
