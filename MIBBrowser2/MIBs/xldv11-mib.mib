 -- --------------------------------------------------------------
 --                    External Alarms Table (XLDV1.1U)
 -- --------------------------------------------------------------

 xldv11ExternalAlarmsTable OBJECT-TYPE
    -- info-NODE:    xldv11ExternalAlarmsTable
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     nac
    -- info-SYNTAX:  SEQUENCE OF Xldv11ExternalAlarmEntry
    -- info-COMMENT: Table of external alarm objects
    -- info-IMPL:    V1.1U V1.3A->
    SYNTAX        SEQUENCE OF Xldv11ExternalAlarmEntry
    ACCESS        not-accessible
    STATUS        mandatory
    DESCRIPTION
	 " The entities of the table are used to control the
	   forwarding of external alarms to the TMN-OS and LCT. "
    ::= { xldv11Alarming 1 }

 xldv11ExternalAlarmEntry OBJECT-TYPE
    -- info-NODE:    xldv11ExternalAlarmEntry 
    -- info-TREE:    xldv11Tlm
    -- info-INDEX:   xldv11ExtAlarmIndex
    -- info-ACC:     nac
    -- info-SYNTAX:  Xldv11ExternalAlarmEntry
    -- info-COMMENT: entries of external alarm table
    -- info-IMPL:    V1.1U V1.3A->
    SYNTAX        Xldv11ExternalAlarmEntry
    ACCESS        not-accessible
    STATUS        mandatory
    DESCRIPTION
	 " The attributes of an external alarm.
	   Notifications : xldv11TrExternalAlarm."
    INDEX         { xldv11ExtAlarmIndex }
    ::= { xldv11ExternalAlarmsTable 1 }

 Xldv11ExternalAlarmEntry ::=
      SEQUENCE { xldv11ExtAlarmIndex           INTEGER,
		 xldv11ExtAlarmAdminState      Xldv11AdminState,
		 xldv11ExtAlarmOperState       Xldv11OperState,
		 xldv11ExtAlarmActivityState   Xldv11ExtAlarmActivityState,
		 xldv11ExtAlarmSeverityIndex   Xldv11AlmSevProfileIndex,
		 xldv11ExtAlarmFilteringIndex  Xldv11AlmFiltProfileIndex,
		 xldv11ExtAlarmName            OCTET STRING }

  xldv11ExtAlarmIndex  OBJECT-TYPE
    -- info-LEAF:    xldv11ExtAlarmIndex
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     ro
    -- info-SYNTAX:  INTEGER
    -- info-VALUE:   1..8
    -- info-COMMENT: identifies an external alarm object
    -- info-ROU:     GET db
    -- info-IMPL:    V1.1U V1.3A->
    -- info-VERCTRL: V1.2A..V1.2B: GET db SET blocked
    SYNTAX        INTEGER (1..8)
    ACCESS        read-only
    STATUS        mandatory
    DESCRIPTION
	 " A unique number identifying an entry of the 
	   external alarm table. "
    --DEFAULT next-function-async     fsnExtEntryNext
    --DEFAULT get-function-async      fsnExtEntryGet
    ::= { xldv11ExternalAlarmEntry 1 }

 xldv11ExtAlarmAdminState OBJECT-TYPE
    -- info-LEAF:    xldv11ExtAlarmAdminState
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     rw
    -- info-SYNTAX:  Xldv11AdminState
    -- info-VALUE:   unlocked(1), locked(2)
    -- info-COMMENT: This attribute is used to activate (unlock) and
    -- info-COMMENT: deactivate (lock) the external alarm input line.
    -- info-ROU:     GET db  SET gf
    -- info-IMPL:    V1.1U V1.3A->
    -- info-VERCTRL: V1.2A..V1.2B: GET db SET blocked
    SYNTAX        Xldv11AdminState
    ACCESS        read-write
    STATUS        mandatory
    DESCRIPTION
	 " This attribute is used to activate (unlock) and deactivate
	   (lock) the forwarding of the external alarm towards the
	   manager. 
	   The range of values is: unlocked, locked. "
    --DEFAULT test-function-async     fsnExtEntryTest
    --DEFAULT next-function-async     fsnExtEntryNext
    --DEFAULT set-function-async      fsnExtEntrySet
    --DEFAULT get-function-async      fsnExtEntryGet
    ::= { xldv11ExternalAlarmEntry 2 }

 xldv11ExtAlarmOperState OBJECT-TYPE
    -- info-LEAF:    xldv11ExtAlarmOperState
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     ro
    -- info-SYNTAX:  Xldv11OperState
    -- info-VALUE:   enabled(1), disabled(2)
    -- info-COMMENT: Indicates whether or not the external alarm is
    -- info-COMMENT: capable of performing its normal functions.
    -- info-ROU:     GET db
    -- info-IMPL:    V1.1U V1.3A->
    -- info-VERCTRL: -
    SYNTAX        Xldv11OperState
    ACCESS        read-only
    STATUS        mandatory
    DESCRIPTION
	 " This attribute summarizes the extAlarmAdminState and the 
	   operational state of the CMU to indicate whether the external
	   alarm name is supervised at the moment. Only if the operational
	   state is enabled, larms will be generated for the objects.
	   The range of values is: enabled, disabled. "
    --DEFAULT next-function-async     fsnExtEntryNext
    --DEFAULT get-function-async      fsnExtEntryGet
    ::= { xldv11ExternalAlarmEntry 3 }

 xldv11ExtAlarmActivityState OBJECT-TYPE
    -- info-LEAF:    xldv11ExtAlarmActivityState
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     ro
    -- info-SYNTAX:  Xldv11ExtAlarmActivityState
    -- info-VALUE:   undeterminate(0), on(1), off(2)
    -- info-COMMENT: status of external alarm
    -- info-ROU:     GET db
    -- info-IMPL:    V1.1U V1.3A->
    -- info-VERCTRL: -
    SYNTAX        Xldv11ExtAlarmActivityState
    ACCESS        read-only
    STATUS        mandatory
    DESCRIPTION
	 " This attribute indicates the status of the external alarm
	   input line. Only if the operational state is enabled
	   and the alarm state is on, an external alarm is generated
	   for this object.
	   The range of values is: undeterminate(0), on(1), off(2). "
    --DEFAULT next-function-async     fsnExtEntryNext
    --DEFAULT get-function-async      fsnExtEntryGet
    ::= { xldv11ExternalAlarmEntry 4 }

 xldv11ExtAlarmSeverityIndex OBJECT-TYPE
    -- info-LEAF:    xldv11ExtAlarmSeverityIndex
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     rw
    -- info-SYNTAX:  Xldv11AlmSevProfileIndex
    -- info-VALUE:   1..40
    -- info-COMMENT: Pointer to the instance of the alarm severity
    -- info-COMMENT: profile that contains the severity assignments
    -- info-COMMENT: for the alarms reported by this external alarm object.
    -- info-ROU:     GET db  SET db
    -- info-IMPL:    V1.1U:ro->V1.3H:ro V1.3A->
    -- info-VERCTRL: GET db SET blocked
    SYNTAX        Xldv11AlmSevProfileIndex
    ACCESS        read-write
    STATUS        mandatory
    DESCRIPTION
	 " The index of the associated entry in the alarm severity
	   profile table. These entries contain the severity
	   assigments for the alarms reported by this managed
	   entity. Default is 1. "
    --DEFAULT test-function-async     fsnExtEntryTest
    --DEFAULT next-function-async     fsnExtEntryNext
    --DEFAULT set-function-async      fsnExtEntrySet
    --DEFAULT get-function-async      fsnExtEntryGet
    ::= { xldv11ExternalAlarmEntry 5 }

 xldv11ExtAlarmFilteringIndex OBJECT-TYPE
    -- info-LEAF:    xldv11ExtAlarmFilteringIndex
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     ro
    -- info-SYNTAX:  Xldv11AlmFiltProfileIndex
    -- info-VALUE:   1..40
    -- info-COMMENT: Pointer to the instance of the alarm filtering profile
    -- info-COMMENT: that contains the filtering assignments for the alarms
    -- info-COMMENT: reported by this external alarm object.
    -- info-ROU:     GET db
    -- info-IMPL:    V1.x
    -- info-VERCTRL: V1.1U:GET db
    SYNTAX        Xldv11AlmFiltProfileIndex
    ACCESS        read-only
    STATUS        mandatory
    DESCRIPTION
	 " It is a pointer to the instance of the alarm filtering
	   assignment profile managed entity that contains the filtering
	   assignments for the alarms reported by this managed entity.
	   (Not implemented) "
    --DEFAULT next-function-async     fsnExtEntryNext
    --DEFAULT get-function-async      fsnExtEntryGet
    ::= { xldv11ExternalAlarmEntry 6 }

    xldv11ExtAlarmName OBJECT-TYPE
    -- info-LEAF:    xldv11ExtAlarmName
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     rw
    -- info-SYNTAX:  OCTET STRING ( SIZE (0..50) ) 
    -- info-VALUE:   -
    -- info-COMMENT: text describing type of external alarm
    -- info-ROU:     GET db SET db
    -- info-IMPL:    V1.1U V1.3A->
    -- info-VERCTRL: -
    SYNTAX        OCTET STRING ( SIZE (0..50) )
    ACCESS        read-write
    STATUS        mandatory
    DESCRIPTION
	 " This text field is used to describe the input line
	   and the type of external alarm.
	   Default values on startup are :
	     XLDV1.1U :
	   index=1 : AND Battery
	   index=2 : OR Battery
	   index=3 : AND Power Supply
	   index=4 : OR Power Supply
	   index={n: 5<=n<=8} : External Alarm n.
	  XLDV1.1 :
	   index=1 : Fan Failure
	   index=2 : Fuse Failure
	   index=3 : High Temperature
	   index=4 : Door Open
	   index=5 : Main Connection Failure
	   index=6 : Power Supply Defect
	   index=7 : Battery Deep Discharge Failure
	   index=8 : External Alarm 8. "
    --DEFAULT test-function-async     fsnExtEntryTest
    --DEFAULT next-function-async     fsnExtEntryNext
    --DEFAULT set-function-async      fsnExtEntrySet
    --DEFAULT get-function-async      fsnExtEntryGet
    ::= { xldv11ExternalAlarmEntry 7 }

