 -- This managed entity manages the control request for storage
 -- of the database.

 xldv11DbmControlReq OBJECT-TYPE
    -- info-LEAF:    xldv11DbmControlReq
    -- info-TREE:    xldv11Dbm
    -- info-ACC:     rw
    -- info-SYNTAX:  Xldv11ControlReq
    -- info-VALUE:   noRequest, commExecStarted
    -- info-VALUE:   backupDb(command)
    -- info-VALUE:   localBackupOk, remoteBackupOk,
    -- info-VALUE:   localBackupFailed, remoteBackupFailed,
    -- info-VALUE:   flashAccessFailed, dbReadFailed 
    -- info-VALUE:   requestBusy,setDataFailed
    -- info-VALUE:   aswTimeout
    -- info-COMMENT: Database Management control request
    -- info-ROU:     GET db SET dm
    -- info-IMPL:    V1.3H->
    -- info-VERCTRL: V1.1U..V1.3B:GET db SET blocked
    SYNTAX        Xldv11ControlReq
    ACCESS        read-write
    STATUS        mandatory
    DESCRIPTION
	 " Marks the received command to store the database
	   (backupDb) into the FEPROM of the ONU.
	   The return value commExecStarted indicates the beginning
	   of DB storage.
	   Notifications : xldv11TrDbBackup indicates the end and 
	   the result of the database storage."
    --DEFAULT set-function-async      fsnDbmSet
    --DEFAULT next-function-async     fsnDbmNext
    --DEFAULT test-function-async     fsnDbmTest
    --DEFAULT get-function-async      fsnDbmGet
    ::= { xldv11DbmControl 1 }

 xldv11DbmControlStatus OBJECT-TYPE
    -- info-LEAF:    xldv11DbmControlStatus
    -- info-TREE:    xldv11Dbm
    -- info-ACC:     ro
    -- info-SYNTAX:  Xldv11ControlStatus
    -- info-VALUE:   requestIdle(1), requestBusy(2), requestPassed(3)
    -- info-VALUE:   requestFailed(4)
    -- info-COMMENT: Control status of command.
    -- info-ROU:     GET db
    -- info-IMPL:    V1.3H->
    -- info-VERCTRL: V1.1U->V1.3B: GET db
    SYNTAX        Xldv11ControlStatus
    ACCESS        read-only
    STATUS        mandatory
    DESCRIPTION
	 " The status of the control request. The value requestIdle
	   will be set after startup/recovery or after a running
	   test was aborted by the SNMP manager.
	   Range of values is:
	      requestIdle,
	      requestBusy,
	      requestPassed,
	      requestFailed . "
    --DEFAULT next-function-async     fsnDbmNext
    --DEFAULT get-function-async      fsnDbmGet
    ::= { xldv11DbmControl 2 }

 xldv11DbmControlTimer OBJECT-TYPE
    -- info-LEAF:    xldv11DbmControlTimer
    -- info-TREE:    xldv11Dbm
    -- info-ACC:     ro
    -- info-SYNTAX:  Integer32
    -- info-VALUE:   -2147483648..2147483647
