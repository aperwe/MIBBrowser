	 -- **************************************************************
	 --             TEXTUAL-CONVENTIONs for XLDV11-MIB
	 --
	 -- use of textual convention syntax necessary for the exported
	 -- types Xldv11ControlStatus, Xldv11AdminState and Unsigned16 in
	 -- order to create the file snmpinfo.dat. For load production the
	 -- textual convention definitions are hidden by comment signs. 
	 -- **************************************************************
	 --
	 -- --------------------------------------------------------------
	 --                            States
	 -- --------------------------------------------------------------

	 -- The control status of an entity.
	 --
	 -- Xldv11ControlStatus ::= TEXTUAL-CONVENTION
	 --      STATUS current
	 --      DESCRIPTION
	 --        "This data type is used as the syntax for the control status 
	 --         attributes."
	 --      SYNTAX INTEGER {
	 --        requestIdle(1),
	 --        requestBusy(2),
	 --        requestPassed(3),
	 --        requestFailed(4)
	 --      }
	 Xldv11ControlStatus ::=
	     INTEGER {
		 requestIdle(1),
		 requestBusy(2),
		 requestPassed(3),
		 requestFailed(4)
	     }

	 -- The operating state of an entity.
	 --
	 Xldv11OperState ::=
	     INTEGER {
		 enabled(1),
		 disabled(2)
	     }

	 -- Indicates whether a slot is empty or not.
	 --
	 Xldv11SlotStatus ::=
	     INTEGER {
		 unknown(0),
		 used(1),
		 empty(2)
	     }

	 -- The administrative state of an entity.
	 --
	 -- Xldv11AdminState ::= TEXTUAL-CONVENTION
	 --      STATUS current
	 --      DESCRIPTION
	 --        "This data type is used as the syntax for the admin state 
	 --         attributes."
	 --      SYNTAX INTEGER {
	 --        unlocked(1),
	 --        locked(2)
	 --      }
	 Xldv11AdminState ::=
	     INTEGER {
		 unlocked(1),
		 locked(2)
	     }

	 -- The availability status of a hardware unit
	 --
	 Xldv11AvailStatus ::=
	     INTEGER {
		     available(1),
		     failed(2),
		     degraded(3),
		     offline(4),
		     notInstalled(5),
		     dependency(7)
	-- only internal states
	--               noDependency(6),
	--               noRestart(8),
	--               warmRestart(9),
	--               coldRestart(10),
	--               reloadRestart(11),
	--               noUpgrade(12),
	--               upgrade(13)
	    }

	 -- The attribute reflects upon the internal states of the
	 -- Central Office Modem.
	 --
	 Xldv11LinkState ::=
	     INTEGER {
		 quiet(0),
		 idle(1),
		 train(2),
		 active(3),
		 download(4),
		 remoteDownload(6),
		 sleepMode(7),
		 unknown(255)
	     }

	 -- The log read status.
	 --
	 Xldv11DbuReadStatus ::=
	     INTEGER {
		 dbuReadComplete(1041),
		 dbuReadFailed(1042),
		 dbuFileCreationFailed(1043),
		 dbuFileTransmissionFailed(1044)
	     }
 
	 -- The access mode of a telnet session
	 --
	 Xldv11TelnetAccess ::=
	     INTEGER {
		 read(1),
		 trace(2),
		 write(3),
		 noAccess(4)
	     }

	 -- The status of a telnet session
	 --
	 Xldv11TvTelnetSessionStatus ::=
	     INTEGER {
		 running(1),
		 stopped(2)
	     }

	 -- The type of endpoint for continuity check
	 --
	 Xldv11EndpointType ::=
	     INTEGER { 
		 source  (1),
		 sink    (2),
		 both    (3)
		 }

	 -- The type of reset result
	 --
	 Xldv11RstResult ::=
	     INTEGER { 
		 requestFailed (4)
		 }

	 -- --------------------------------------------------------------
	 --                           Types
	 -- --------------------------------------------------------------

	 --
	 --
	 -- The textual convention like SNMPv2-TC. See rfc1903.
	 -- In the XldV1 system we terminate all strings with '\0'.
	 --
	 --
	 DisplayString ::= OCTET STRING (SIZE (0..255))


	 -- The textual convention for a 32 bit integer value.
	 --
	 Integer32 ::= INTEGER (-2147483648..2147483647)


	 -- The textual convention for a 32 bit integer index , 
	 -- positive, beginning with value 1.
	 -- Unsigned16 ::= TEXTUAL-CONVENTION
	 --    STATUS current
	 --    DESCRIPTION
	 --      "Textual convention for a 16 bit integer value."
	 --    SYNTAX INTEGER (0..65535)

	 Unsigned16::= INTEGER (0..65535)

	 -- The textual convention for a 32 bit integer index , 
	 -- positive, beginning with value 0, used for timer counts.
	 -- MR1202a

	 Unsigned32 ::= INTEGER (0..2147483647)


	 -- A general hardware unit type identifier.
	 --
	 Xldv11HwUnitType ::=
	     INTEGER {
		 equipment(1),
		 equipmentHolder(2),
		 plugInUnit(3)
	     }

	 -- The type of a TLM interface.
	 --
	 Xldv11IfType ::=
	     INTEGER {
