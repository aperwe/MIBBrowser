 xldv11AlarmSeverityProfileTable OBJECT-TYPE
    -- info-NODE:    xldv11AlarmSeverityProfileTable
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     nac
    -- info-SYNTAX:  SEQUENCE OF Xldv11AlarmSeverityProfileEntry
    -- info-COMMENT: table of alarm severity profiles
    -- info-IMPL:    V1.3A->
    -- info-VERCTRL: V1.1U..V1.2B:GET db SET blocked
    SYNTAX        SEQUENCE OF Xldv11AlarmSeverityProfileEntry
    ACCESS        not-accessible
    STATUS        mandatory
    DESCRIPTION
	 " The alarm severity profile table.
	   This table specifies which profile exists. 
	   XLDV1.1 : profiles can be created or deleted dynamically.
	   Creating or deleting an entry in this table automatically creates
	   or deletes the corresponding entries in the
	   xldv11AlarmSevTable. "
    ::= { xldv11Alarming 2 }

 xldv11AlarmSeverityProfileEntry OBJECT-TYPE
    -- info-NODE:    xldv11AlarmSeverityProfileEntry
    -- info-TREE:    xldv11Alarming
    -- info-INDEX:   xldv11AlmSevProfileIndex
    -- info-ACC:     nac
    -- info-SYNTAX:  Xldv11AlarmSeverityProfileEntry
    -- info-COMMENT: entry of alarm severity profiles table
    -- info-IMPL:    V1.3A->
    -- info-VERCTRL: V1.1U..V1.2B:GET db SET blocked
    SYNTAX        Xldv11AlarmSeverityProfileEntry
    ACCESS        not-accessible
    STATUS        mandatory
    DESCRIPTION
	 " A group of severities, one for each alarm type in
	   the communications alarm group.
	   Notifications : none."
    INDEX         { xldv11AlmSevProfileIndex }
    ::= { xldv11AlarmSeverityProfileTable 1 }

 Xldv11AlarmSeverityProfileEntry ::=
    SEQUENCE { xldv11AlmSevProfileIndex      Xldv11AlmSevProfileIndex,
	       xldv11AlmSevProfileRowStatus  Xldv11RowStatus }

 xldv11AlmSevProfileIndex OBJECT-TYPE
    -- info-LEAF:    xldv11AlmSevProfileIndex
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     ro
    -- info-SYNTAX:  Xldv11AlmSevProfileIndex
    -- info-VALUE:   1..40
    -- info-COMMENT: identifies this profile
    -- info-ROU:     GET db
    -- info-VALUE:   create(1), delete(2)
    -- info-VERCTRL: V1.1U->:GET db
    SYNTAX        Xldv11AlmSevProfileIndex
    ACCESS        read-only
    STATUS        mandatory
    DESCRIPTION
	 " A number identifying this alarm severity profile.
	   Default value: 1 . "
    --DEFAULT get-function-async      fsnAlmSevProfileEntryGet
    --DEFAULT next-function-async     fsnAlmSevProfileEntryNext
    ::= { xldv11AlarmSeverityProfileEntry 1 }

 xldv11AlmSevProfileRowStatus OBJECT-TYPE
    -- info-LEAF:    xldv11AlmSevProfileRowStatus
    -- info-TREE:    xldv11Alarming
    -- info-ACC:     rw
    -- info-SYNTAX:  Xldv11RowStatus
    -- info-VALUE:   create(1), delete(2)
    -- info-COMMENT: used to create or delete a profile
