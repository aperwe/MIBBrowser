VoSDSL-MIB DEFINITIONS ::= BEGIN

--  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, enterprises
        FROM SNMPv2-SMI
    entPhysicalIndex
        FROM ENTITY-MIB
    ifIndex
        FROM RFC1213-MIB  
    DisplayString,TruthValue,DateAndTime,RowStatus
        FROM SNMPv2-TC
    sysName
        FROM SNMPv2-MIB;

--  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

sni            OBJECT IDENTIFIER ::= { enterprises 231 }
siemensUnits   OBJECT IDENTIFIER ::= { sni 7 }
oenProductMibs OBJECT IDENTIFIER ::= { siemensUnits 1 }
sn             OBJECT IDENTIFIER ::= { oenProductMibs 4 }

--  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

vosdsl MODULE-IDENTITY
    LAST-UPDATED "0025111650Z"
    ORGANIZATION "SIEMENS AG, PSE EZE MSA4"
    CONTACT-INFO
        "   PSE EZE MSA 4
            Erdberger Laende 26
            A-1031 Vienna, Austria

            Josef Schoeberl
            Tel:   +43 (0)51707-37941
            Fax:   +43 (0)51707-57911
            Email: josef.schoeberl@siemens.at "
    DESCRIPTION
        "The MIB for Voice over Symmetrical Digital Subscriber Line
         (VoSDSL):
         - objects describing the SLMI:VoSDSL (module and ports)
         - objects describing the SLMI:VoSDSL interface (layer 1)"
    ::= { sn 13 }

--  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

vosdslMIBObjects     OBJECT IDENTIFIER ::= { vosdsl 1 }
vosdslMIBTraps       OBJECT IDENTIFIER ::= { vosdsl 2 }

--  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
--  MIB Objects consist of following groups
vosdslLc             OBJECT IDENTIFIER ::= { vosdslMIBObjects 1 }
vosdslLcPort         OBJECT IDENTIFIER ::= { vosdslMIBObjects 2 }
vosdslIfL1           OBJECT IDENTIFIER ::= { vosdslMIBObjects 3 }


--  vosdslLc group  - - - - - - - - - - - - - - - - - - - - - - - - - -
--  contains following subgroups
vosdslLcVersion           OBJECT IDENTIFIER ::= { vosdslLc 1 }
vosdslLcModule            OBJECT IDENTIFIER ::= { vosdslLc 2 }
vosdslLcNtuCompatibility  OBJECT IDENTIFIER ::= { vosdslLc 3 }


--  vosdslLcVersion subgroup

vosdslLcVersionTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslLcVersionEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "This table includes attributes describing the
         SLMI:VoSDSL linecard version."
    ::= { vosdslLcVersion 1 }

vosdslLcVersionEntry OBJECT-TYPE
    SYNTAX         VosdslLcVersionEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per physical entity of type module (compare
         ENTITY-MIB)."
    INDEX          { entPhysicalIndex }
    ::= { vosdslLcVersionTable 1 }

VosdslLcVersionEntry ::= SEQUENCE {
    vosdslLcVersionMibLevel                   Integer32
}

vosdslLcVersionMibLevel OBJECT-TYPE
    SYNTAX         Integer32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Identifies the implemented revision level of the MIB.
         The initial revision has the value 1. For each
         functional enhancement the MIB level is incremented
         by 1."
    ::= { vosdslLcVersionEntry 1 }


--  vosdslLcModule subgroup

vosdslLcModuleTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslLcModuleEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "This table includes attributes describing the
         SLMI:VoSDSL linecard module."
    ::= { vosdslLcModule 1 }

vosdslLcModuleEntry OBJECT-TYPE
    SYNTAX         VosdslLcModuleEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per physical entity of type module (compare
         ENTITY-MIB)."
    INDEX          { entPhysicalIndex }
    ::= { vosdslLcModuleTable 1 }

VosdslLcModuleEntry ::= SEQUENCE {
    vosdslLcModuleCoding                      INTEGER,
    vosdslLcModuleAttainableRate              Unsigned32,
    vosdslLcModuleType                        BITS
}

vosdslLcModuleCoding OBJECT-TYPE
    SYNTAX         INTEGER { other(1),
                             scPam(2)
                           }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Specifies the coding type used on the lines of the
         SLMI:VoSDSL.
         Other types may be added in the future.
         scPam - simply coded pulse amplitude modulation"
    ::= { vosdslLcModuleEntry 1 }

vosdslLcModuleAttainableRate OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "The value of this object multiplied with 64 kbit/s,
         is the maximum physical data rate theoretically
         attainable by the SLMI:VoSDSL linecard. This value
         may be higher than the actual physical data rate.
         It may also differ from the configured physical 
         data rate and from the physical data rate
         attainable by the vosdsl-NTU."
    ::= { vosdslLcModuleEntry 2 }

vosdslLcModuleType OBJECT-TYPE
    SYNTAX         BITS { noService(0),
                          isdnBa(1),
                          ab(2),
                          s2m(3),
                          ethernet(4),
                          other(5)
                        }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Defines the vosdsl-NTU interfaces that are supported
         by the SLMI:VoSDSL linecard.
         This is a bitmap of possible services. Bit
         'noService(0) may be set only if all other bits are
         not set. For SLMI:MSA noService will be set"
    ::= { vosdslLcModuleEntry 3 }


--  vosdslLcNtuCompatibility subgroup

vosdslLcNtuCompTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslLcNtuCompEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Table of compatible vosdsl-NTUs."
    ::= { vosdslLcNtuCompatibility 1 }

vosdslLcNtuCompEntry OBJECT-TYPE 
    SYNTAX         VosdslLcNtuCompEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One or more entries per physical entity of type
         module (compare ENTITY-MIB)."
    INDEX          { entPhysicalIndex, vosdslLcNtuCompIndex}
    ::= { vosdslLcNtuCompTable 1 }

VosdslLcNtuCompEntry ::= SEQUENCE {
    vosdslLcNtuCompIndex                       Integer32,
    vosdslLcNtuCompVendorId                    DisplayString,
    vosdslLcNtuCompHwVersion                   DisplayString,
    vosdslLcNtuCompSwVersion                   DisplayString,
    vosdslLcNtuCompRowStatus               RowStatus
}

vosdslLcNtuCompIndex OBJECT-TYPE
    SYNTAX         Integer32(1..20)
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Table index of compatible vosdsl-NTU."
::= { vosdslLcNtuCompEntry 1 }

vosdslLcNtuCompVendorId OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-create
    STATUS         current
    DESCRIPTION
        "Vendor name of compatible vosdsl-NTU."
::= { vosdslLcNtuCompEntry 2 }

vosdslLcNtuCompHwVersion OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-create
    STATUS         current
    DESCRIPTION
        "Hardware version of compatible vosdsl-NTU."
::= { vosdslLcNtuCompEntry 3 }

vosdslLcNtuCompSwVersion OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-create
    STATUS         current
    DESCRIPTION
        "Software version of compatible vosdsl-NTU."
::= { vosdslLcNtuCompEntry 4 }

vosdslLcNtuCompRowStatus OBJECT-TYPE
    SYNTAX       RowStatus
    MAX-ACCESS     read-create
    STATUS         current
    DESCRIPTION
        "The Status of the row."
::= { vosdslLcNtuCompEntry 5 }


--  vosdslLcPort group
--  contains following subgroups
vosdslLcPortConf          OBJECT IDENTIFIER ::= { vosdslLcPort 1 }
vosdslLcPortState         OBJECT IDENTIFIER ::= { vosdslLcPort 2 }
vosdslLcPortNtu           OBJECT IDENTIFIER ::= { vosdslLcPort 3 }
vosdslLcPortThresh        OBJECT IDENTIFIER ::= { vosdslLcPort 4 }

--  vosdslLcPortConf subgroup

vosdslLcPortConfTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslLcPortConfEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Table contains information about the port
         configuration data."
    ::= { vosdslLcPortConf 1 }

vosdslLcPortConfEntry OBJECT-TYPE
    SYNTAX         VosdslLcPortConfEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per SLMI:VoSDSL port."
    INDEX          { entPhysicalIndex }
    ::= { vosdslLcPortConfTable 1 }

VosdslLcPortConfEntry ::= SEQUENCE {
    vosdslLcPortConfNominalTxRate         Unsigned32,
    vosdslLcPortConfMinimumTxRate         Unsigned32,
    vosdslLcPortConfEmergencyTxRate       Unsigned32,
    vosdslLcPortConfTxPower               Unsigned32,
    vosdslLcPortConfNtuHwAddress          OCTET STRING (SIZE(6))
}

vosdslLcPortConfNominalTxRate OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "The value of this object multiplied with 64 kbit/s,
         is the configured nominal physical transmission
         rate on the line. The Default Value is 36 (2304 kBit/sec).
         This value is given by NetM.
         The value 127 (0x7F) indicates that the
         configuration value is not availabe."
    DEFVAL {36}
::= { vosdslLcPortConfEntry 1 }

vosdslLcPortConfMinimumTxRate OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "The value of this object multiplied with 64 kbit/s
         is the configured minimal physical transmission
         rate on the line.
         This value is given by NetM. The Default Value is 0.
         The value 127 (0x7F) indicates that the
         configuration value is not availabe."
    DEFVAL {0}
    ::= { vosdslLcPortConfEntry 2 }

vosdslLcPortConfEmergencyTxRate OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "The value of this object multiplied with 64 kbit/s
         is the configured emergency transmission rate on the line.
         This value is given by NetM. The Default Value is 0."
    DEFVAL {0}
    ::= { vosdslLcPortConfEntry 3 }

vosdslLcPortConfTxPower OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
"The value of this object is 
         0 .. maximum Transmission Power 
         1 .. reduced Transmission Power
         This value is given by NetM. The Default Value is 0."
    DEFVAL {0}
    ::= { vosdslLcPortConfEntry 4 }

vosdslLcPortConfNtuHwAddress OBJECT-TYPE
    SYNTAX         OCTET STRING (SIZE(6))
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
" The pseudo HW- Address (MAC- Adress) of the connected NTU.
  The NTU (IAD) needs this address for its BOOTP request."
    ::= { vosdslLcPortConfEntry 5 }

--  vosdslLcPortState subgroup

vosdslLcPortStateTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslLcPortStateEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Table contains SLMI:VoSDSL specific port state
         information."
    ::= { vosdslLcPortState 1 }

vosdslLcPortStateEntry OBJECT-TYPE
    SYNTAX         VosdslLcPortStateEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per SLMI:VoSDSL port."
    INDEX          { entPhysicalIndex }
    ::= { vosdslLcPortStateTable 1 }

VosdslLcPortStateEntry ::= SEQUENCE {
    vosdslLcPortStateL1                       INTEGER,
    vosdslLcPortStateCurrTxRate               Unsigned32,
    vosdslLcPortStateDetails                  BITS
}

vosdslLcPortStateL1 OBJECT-TYPE
    SYNTAX         INTEGER { unknown(1),
                             active(2),
                             inActive(3),
                             failure(4)
                           }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Information if vosdsl layer 1 is currently active."
    ::= { vosdslLcPortStateEntry 1 }

vosdslLcPortStateCurrTxRate OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "The value of this object is the actual physical
         transmission rate in kbit/s on the line. If the minimal 
         transmission rate (configured by NetM) can not be reached,
         the object reflects the highest possible transmission rate 
         taking into account the actual linecard, vosdsl-NTU and the 
         line. The value 0 indicates that the transmission rate is 
         currently not available (in case of activation process, 
         transmission interrupting errors,layer 1 affecting tests 
         or if the line is not available due to configuration activities.
         The actual physical data rate on the line may differ from 
         the physical data rate attainable by the actual linecard 
         and the vosdsl-NTU and from that configured by NetM"
::= { vosdslLcPortStateEntry 2 }

vosdslLcPortStateDetails OBJECT-TYPE
    SYNTAX         BITS { lossOfSync(0),
                          berPortfailure(1),
                          lineFeedingOverload(2),
                          noPortAccess(3),
                          l1ActivationTested(4),
                          l1ActivationFault(5),
                          slcTested(6),
                          slcFault(7),
                          vosdslNtuTested(8),
                          vosdslNtuFault(9),
                          vosdslNtuVersionTested(10),
                          vosdslNtuVersionFault(11)
                         }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Information about possible port error as detailed
         as it can be determined by the SLMI:VoSDSL:
         - The first six bits (lossOfSync, sesPortfailureberPortfailure,
           lineFeedingOverload, noPortAccess) are set if a
           corresponding error is detected.
         - The remaining bits are used in pairs: Always two
           bits belong together. The first bit shows if the
           test result shown by the second bit is valid. If
           the first bit is not set, this means, that the
           linecard can not make a statement if there is an
           error or not. If it is set, the statement of the
           second bit (errror if set, no error if not set)
           is ok."
    ::= { vosdslLcPortStateEntry 3 }


--  vosdslLcPortNtu subgroup

vosdslLcPortNtuTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslLcPortNtuEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Contains HW/FW/SW version information for vosdsl-NT."
    ::= { vosdslLcPortNtu 1 }

vosdslLcPortNtuEntry OBJECT-TYPE
    SYNTAX         VosdslLcPortNtuEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per SLMI:VoSDSL port."
    INDEX          { entPhysicalIndex }
    ::= { vosdslLcPortNtuTable 1 }

VosdslLcPortNtuEntry ::= SEQUENCE {
    vosdslLcPortNtuSerialNo                    DisplayString,
    vosdslLcPortNtuVendorId                    DisplayString,
    vosdslLcPortNtuHwVersion                   DisplayString,
    vosdslLcPortNtuFwVersion                   DisplayString,
    vosdslLcPortNtuSwVersion                   DisplayString,
    vosdslLcPortNtuCompatibility               BITS,
    vosdslLcPortNtuSDSLVersion                 DisplayString,
    vosdslLcPortNtuVendorModel                 DisplayString,
    vosdslLcPortNtuVersionCheckTrapEnable      INTEGER
}

vosdslLcPortNtuSerialNo OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "The serial number of the vosdsl-NTU. Empty if a serial
         number is not available."
    ::= { vosdslLcPortNtuEntry 1 }

vosdslLcPortNtuVendorId OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Name of vendor of the vosdsl-NTU."
    ::= { vosdslLcPortNtuEntry 2 }

vosdslLcPortNtuHwVersion OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Unambiguous indication of the vosdsl-NTU hardware
         version."
    ::= { vosdslLcPortNtuEntry 3 }

vosdslLcPortNtuFwVersion OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Unambiguous indication of the vosdsl-NTU Firmware
         version."
    ::= { vosdslLcPortNtuEntry 4 }

vosdslLcPortNtuSwVersion OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Unambiguous indication of the vosdsl-NTU software
         (downloadable) version."
    ::= { vosdslLcPortNtuEntry 5 }

vosdslLcPortNtuCompatibility OBJECT-TYPE
    SYNTAX         BITS { vendorCompatible(0),
                          hwCompatible(1),
                          fwCompatible (2),
                          swCompatible(3)
                        }
    MAX-ACCESS     accessible-for-notify
    STATUS         current
    DESCRIPTION
        "Provides information about the compatibility
         between SLMI:VoSDSL linecard and vosdsl-NTU:
         vendorCompatible - the vendor name of the current
                            vosdsl-NTU is available in the
                            vosdslLcNtuCompTable
         hwCompatible - only set in case of vendor
                        compatibility if the hardware
                        version of the current vosdsl-NTU is
                        available in the vosdslLcNtuCompTable
         fwCompatible - only set in case of vendor
                        compatibility if the firmware
                        version of the current vosdsl-NTU is
                        available in the vosdslLcNtuCompTable                       
         swCompatible - only set in case of vendor, hardware
                        and software compatibility if the 
                        software version of the current
                        vosdsl-NTU is available in the
                        vosdslLcNtuCompTable"
    ::= { vosdslLcPortNtuEntry 6 }

vosdslLcPortNtuSDSLVersion OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "The SDSL Version number of the vosdsl-NTU. Empty if the SDSL-Version
         number is not available."
    ::= { vosdslLcPortNtuEntry 7 }

vosdslLcPortNtuVendorModel OBJECT-TYPE
    SYNTAX         DisplayString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Vendor Model name of the vosdsl-NTU."
    ::= { vosdslLcPortNtuEntry 8 }

vosdslLcPortNtuVersionCheckTrapEnable OBJECT-TYPE
    SYNTAX         INTEGER { enabled(1),
                             disabled(2)
                           }
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "Indicatesn whether vosdslLcPortNtuVersionCheckTrap is
         generated."
    DEFVAL { disabled }
    ::= { vosdslLcPortNtuEntry 9 }

--  vosdslLcPortThresh subgroup

vosdslLcPortThreshTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslLcPortThreshEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "This table contains thresholds for performance
         traps."
    ::= { vosdslLcPortThresh 1 }

vosdslLcPortThreshEntry OBJECT-TYPE
    SYNTAX         VosdslLcPortThreshEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per SLMI:VoSDSL interface."
    INDEX          { ifIndex }
    ::= { vosdslLcPortThreshTable 1 }

VosdslLcPortThreshEntry ::= SEQUENCE {
    vosdslLcPortBERMeasureInterval            Unsigned32,
    vosdslLcPortThreshLowerBER                OCTET STRING (SIZE(1)),
    vosdslLcPortThreshUpperBER                OCTET STRING (SIZE(1)),
    vosdslLcPortThreshRestartBER              OCTET STRING (SIZE(1))
}

vosdslLcPortBERMeasureInterval OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "Measurement interval for Bit Error Rate evaluation
         in seconds."
    DEFVAL {1}
    ::= { vosdslLcPortThreshEntry 1 }

vosdslLcPortThreshLowerBER OBJECT-TYPE
    SYNTAX         OCTET STRING (SIZE(1))
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "BER- Threshold for a Good Line Trap.
        The value is expressed hexadecimal representation.
        The first nibble represents the mantisse of the value. 
        The second nibble represents the negative exponent of the power of ten value. 
        Examples are: 0x1A = Bit Error Rate of 1,0 EE-10, 
                      0x57 = Bit Error Rate of 5,0 EE-07.
        A value of 0x00 will disable the trap."
    ::= { vosdslLcPortThreshEntry 2 }

vosdslLcPortThreshUpperBER OBJECT-TYPE
    SYNTAX         OCTET STRING (SIZE(1))
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        " BER- Threshold for a Bad Line Trap.
        The value is expressed hexadecimal representation.
        The first nibble represents the mantisse of the value. 
        The second nibble represents the negative exponent of the power of ten value. 
        Examples are: 0x1A = Bit Error Rate of 1,0 EE-10, 
                      0x57 = Bit Error Rate of 5,0 EE-07.
        A value of 0x00 will disable the trap."
    ::= { vosdslLcPortThreshEntry 3 }

vosdslLcPortThreshRestartBER OBJECT-TYPE
    SYNTAX         OCTET STRING (SIZE(1))
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "BER- Threshold for Restarting the Line
        The value is expressed hexadecimal representation.
        The first nibble represents the mantisse of the value. 
        The second nibble represents the negative exponent of the power of ten value. 
        Examples are: 0x1A = Bit Error Rate of 1,0 EE-10, 
                      0x57 = Bit Error Rate of 5,0 EE-07.
        A value of 0x00 will disable the trap."
    ::= { vosdslLcPortThreshEntry 4 }

--  vosdslIfL1 group  - - - - - - - - - - - - - - - - - - - - - - - - -
--  contains following subgroups
vosdslIfL1PerfData          OBJECT IDENTIFIER ::= { vosdslIfL1 1 }
vosdslIfNtuL1PerfData       OBJECT IDENTIFIER ::= { vosdslIfL1 2 }

--  vosdslIfL1PerfData subgroup

vosdslIfL1PerfDataTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslIfL1PerfDataEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "This table includes performance measurement data
         describing the quality of the vosdsl line."
    ::= { vosdslIfL1PerfData 1 }

vosdslIfL1PerfDataEntry OBJECT-TYPE
    SYNTAX         VosdslIfL1PerfDataEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per SLMI:VoSDSL interface."
    INDEX          { ifIndex }
    ::= { vosdslIfL1PerfDataTable 1 }

VosdslIfL1PerfDataEntry ::= SEQUENCE {
    vosdslIfL1PerfValid15MinIntervals         Unsigned32,
    vosdslIfL1PerfCurr15MinTimeElapsed        Unsigned32,
    vosdslIfL1PerfCurr15MinCrcAnomalies       Unsigned32,
    vosdslIfL1PerfCurr15MinErrFreeSecs        Unsigned32,
    vosdslIfL1PerfCurr15MinErrSecs            Unsigned32,
    vosdslIfL1PerfCurr15MinSeverelyErrSecs    Unsigned32,
    vosdslIfL1PerfCurr15MinUnavailableSecs    Unsigned32,
    vosdslIfL1PerfCurr15MinTxRate             Unsigned32,
    vosdslIfL1PerfValid1DayIntervals          Unsigned32,
    vosdslIfL1PerfCurr1DayTimeElapsed         Unsigned32,
    vosdslIfL1PerfCurr1DayCrcAnomalies        Unsigned32,    
    vosdslIfL1PerfCurr1DayErrFreeSecs         Unsigned32,
    vosdslIfL1PerfCurr1DayErrSecs             Unsigned32,
    vosdslIfL1PerfCurr1DaySeverelyErrSecs     Unsigned32,
    vosdslIfL1PerfCurr1DayUnavailableSecs     Unsigned32,
    vosdslIfL1PerfCurr1DayTxRate              Unsigned32
}

vosdslIfL1PerfValid15MinIntervals OBJECT-TYPE
    SYNTAX         Unsigned32(0..32)
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Number of previous 15-minute intervals in the
         'vosdslIfL1PerfIntervalTable' for which valid data has
         been stored. This value will be equal to the maximum
         number of intervals that are kept (n) unless the
