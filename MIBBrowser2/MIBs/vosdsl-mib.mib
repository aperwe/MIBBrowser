    ::= { vosdslIfNtuL1PerfIntervalEntry 6 }

vosdslIfNtuL1PerfIntervalTxRate OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Physical transmission rate in kbit/s available in
         the interval. If the physical transmission rate 
         changed during the interval at least once, the value is 0."
    ::= { vosdslIfNtuL1PerfIntervalEntry 7 }

vosdslIfNtuL1PerfIntervalValidData OBJECT-TYPE
    SYNTAX         TruthValue
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This variable indicates if there are valid data for
         this interval."
    ::= { vosdslIfNtuL1PerfIntervalEntry 8 }

vosdslIfNtuL1PerfDayTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF VosdslIfNtuL1PerfDayEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "This table provides 1 day intervals of
         performance data measured at the subscriber line
         for the last 7 days."
    ::= { vosdslIfNtuL1PerfData 3 }

vosdslIfNtuL1PerfDayEntry OBJECT-TYPE
    SYNTAX         VosdslIfNtuL1PerfDayEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "One entry per interval and per each vosdsl line."
    INDEX          { ifIndex, vosdslIfNtuL1PerfDayNumber }
    ::= { vosdslIfNtuL1PerfDayTable 1 }

VosdslIfNtuL1PerfDayEntry ::= SEQUENCE {
    vosdslIfNtuL1PerfDayNumber              Integer32,
    vosdslIfNtuL1PerfDayCrcAnomalies        Unsigned32,
    vosdslIfNtuL1PerfDayErrFreeSecs         Unsigned32,
    vosdslIfNtuL1PerfDayErrSecs             Unsigned32,
    vosdslIfNtuL1PerfDaySeverelyErrSecs     Unsigned32,
    vosdslIfNtuL1PerfDayUnavailableSecs     Unsigned32,
    vosdslIfNtuL1PerfDayTxRate              Unsigned32,
    vosdslIfNtuL1PerfDayValidData           TruthValue
}

vosdslIfNtuL1PerfDayNumber OBJECT-TYPE
    SYNTAX         Integer32(1..7)
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Performance data interval number. 1 is the most
         recent interval; interval 7 is 1 week ago."
    ::= { vosdslIfNtuL1PerfDayEntry 1 }

vosdslIfNtuL1PerfDayCrcAnomalies OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Number of CRC Anomalies in the interval."
    ::= { vosdslIfNtuL1PerfDayEntry 2 }

vosdslIfNtuL1PerfDayErrFreeSecs OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Seconds in the interval with errorfree vosdsl layer 1."
    ::= { vosdslIfNtuL1PerfDayEntry 3 }

vosdslIfNtuL1PerfDayErrSecs OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Errored Seconds in the interval. The errored second
         parameter is a count of one-second intervals
         containing one or more CRC errors."
    ::= { vosdslIfNtuL1PerfDayEntry 4 }

vosdslIfNtuL1PerfDaySeverelyErrSecs OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Severely Errored Seconds in the interval. The
         severely errored seconds parameter is a count of
         one-second intervals containing >= 50 CRC errors
         or >= 1 LOSW defect detections."
    ::= { vosdslIfNtuL1PerfDayEntry 5 }

vosdslIfNtuL1PerfDayUnavailableSecs OBJECT-TYPE
    SYNTAX         Unsigned32(0..900)
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Seconds with layer 1 unavailable in the
         interval. Layer 1 is unavailable due to test
         activation, layer 1 affecting tests or layer 1
         affecting portfailures or a LOSW failure detection."
    ::= { vosdslIfNtuL1PerfDayEntry 6 }

vosdslIfNtuL1PerfDayTxRate OBJECT-TYPE
    SYNTAX         Unsigned32
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "Physical transmission rate in kbit/s available in
         the interval. If the physical transmission rate 
         changed during the interval at least once, the value is 0."
    ::= { vosdslIfNtuL1PerfDayEntry 7 }

vosdslIfNtuL1PerfDayValidData OBJECT-TYPE
    SYNTAX         TruthValue
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This variable indicates if there are valid data for
         this interval."
    ::= { vosdslIfNtuL1PerfDayEntry 8 }

--  Trap objects consist of following groups

vosdslLcPortTraps         OBJECT IDENTIFIER ::= {vosdslMIBTraps 1}

--  vosdslLcPortTraps group - - - - - - - - - - - - - - - - - - - - - -

vosdslLcPortStateTxRateChangeTrap NOTIFICATION-TYPE
    OBJECTS        { sysName,
                     vosdslLcPortStateCurrTxRate,
                     vosdslLcPortConfNominalTxRate,
                     vosdslLcPortConfMinimumTxRate,
                     vosdslLcModuleAttainableRate
                   }
    STATUS         current
    DESCRIPTION
        "Actual transmission rate on SDSL line changed.
         If the transmission rate changes to 'not available'
         (127 resp. 0x7F) the trap is only sent if the line
         has become unavailable due to change of OST
         (operational state) configuration.
         If the transmission rate changes from 'not
         available' (127 resp. 0x7F) the trap is sent if the
         line has become available due to change of OST
         (operational state) configuration or if the current
         transmission rate is different from that
         before it changed to 'not availalbe'."
    ::= { vosdslLcPortTraps 1 }

vosdslLcPortNtuVersionCheckTrap NOTIFICATION-TYPE
    OBJECTS        { sysName,
                     vosdslLcPortNtuVendorId,
                     vosdslLcPortNtuHwVersion,
                     vosdslLcPortNtuFwVersion,
                     vosdslLcPortNtuSwVersion,
                     vosdslLcPortNtuCompatibility
}
    STATUS         current
    DESCRIPTION
        "VoSDSL-NTU version check is necessary.
         This happens after reset of the VoSDSL-NTU if it is
         without software or if there is no entry in
         vosdslLcNtuCompTable that validates the VoSDSL-NTUs
         vendor/hardware/software combination."
    ::= { vosdslLcPortTraps 2 }


vosdslLcPortBadLineTrap NOTIFICATION-TYPE
    OBJECTS        { sysName,
                     vosdslLcPortThreshUpperBER
                   }
    STATUS         current
    DESCRIPTION
        "Upper Bit error rate threshold has been exceeded."
    ::= { vosdslLcPortTraps 3 }

vosdslLcPortGoodLineTrap NOTIFICATION-TYPE
    OBJECTS        { sysName,
                     vosdslLcPortThreshLowerBER
                   }
    STATUS         current
    DESCRIPTION
"Actual Bit error rate has crossed underneath the lower BER 
 threshold and a Bad Line Trap has been sent before."
    ::= { vosdslLcPortTraps 4 }

END
